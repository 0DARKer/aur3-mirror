pkgname=craftbukkit-plugin-skylandsplus
pkgver=0.12
pkgrel=1
pkgdesc="Skylands world generator plugin for Bukkit"
arch=(any)
url="http://dev.bukkit.org/server-mods/skylandsplus/"
license=('GPLv3')
depends=(craftbukkit)
optdepends=('craftbukkit-plugin-multiverse-core')
source=("http://dev.bukkit.org/media/files/590/770/SkylandsPlus.jar")
noextract=("SkylandsPlus.jar")
md5sums=('7f56f82636bd30e9fcc67fcd55c01330')

package() {
  mkdir -p "$pkgdir/opt/craftbukkit/plugins"
  install -m 644 "$srcdir/SkylandsPlus.jar" "$pkgdir/opt/craftbukkit/plugins/"
}

