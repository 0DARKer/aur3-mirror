pkgname=craftbukkit-plugin-skylandsplus
pkgver=0.14.2
pkgrel=1
pkgdesc="Skylands world generator plugin for Bukkit"
arch=(any)
url="http://dev.bukkit.org/server-mods/skylandsplus"
license=("GPLv3")
depends=("craftbukkit>=1.4.5r1.0")
optdepends=("craftbukkit-plugin-multiverse-core")
source=("http://dev.bukkit.org/media/files/658/82/skylandsplus-0.14.2-SNAPSHOT.jar")
noextract=("skylandsplus-$pkgver-SNAPSHOT.jar")
md5sums=('ae4f3e9ef4f7c8acb1f48f5d9396892f')

package() {
  install -Dm644 "$srcdir/skylandsplus-$pkgver-SNAPSHOT.jar" "$pkgdir/srv/craftbukkit/plugins/skylandsplus.jar"
}

