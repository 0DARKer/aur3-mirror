pkgname=craftbukkit-plugin-skylandsplus
pkgver=0.14.3
pkgrel=1
pkgdesc="Skylands world generator plugin for Bukkit"
arch=(any)
url="http://dev.bukkit.org/server-mods/skylandsplus"
license=("GPLv3")
depends=("craftbukkit>=1.4.5r1.0")
optdepends=("craftbukkit-plugin-multiverse-core")
source=("http://dev.bukkit.org/media/files/659/58/skylandsplus-0.14.3.jar")
noextract=("skylandsplus-$pkgver.jar")
md5sums=('d25afd302458cad329992130519e7144')

package() {
  install -Dm644 "$srcdir/skylandsplus-$pkgver.jar" "$pkgdir/srv/craftbukkit/plugins/skylandsplus.jar"
}

