# Maintainer: Daniel Kozak <kozzi11@gmail.com>

pkgname=nuget-cert
pkgver=1
pkgrel=1
pkgdesc="Certificates for NuGet" 
arch=('i686' 'x86_64')
url="http://stackoverflow.com/a/16589218"
license=('custom')
depends=('mono>=3.4')
makedepends=()
options=()
source=('certs.tar.gz')
provides=('nuget-certs')
conflicts=('nuget-certs')
md5sums=('5b55ba6152e01cee52bce3b29cd2e47a')


#build() {}

package() {
  
  cd ${srcdir}
  mkdir -p $pkgdir/usr/share/.mono  
  cp -Raf {certs,keypairs} $pkgdir/usr/share/.mono/
}

