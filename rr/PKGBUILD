pkgname=rr
pkgver=1.0.0
pkgrel=3
pkgdesc="Lightweight tool for recording and replaying execution of applications (trees of processes and threads)"
arch=('i686' 'x86_64')
url="http://rr-project.org/"
license=('MIT')
source=("http://rr-project.org/releases/rr-$pkgver-Linux-$CARCH.deb")
