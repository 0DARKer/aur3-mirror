package() {
    cd $srcdir/SteamPunk
    install -d $pkgdir/usr/share/icons/Steampunk
    cp -rf *   $pkgdir/usr/share/icons/Steampunk
    #chmod -R 644 $pkgdir/usr/share/icons/Steampunk/*
}



