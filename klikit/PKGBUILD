 
# Contributor: said <atvordhosbn at gmail dot com> 

pkgname=klikit
pkgver=1.0
pkgrel=1
pkgdesc="Classical brain-twister 15-puzzle."
arch=('i686' 'x86_64')
url="http://code.google.com/p/klikit/"
license=('GPL2')
depends=('qt>=4.2')
source=(http://klikit.googlecode.com/files/Klikit-src.tar.gz)
md5sums=('ac0391a36b3265da577f5d3ecd195521')

build() {
    cd  $srcdir/Klikit-src
    qmake || return 1
    make || return 1
    install -Dm755 Klikit $pkgdir/usr/bin/$pkgname || return 1 
}
