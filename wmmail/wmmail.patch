+                           y = PLGetDictionaryEntry(x, sub_key); \
+                           PLRelease(sub_key); }
+
+/* function prototypes */
+static proplist_t domain_load(char *);
+
+static int get_boolean(proplist_t, proplist_t, int *);
+static int get_coord(proplist_t, proplist_t, int *, int *);
