pkgname=python-flask-principal
pkgver=0.3.3
pkgrel=1
pkgdesc="Identity management for flask."
arch=(any)
url="http://pypi.python.org/pypi/Flask-Principal"
license=("MIT")
depends=("python2" "python2-flask")
makedepends=("setuptools")
source=("http://pypi.python.org/packages/source/F/Flask-Principal/Flask-Principal-0.3.3.tar.gz")
md5sums=("d72a708e6132d9aca4f2374c480cad5f")

build() {
    cd $srcdir/Flask-Principal-$pkgver
    python2 setup.py install --root="$pkgdir" --prefix=/usr
}
