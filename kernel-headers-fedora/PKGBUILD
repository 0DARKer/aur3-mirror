# Archlive <http://archlive-pkg.googlecode.com>

pkgname=('kernel-headers-fedora')
pkgver=2.6.31
pkgrel=23.fc12
url="http://www.kernel.org/"
pkgdesc="The Linux Kernel Headers, for Fedora kernel."
arch=('i686' 'x86_64')
license=('GPL2')
depends=()
makedepends=('rpmextract')
backup=()
provides=("kernel-headers=$pkgver")
conflicts=('kernel-headers')
source=(http://kojipkgs.fedoraproject.org/packages/kernel/${pkgver}/${pkgrel}/${CARCH}/kernel-headers-${pkgver}-${pkgrel}.${CARCH}.rpm
	)
md5sums=()

build()
{
	cd $pkgdir
	rpmextract.sh $srcdir/kernel-headers-${pkgver}-${pkgrel}.${CARCH}.rpm
}
