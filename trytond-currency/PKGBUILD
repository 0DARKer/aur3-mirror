pkgname=trytond-currency
_pkgname=trytond_currency
pkgver=1.8.0
_pkgdir=1.8
pkgrel=1
pkgdesc="Currency module of the Tryton application platform"
arch=('any')
url="http://www.tryton.org/"
license=('GPL3')
groups=('trytond-modules')
depends=('python2' 'trytond>=1.8')
makedepends=('python-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")

build() {
  cd $srcdir/$_pkgname-$pkgver
  pythonq setup.py install --root=$pkgdir
}

md5sums=('7deb7a35bae66ef48201e9206a596bfc')
