pkgname=trytond-account-product
_pkgname=trytond_account_product
pkgver=2.2.0
_pkgdir=2.2
pkgrel=1
pkgdesc="The account_product module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-account>=2.2' 'trytond-company>=2.2' 'trytond-product>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("9cbcf79ef4f8980d0ae7a1b046a56c49")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}