pkgname=python-setproctitle
pkgver=1.0
pkgrel=1
pkgdesc="allows a python process to change its process title"
license=("BSD")
url="http://code.google.com/p/py-setproctitle/"
depends=('python')
source=(http://pypi.python.org/packages/source/s/setproctitle/setproctitle-$pkgver.tar.gz)
arch=('i686' 'x86_64')
options=(!emptydirs)
md5sums=('27504766d1a0ea30a88d8eaf1e311fa3')

build() {
  cd $startdir/src/setproctitle-$pkgver
  /usr/bin/python setup.py install --root=$startdir/pkg
}
