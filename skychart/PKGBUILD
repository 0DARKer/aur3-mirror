pkgname=skychart
pkgver=3.2
pkgrel=1
pkgdesc="Free software to draw sky charts"
arch=(i686)
license=('GPL')
depends=('gtk2')
url="http://www.ap-i.net/skychart/start"
source=(http://download.origo.ethz.ch/skychart/2128/skychart-$pkgver-1548-linux_i386.tar.bz2)
sha1sums=('3cb92b889cf3b479f3d07f0041437a6b44a56dd8')

build() {
mkdir ${pkgdir}/usr/
tar -jxvf ${srcdir}/skychart-$pkgver-1548-linux_i386.tar.bz2 -C ${pkgdir}/usr/
}
