pkgname=lib32-orbit2
pkgver=2.14.19
pkgrel32=1
pkgrel=1
pkgdesc="Thin/fast CORBA ORB"
arch=('x86_64')
license=('LGPL' 'GPL')
groups=('lib32')
depends=('lib32-libidl2')
url="http://www.gnome.org"
source=("ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz")
md5sums=('8359065191a9c84220051b1d874dc9c8')

build() {
	mkdir -p $pkgdir/opt/lib32/usr/lib || return 1
	cp -R $srcdir/usr/lib/*.so* $pkgdir/opt/lib32/usr/lib || return 1
}

