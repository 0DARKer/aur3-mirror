pkgname=spoonwep2
pkgver=2
pkgrel=1
pkgdesc="Tools for aircrack - BT-Radio-Network-Analysis-80211-Cracking"
url='http://remote-exploit.org'
arch=('i686' 'x86_64')
license=('GPL')
depends=('aircrack-ng')
source=('spoonwep2.deb')
md5sums=('bd9f6a7ca5cb31f15780e97b8508ece7')

build() {
	ar -x ${startdir}/spoonwep2.deb || return 1
	tar -xf ${srcdir}/data.tar.gz || return 1
	cp -R ${srcdir}/usr ${pkgdir}
	mkdir ${pkgdir}/usr/local/bin/wifispoonfeeder/spoonwep/tmp
}
