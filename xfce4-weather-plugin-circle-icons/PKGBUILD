pkgname=xfce4-weather-plugin-circle-icons
pkgver=0.1
pkgrel=1
pkgdesc="Flat-circle icons for xfce4-weather-plugin"
arch=('any')
url="https://github.com/schizo-ri/xfce4-weather-icons"
license=('CC0 1.0 Universal')
source=('https://github.com/schizo-ri/xfce4-weather-icons/archive/master.zip')
md5sums=('3317678a4467369eeb8465897396d45b')
sha256sums=('f49986354508596700adaf16da6a21faaaf4449f6bf00c287af0c3b76ef27d4c')
makedepends=()

build() {
  mkdir -p "$pkgdir/usr/share/xfce4/weather/icons"
   
  cp -Rf $srcdir/xfce4-weather-icons-master/flat-circle/ "$pkgdir/usr/share/xfce4/weather/icons/"
}
