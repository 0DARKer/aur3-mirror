pkgname=genepop
pkgver=4.1
pkgrel=1
pkgdesc="A population genetics software."
arch=('i686' 'x86_64')
url="http://genepop.curtin.edu.au/"
license=('CeCILL')
depends=('gcc-libs')
source=(http://kimura.univ-montp2.fr/~rousset/sources.zip)
md5sums=('ace46de18238d53c29189ecd963fc0cd')
build() {
  cd "$srcdir"
  g++ $CXXFLAGS -v -DNO_MODULES -o genepop GenepopS.cpp
  install -D -m 755 genepop "$pkgdir/usr/bin/genepop"
}
