pkgname=xfce-menu-edit
pkgver=0.8
pkgrel=1
pkgdesc="XFCE Menu Editor"
arch=(i686 x86_64)
license=('GNU')
url="http://xfcemenuedit.mon-asso.org/"
source=(http://xfcemenuedit.mon-asso.org/src/$pkgname-$pkgver.tar.bz2)
md5sums=('35f1f954cdfdef436a7662a876726d6f')

build() {
  cd $startdir/src/$pkgname-$pkgver
  ./configure --prefix=/usr
  make || return 1
  make prefix=$startdir/pkg/usr install
}