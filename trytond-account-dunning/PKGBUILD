# Maintainer: Robin Baumgartner <robin@baumgartners.ch>
pkgname=trytond-account-dunning
_pkgname=trytond_account_dunning
pkgver=3.2.0
_pkgdir=3.2
pkgrel=1
pkgdesc="The account_dunning module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=3.2' 'trytond-account>=3.2' 'trytond-company>=3.2' 'trytond-party>=3.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("e491229cf4b7af1b364473d012fb94a9")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py build
}

package() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py install --root=$pkgdir
}