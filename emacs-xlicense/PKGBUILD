# Maintainer: Nathan Owe <ndowens.aur at gmail dot com>
pkgname=emacs-xlicense
pkgver=1
pkgrel=1
pkgdesc="eXtra License mode for Emacs"
arch=('any')
url="http://www.emacswiki.org/emacs/ExtraLicense"
license=('unknown')
depends=('emacs')
source=("http://www.emacswiki.org/emacs/xlicense.el")
md5sums=('59b9e5665056b44816a0b7ddef088a1f')

package() {
  cd "$srcdir/"
  install -d "$pkgdir/usr/share/emacs/site-lisp/$pkgname"
  install -Dm644 xlicense.el \
  	  "$pkgdir/usr/share/emacs/site-lisp/$pkgname/xlicense.el"
}

# vim:set ts=2 sw=2 et:
