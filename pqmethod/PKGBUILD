pkgname=pqmethod
pkgver=2.33
pkgrel=0
pkgdesc="PQMethod"
url="http://schmolck.userweb.mwn.de/qmethod/"
arch=('x86_64' 'i686')
license=('Generic')
depends=('g77')
makedepends=()
conflicts=()
replaces=()
backup=()
install='pqmethod.install'
source=("http://schmolck.userweb.mwn.de/qmethod/pqm233s.zip")

build() {
  mkdir -p "$pkgdir/usr/bin"
  cd "${srcdir}"
  g77-3.4 "$pkgname.for" -o "$pkgname"
  cp $srcdir/* $pkgdir/usr/bin 
 }
 
