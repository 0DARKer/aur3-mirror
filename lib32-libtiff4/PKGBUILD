# Maintainer: lorim <lorimz AT gmail DOT com>

pkgname=lib32-libtiff4
pkgver=4_3.9.4
_pkgrel32=5
pkgrel=4
pkgdesc="A package that installs libtiff.so.4"
arch=('x86_64')
url="http://bbs.archlinux.org/viewtopic.php?id=45393"
license=('unknown')
groups=('lib32')
depends=('lib32-zlib' 'lib32-libjpeg6')
makedepends=(deb2targz)
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
source=(http://security.ubuntu.com/ubuntu/pool/main/t/tiff/libtiff${pkgver}-${_pkgrel32}_i386.deb)
noextract=()

build() {

  deb2targz libtiff${pkgver}-${_pkgrel32}_i386.deb
  tar -xvvf libtiff${pkgver}-${_pkgrel32}_i386.tar.gz
  mkdir -p ${pkgdir}/usr/lib32
  cp -R ${srcdir}/usr/lib/* $pkgdir/usr/lib32/

}

# vim:set ts=2 sw=2 et:
md5sums=('b5bb38a17dc11e74b4474adc6b88d746')
