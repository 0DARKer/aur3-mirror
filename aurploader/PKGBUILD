#Maintainer: Xyne <ac xunilhcra enyx, backwards>
pkgname=aurploader
pkgver=2012.02.16.67022
pkgrel=20
pkgdesc='upload packages to the AUR'
arch=(any)
license=(GPL)
url="http://xyne.archlinux.ca/projects/aurploader"
depends=(curl perl)
source=(http://xyne.archlinux.ca/projects/aurploader/archives/aurploader-2012.02.16.67022.tar.xz)
sha512sums=(07bd1d0482ea06008048c4415f03fdc1dd78d58a1504e88a5d60d66deac1452e27d1c31771adce4d9cfb156d3f963b844d16e724c3ca92747309eabb8a148731)
md5sums=(063d62614dc1faf9ee22344315c14a0f)

build () 
{ 
  install -D -m755 "$srcdir/$pkgname-$pkgver/$pkgname" "$pkgdir/usr/bin/$pkgname";
  install -D -m644 "$srcdir/$pkgname-$pkgver/man/$pkgname.1.gz" "$pkgdir/usr/share/man/man1/$pkgname.1.gz"
}


