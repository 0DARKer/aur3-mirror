pkgname=jsoup
pkgver=1.5.2
pkgrel=1
pkgdesc="An HTML parser for Java"
arch=('any')
url="http://jsoup.org/"
license="MIT"
depends=('java-runtime')
conflicts=('postgresql-jdbc3')
source=(http://jsoup.org/packages/jsoup-${pkgver}.jar \
	http://jsoup.org/packages/jsoup-${pkgver}-javadoc.jar \
	http://jsoup.org/packages/jsoup-${pkgver}-sources.jar )
noextract=('*.jar')

package() {
	mkdir -p $pkgdir/usr/share/java/$pkgname
	cp $srcdir/* $pkgdir/usr/share/java/$pkgname
}


md5sums=('066093d46734321e1cf01d04f428b82b' \
	 '08f605fa96a202c6dc9284c6b4b3ba1e' \
	 '9c7795394159ce242d9d763a93ed689d')
