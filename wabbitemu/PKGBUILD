# Maintainer: Drew DeVault <sir@cmpwn.com>
pkgname=wabbitemu
pkgver=1.8.2.26
pkgrel=2
pkgdesc='An emulator for Texas Instruments Calculators'
arch=('i686' 'x86_64')
url='https://wabbit.codeplex.com/'
license=('GPL')
depends=('wine')
source=(
  'WabbitEmu.exe::https://download-codeplex.sec.s-msft.com/Download/Release?ProjectName=wabbit&DownloadId=122222&FileTime=130380060355670000&Build=20911'
  'wabbitemu'
  )
md5sums=(SKIP
         '1a1d0a376bf296da5cbcc5c9fcabe41c')

package() {
  mkdir -p $pkgdir/opt/wabbitemu
  install -Dm644 "$srcdir/WabbitEmu.exe" "$pkgdir/opt/wabbitemu/"
  install -Dm755 "$srcdir/wabbitemu" "$pkgdir/usr/bin/wabbitemu"
}
