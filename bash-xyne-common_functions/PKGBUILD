pkgname=bash-xyne-common_functions
pkgver=2010.10.26.1
pkgrel=1
pkgdesc="Xyne's commonly used bash functions."
optdepends=('curl: for webpage interaction' 'wget: for webpage interaction')
arch=('any')
license=('GPL')
url="http://xyne.archlinux.ca/projects/${pkgname}"
source=(http://xyne.archlinux.ca/src/${pkgname}-${pkgver}.tar.gz)
md5sums=('b7c9433f075f73a9271d1eb11a48ef81')

build() {
  _file="common_functions"
  install -D -m644 $srcdir/$pkgname/$_file $pkgdir/usr/share/xyne/bash/$_file
}
