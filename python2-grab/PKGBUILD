pkgname=python2-grab  
pkgver=0.4.2
pkgrel=1 
pkgdesc="Pycurl wrapper"
arch=('any')
url="http://bitbucket.org/lorien/grab/"
license=('BSD')
groups=()
depends=('python2' 'python-pycurl' 'python2-lxml')
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
changelog=
source=(http://pypi.python.org/packages/source/g/grab/grab-${pkgver}.tar.gz)
md5sums=('5c8b87a70900b53256ef0f87d85e4718')
noextract=()

build() {
  cd $startdir/src/grab-$pkgver
  python2 setup.py install --root="${pkgdir}" --optimize=1
}

