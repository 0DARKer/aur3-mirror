#Maintainer: Xyne <ac xunilhcra enyx, backwards>
pkgname=cronwhip
pkgver=2012.02.16.56696
pkgrel=14
pkgdesc='Run missed cronjobs.'
arch=(any)
license=(GPL)
url="http://xyne.archlinux.ca/projects/cronwhip"
depends=(perl)
source=(http://xyne.archlinux.ca/projects/cronwhip/archives/cronwhip-2012.02.16.56696.tar.xz)
sha512sums=(628ead24287b479eb85704be9572740d1aa8668edcf621ac9d240825509defced69ec36063cb92057d4efeed03bc55e9bb6a7fea9175a1ff7f74fab1026938de)
md5sums=(6bb2072b70fbce5165e16a51aef6d560)
install=cronwhip.install

build () 
{ 
  install -D -m755 "$srcdir/$pkgname-$pkgver/$pkgname" "$pkgdir/usr/bin/$pkgname";
  install -D -m644 "$srcdir/$pkgname-$pkgver/man/$pkgname.1.gz" "$pkgdir/usr/share/man/man1/$pkgname.1.gz"
}


