# Maintainer: 謝致邦 <Yeking@Red54.com>

pkgname=unrpyc
pkgver=20121227
pkgrel=1
pkgdesc="A ren'py script decompiler"
arch=("any")
url="http://github.com/Red54/unrpyc"
license=('MIT')
depends=("python2")
source=(http://github.com/Red54/unrpyc/archive/master.tar.gz)
md5sums=('d5c1be5390c33ab6b2418ad9e02eedcb')

package() {
	cd "$srcdir/unrpyc-master"
	install -Dm0755 unrpyc $pkgdir/usr/bin/unrpyc
	install -Dm0755 decompiler.py $pkgdir/usr/bin/decompiler.py
}
