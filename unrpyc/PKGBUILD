pkgname=unrpyc
pkgver=20121227
pkgrel=1
pkgdesc="A ren'py script decompiler"
arch=("any")
url="http://github.com/yuriks/unrpyc"
license=('MIT')
depends=("python2")
source=(http://github.com/yuriks/unrpyc/archive/master.tar.gz)
md5sums=('d4ac9492277f1c05d3cea370b6ab88e7')

package() {
	cd "$srcdir/unrpyc-master"
	install -dm0755 $pkgdir/usr/bin
	install -m0755 *.py $pkgdir/usr/bin
}
