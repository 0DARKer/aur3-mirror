pkgname=lib32-printproto
pkgname32=${pkgname/lib32-/}
pkgver=1.0.4
pkgrel=2
pkgrel32=2
pkgdesc="X11 print extension wire protocol"
arch=(x86_64)
url="http://xorg.freedesktop.org/"
license=('GPL')
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname32}-${pkgver}-${pkgrel32}-any.pkg.tar.xz)
md5sums=('dc2316db80d37d914a040b6e63d9e003')

build() {
  mkdir -p $startdir/pkg/opt/lib32/usr/
  cp -rPf $startdir/src/usr/lib $startdir/pkg/opt/lib32/usr/lib
}
