pkgname=aurnotify
pkgver=0.1.5
pkgrel=1
pkgdesc="A tool to set notify status of favorite packages on AUR"
url="http://www.archlinux.org/"
license="GPL"
arch=('i686' 'x86_64')
depends=('wget' 'bash')
source=($pkgname)
md5sums=('a3682b6d17d27e27dd4db95ec64b0d88')

build() {
    install -D -m 755 ${startdir}/$pkgname ${pkgdir}/usr/bin/$pkgname
}
