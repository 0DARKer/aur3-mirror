pkgname=argcomplete
pkgver=0.8.8
pkgrel=1
pkgdesc="Argcomplete provides easy, extensible command line tab completion of arguments for your Python script."
arch=('any')
url="https://pypi.python.org"
license=('Apache')
depends=('python2' 'python')
source=('https://pypi.python.org/packages/source/a/argcomplete/argcomplete-0.8.8.tar.gz')
sha256sums=('4ea33264081d46f259e2672f20ce4cd47e539118e70991ce5edc69b67bc93cfc')

package() {
    cd "$srcdir/$pkgname-$pkgver"
    python2 setup.py install --root="$pkgdir/" --optimize=1
}
