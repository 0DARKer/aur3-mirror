pkgname=lewm
pkgver=0.69
pkgrel=1
pkgdesc="Lua script that brings ratpoison to musca Window Managers"
url="http://github.com/cdede/lewm/"
arch=('any')
license=('GPL')
depends=('dmenu'  'archlinux-xdg-menu' )
source=(https://github.com/cdede/lewm/tarball/${pkgver})
sha1sums=('097e79cae2ae460cee2e0b32a7f03a2e13bb5d33')

build() {
    cd ${srcdir}/*${pkgname}*
    PREFIX=${pkgdir}  ./build.sh install
}
