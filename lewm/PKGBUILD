pkgname=lewm
pkgver=0.84.1
pkgrel=1
pkgdesc="commandline get keepassx password"
url="http://github.com/cdede/lewm/"
arch=('any')
license=('GPL')
depends=('python-kppy' )
source=(https://github.com/cdede/lewm/tarball/${pkgver})
sha256sums=('2fc84462dfe367625d8a7f69573d423c8546268cc46aa55d6c53de2d5c74e82e')

build() {
  cd  ${srcdir}/*${pkgname}*/
  python  ./setup.py install --root=$pkgdir || return 1
}
