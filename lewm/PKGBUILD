pkgname=lewm
pkgver=0.9.13
pkgrel=1
pkgdesc="commandline get keepassx password"
url="http://github.com/cdede/lewm/"
arch=('any')
license=('GPL3')
depends=('python-kppy' )
source=(https://github.com/cdede/lewm/tarball/${pkgver})
sha256sums=('42e189b6ec5fb35256572da801b312688ec675ffc00c65466974954d07bea8b3')

build() {
  cd  ${srcdir}/*${pkgname}*/
  python  setup.py build
}
package() {
  cd  ${srcdir}/*${pkgname}*/
  python setup.py install --root="$pkgdir/" --optimize=1
}
