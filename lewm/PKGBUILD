pkgname=lewm
pkgver=0.71
pkgrel=1
pkgdesc="LEt fortune's Wheel help you to choose at the Moment"
url="http://github.com/cdede/lewm/"
arch=('any')
license=('GPL')
depends=('python2' )
source=(https://github.com/cdede/lewm/tarball/${pkgver})
sha1sums=('956457501cf4901ed8e5dc40025b27937c794ef6')

build() {
    cd ${srcdir}/*${pkgname}*
    python2 ./setup.py install --root=$pkgdir || return 1
}
