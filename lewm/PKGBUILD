pkgname=lewm
pkgver=0.9.32
pkgrel=1
pkgdesc="commandline get keepassx password"
url="http://github.com/cdede/lewm/"
arch=('any')
license=('GPL3')
depends=('python-kppy' )
source=(https://github.com/cdede/lewm/tarball/${pkgver})
sha256sums=('426f60c99a0ac49bfb4855de9b838e8b8ff512b9a480a990c734104deb1cac35')

build() {
  cd  ${srcdir}/*${pkgname}*/
  python  setup.py build
}
package() {
  cd  ${srcdir}/*${pkgname}*/
  python setup.py install --root="$pkgdir/" --optimize=1
}
