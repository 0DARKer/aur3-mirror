pkgname=lewm
pkgver=0.9.35
pkgrel=1
pkgdesc="commandline get keepassx password"
url="http://github.com/cdede/lewm/"
arch=('any')
license=('GPL3')
depends=('python-kppy' )
source=(https://github.com/cdede/lewm/tarball/${pkgver})
sha256sums=('aa045e435dbe3a7cdfd728a829da7d3c1604fa5273a79ac77d58fdd9fc191b65')

build() {
  cd  ${srcdir}/*${pkgname}*/
  python  setup.py build
}
package() {
  cd  ${srcdir}/*${pkgname}*/
  python setup.py install --root="$pkgdir/" --optimize=1
}
