pkgname=lewm
pkgver=0.8
pkgrel=1
pkgdesc="commandline get keepassx password"
url="http://github.com/cdede/lewm/"
arch=('any')
license=('GPL')
depends=('python-kppy' )
source=(https://github.com/cdede/lewm/tarball/${pkgver})
sha256sums=('1f61e4bb0a8f7589ee751ca3ad38c83271945ffa917eb0796858847e600aaaee')

build() {
  cd  ${srcdir}/*${pkgname}*/
  python  ./setup.py install --root=$pkgdir || return 1
}
