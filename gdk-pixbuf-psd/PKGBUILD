# Maintainer: Alessio Sergi <asergi [at] archlinux [dot] us>

pkgname=gdk-pixbuf-psd
pkgver=20101013
pkgrel=2
pkgdesc="GdkPixbuf loader for Adobe Photoshop (PSD) file format"
arch=('i686' 'x86_64')
url="http://code.google.com/p/gdk-pixbuf-psd/"
license=('LGPL')
depends=('gtk2')
install=${pkgname}.install
source=(http://db.tt/yK5A3BE)
md5sums=('31541e514bc4692d3e48d540a1ef4bd5')

build() {
  cd ${srcdir}/${pkgname}-${pkgver}
  make
}

package() {
  cd ${srcdir}/${pkgname}-${pkgver}
  make DESTDIR=${pkgdir} install
}
