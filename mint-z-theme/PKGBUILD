pkgname=mint-z-theme
pkgver=1.0.4
pkgrel=1
pkgdesc="Linux Mint GTK2/GTK3 and GNOME Shell theme based on Zukitwo, Faience, Adwaita, Faenza and others"
arch=('i686' 'x86_64')
url="http://packages.linuxmint.com/pool/main/m/mint-z-theme/"
license=('GPL')
depends=('gtk-engine-murrine' 'gtk-engine-unico')
conflicts=(${pkgname}-git)
source=("${url}/${pkgname}_${pkgver}.tar.gz")
md5sums=('9ffea6fac84691fc07551c4fb29e612d')

build() {
 cp -r "${srcdir}"/${pkgname}/usr "${pkgdir}"
}
