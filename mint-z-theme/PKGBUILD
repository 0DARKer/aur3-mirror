# Maintainer: crazyelf5

pkgname=mint-z-theme
pkgver=1.1.1
pkgrel=1
pkgdesc="Linux Mint GTK2/GTK3 and GNOME Shell theme based on mintified versions of Zukitwo, Faience, Adwaita, Faenza and other beautiful themes from the Linux community"
arch=('any')
url="http://packages.linuxmint.com/pool/main/m/mint-z-theme"
license=('GPL3')
depends=('gtk-engine-murrine' 'gtk-engine-unico')
conflicts=(${pkgname}-git)
source=("${url}/${pkgname}_${pkgver}.tar.gz")
md5sums=('4334fc72d4391b71c878ab6342281d73')

build() {
	cp -r "${srcdir}"/${pkgname}/usr "${pkgdir}"
}
