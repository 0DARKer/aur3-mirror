pkgname=conky-lua-arch
pkgver=0.2
pkgrel=1
pkgdesc="A conky-lua for ArchLinux"
arch=('any')
url="http://www.vklinux.net/index.php?option=com_content&view=article&id=14&Itemid=24"
license=('GPL')
depends=('conky-lua')
source=("http://www.vklinux.net/project/conkyluaarch/$pkgname-$pkgver.tar.gz")


package() {
	cd $srcdir/$pkgname-$pkgver
	make DESTDIR=${pkgdir} install
}



md5sums=('af19719752b94899bc1ba7744e67c7ae')
