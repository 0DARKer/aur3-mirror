pkgname=conky-lua-arch
pkgver=0.2
pkgrel=2
pkgdesc="A conky-lua for ArchLinux"
arch=('any')
url="http://www.vklinux.net/index.php/forum/44-archlinux-project/62-conky-lua-arch"
license=('GPL')
depends=('conky-lua')
source=("http://sourceforge.net/projects/conkyluaarch/files/$pkgname-$pkgver.tar.gz")


package() {
	cd $srcdir/$pkgname-$pkgver
	make DESTDIR=${pkgdir} install
}



md5sums=('eef075d282f9bd831cc82d046bfaa118')
