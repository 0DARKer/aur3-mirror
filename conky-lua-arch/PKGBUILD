# Maintainer: ale_xiovk <varialking et gmail dot com>

pkgname=conky-lua-arch
pkgver=0.1
pkgrel=1
pkgdesc="A conky-lua for ArchLinux"
arch=('any')
url="http://vkteam.altervista.org/conky/conky.html"
license=('GPL')
depends=('conky-lua')
source=("http://vkteam.altervista.org/conky/$pkgname-$pkgver.tar.gz")

package() {
	cd $srcdir/$pkgname-$pkgver
	make DESTDIR=${pkgdir} install
}



md5sums=('baab9106e8d0df6a91a615c8bc84b1ae')
