pkgname=dfu-util
pkgver=2008.9
pkgrel=2
pkgdesc="Allow you to connect to the FreeRunner through the USB cable and flash it"
url="http://wiki.openmoko.org/wiki/Dfu-util"
license=('GPL2')
arch=('i686' 'x86_64')
makedepends=()
depends=()
source=(http://downloads.openmoko.org/distro/releases/Om$pkgver/$pkgname)
md5sums=('21b832d421744f1e2d490ecb62f79ab0')





build() {
  cd $startdir/src
  install -D -m755 $pkgname $startdir/pkg/usr/bin/$pkgname
}
