# Contributor: Sandro Andrade <sandroandrade at kde.org>
pkgname=qtwrappedobjects5
pkgver=0.89
pkgrel=0
pkgdesc="QtWrappedObjects is a library for easily handling of wrapped objects by means of smart delegators"
arch=('i686' 'x86_64')
url="http://qt.gitorious.org/qtplayground/qtmof"
license=('LGPL')
depends=('qt5')
source=(http://qt.gitorious.org/qtplayground/qtmof/archive-tarball/917a57e7)
 
build() {
  cd $srcdir/qtplayground-qtmof/
  qmake-qt5 -r
 
  cd $srcdir/qtplayground-qtmof/src/wrappedobjects/
 
  make || return 1
}
 
package() {
  cd $srcdir/qtplayground-qtmof/src/wrappedobjects/
  make INSTALL_ROOT=${pkgdir} install
}
 
md5sums=('3999ff45c36e5d1008494d1f436fd90f')
