pkgname=xfce-theme-meliae
pkgver=1.0
pkgrel=2
pkgdesc="xfce4 theme icon meliae"
arch=('any')
license=('GPL')
url=('http://gnome-look.org/content/show.php/Meliae?content=88482')
groups=
provides=
depends=
makedepends=
conflicts=
replaces=
backup=
install=
source=(http://www.mathix.org/depot/MeliaeSVG.tar.gz)
md5sums=('ee9c65d746989c9159c2501301da7dc0')
 
build() {
  cd ${srcdir}/MeliaeSVG
  install -d ${pkgdir}/usr/share/icons/MeliaeSVG
  cp -rf * ${pkgdir}/usr/share/icons/MeliaeSVG || return 1
}
