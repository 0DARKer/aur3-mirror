pkgname=python2-runsnakerun
pkgver=2.0.1
pkgrel=1
pkgdesc="GUI Viewer for Python profiling runs"
arch=('any')
url="http://www.vrplumber.com/programming/runsnakerun/"
license=('BSD')
depends=('python2-squaremap')
source=('http://pypi.python.org/packages/source/R/RunSnakeRun/RunSnakeRun-2.0.1.tar.gz#md5=e96bae8c922d1e4dc066e99124dfb80b')
md5sums=('e96bae8c922d1e4dc066e99124dfb80b')

package() {
  cd "$srcdir/RunSnakeRun-$pkgver"
  python2 setup.py install --root=$pkgdir/ --optimize=1
}

