# Contributor: Ingo Becker <i.becker@netgaertner.de>
pkgname=pthsem
pkgver=2.0.7 
pkgrel=1
pkgdesc="Extend version of pth with support for semaphores added." 
arch=(i686)
url="http://www.auto.tuwien.ac.at/~mkoegler/index.php/pth"
license=('GPL')
source=(http://www.auto.tuwien.ac.at/~mkoegler/pth/${pkgname}-${pkgver}.tar.gz)
md5sums=('b277716ee1224ca9925176fa29e1f0c5')
build() {
          cd ${startdir}/src/${pkgname}-${pkgver}
            ./configure --prefix=/usr
              make || return 1
                make prefix=$startdir/pkg/usr install
        }

