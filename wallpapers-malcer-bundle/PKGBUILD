package() {
        install -Dm644 ${srcdir}/LICENSE ${pkgdir}/usr/share/licenses/${pkgname}/LICENSE
  	cd ${srcdir}
	find * -type f -exec install -Dm 644 '{}' "${pkgdir}/usr/share/wallpapers/{}" \;
	find ${pkgdir}/usr/share/wallpapers/ -type f -name '*promotional.*' -delete
	find ${pkgdir}/usr/share/wallpapers/ -type f -name '*.txt' -delete
	find ${pkgdir}/usr/share/wallpapers/ -type f -name 'LICENSE' -delete
}  
