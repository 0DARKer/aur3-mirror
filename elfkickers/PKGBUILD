pkgname=elfkickers
pkgver=3.0
pkgrel=1
pkgdesc="A collection on programs for dealing with the ELF file format"
arch=(i686 x86_64)
license=(GPL)
depends=('gcc')
makedepends=('make')
source=(http://www.muppetlabs.com/~breadbox/pub/software/ELFkickers-${pkgver}.tar.gz)
url="http://www.muppetlabs.com/~breadbox/software/elfkickers.html"
md5sums=('9602acaa070c7be7433cf43d6f2ae675')

build() {
  mkdir -p ${startdir}/pkg/usr/bin
  cd ${startdir}/src/ELFkickers-${pkgver}
  make || return 1
  mkdir -p ${pkgdir}/usr/bin ${pkgdir}/usr/share/man/man1
  make prefix=${pkgdir}/usr install
}
