# The amount of processes that should be spawned (Default: 1)
PREFORK=1

# The name of the account under which SOGo will be running (Default: sogo)
USER=sogo

# The PORT the service should listen on (Default: 20000)
PORT=20000

# Log file location (Default: /var/log/sogo/sogo.log)
LOGFILE=/var/log/sogo/sogo.log
