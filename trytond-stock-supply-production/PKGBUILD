pkgname=trytond-stock-supply-production
_pkgname=trytond_stock_supply_production
pkgver=2.8.0
_pkgdir=2.8
pkgrel=1
pkgdesc="The stock_supply_production module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.8' 'trytond-product>=2.8' 'trytond-production>=2.8' 'trytond-stock>=2.8' 'trytond-stock-supply>=2.8')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("1f4a12ce67baaa927da8c1da3790c8d0")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py install --root=$pkgdir
}