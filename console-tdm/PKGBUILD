# Maintainer: Iru Dog <mytbk920423@gmail.com>

pkgname=console-tdm
pkgdesc="A session selector similar to CDM."
url=https://code.google.com/p/t-display-manager/
arch=('any')
pkgver=0.2.3
pkgrel=1
license=('GPL')
depends=('xorg-xinit')
source=("http://t-display-manager.googlecode.com/files/tdm-${pkgver}.tar.xz")
install=tdm.install

build() {
	true
}

package(){
	mkdir -p $pkgdir/usr/bin
	cd $srcdir/tdm-${pkgver}
	DESTDIR=${pkgdir} PREFIX=/usr ./install.sh 
}

md5sums=(
"ecab423f79d2408797de6fbd17789f10"
)
