 
# Contributor: said 

pkgname=soulseekqt
pkgver=PublicBuild4
pkgrel=1
pkgdesc="Soulseek music-sharing client"
arch=('i686')
url="http://www.slsknet.org/"
license=('?')
source=(http://www.slsknet.org/SoulseekQT/Linux/SoulseekQT-${pkgver}-32bit $pkgname.desktop $pkgname.png)
depends=('qt')
md5sums=('4c5c64966f206f1d5b0a465d7a3bb5e6'
         '0a2137a47e9ac0b4b57a0d55872d0842'
         '256ec24f7ad2d39ac8da0f1c03d3cafe')

package() {
  install -Dm755 $srcdir/SoulseekQT-${pkgver}-32bit $pkgdir/usr/bin/$pkgname
  install -Dm644 $srcdir/$pkgname.desktop $pkgdir/usr/share/applications/$pkgname.desktop
  install -Dm644 $srcdir/$pkgname.png $pkgdir/usr/share/pixmaps/$pkgname.png
}
