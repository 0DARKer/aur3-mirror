pkgname=trytond-sale-supply
_pkgname=trytond_sale_supply
pkgver=2.8.1
_pkgdir=2.8
pkgrel=1
pkgdesc="The sale_supply module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.8' 'trytond-purchase>=2.8' 'trytond-sale>=2.8' 'trytond-stock>=2.8' 'trytond-stock-supply>=2.8')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("f6d0425977a49802423cbfab08856690")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py install --root=$pkgdir
}