pkgname=varnam-gtk
pkgver=3.14_01.01042015
pkgrel=2
pkgdesc="A desaturated blue patterned Theme for Gtk2 & 3, GoogleChrome, Gnome-Shell, & Openbox"
arch=('any')
url="http://fav.me/d8cn53k"
license=('GPLv3')
depends=('gtk-engines' 'gnome-themes-standard>=3.14' 'gtk-engine-murrine>=0.98.2')

source=("https://dl.dropboxusercontent.com/u/330352/varNaM-Gtk-3.14_01.01042015.zip")


md5sums=('55bd25d3a58277d0a8b9194f437fdf9d')


package() {
  # install themes

  find "varNaM"/ -type f \
      -exec install -Dm644 "{}" "$pkgdir/usr/share/themes/{}" \;           
}

