pkgname=tufao-doc
pkgver=0.1.0
pkgrel=1
pkgdesc="The Tufão documentation"
arch=(any)
url="http://tufao.googlecode.com/"
license=('FDL1.3')
depends=('tufao')
source=('http://tufao.googlecode.com/files/tufao-0.1-doc.zip')
md5sums=('37a3d7ac1c0bc5ad7b15499c17127de1')

package() {
  cd "$srcdir"

  mkdir -p $pkgdir/usr/share/doc/tufao
  cp -r doc/* $pkgdir/usr/share/doc/tufao
}
