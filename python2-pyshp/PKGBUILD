pkgname=python2-pyshp
_pkgname=pyshp
pkgver=1.2.0
pkgrel=2
pkgdesc="Python read/write support for ESRI Shapefile format"
arch=('any')
url="https://pypi.python.org/pypi/pyshp"
license=('MIT')
depends=('python2' 'python2-setuptools')
options=(!emptydirs)
source=(https://pypi.python.org/packages/source/p/$_pkgname/$_pkgname-$pkgver.tar.gz)
md5sums=('9075f34109f0bb4b45a90fd1fb5db94b')
sha1sums=('0deee31c9da429559a2f25b3a972c5056165053d')
sha256sums=('16e9a9b5caae5b69149c7be09415381b61af4f85f2087d08c0698c75e70000e7')

package() {
  cd "$srcdir/$_pkgname-$pkgver"
  python2 setup.py install --root="$pkgdir" --optimize=1
}

