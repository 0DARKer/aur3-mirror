# Contributor: Stefan Husmann <stefan-husmann at t-online dot de>

pkgname=jgraphx
pkgver=1.11.0.0
pkgrel=1
pkgdesc="Open source graph drawing component."
arch=('any')
url="http://www.jgraph.com/jgraphx.html"
license=('BSD')
depends=('java-runtime')
source=("http://downloads.jgraph.com/downloads/${pkgname}/archive/${pkgname}-${pkgver//./_}.zip"
)
md5sums=('a3f13fef1fece2edc44da05a46ccb63d')

package() {
	install -Dm644 "${srcdir}/${pkgname}/lib/${pkgname}.jar" "${pkgdir}/usr/share/java/${pkgname}/${pkgname}.jar"
	install -Dm644 "${srcdir}/${pkgname}/license.txt" "${pkgdir}/usr/share/licenses/${pkgname}/license.txt"
}

