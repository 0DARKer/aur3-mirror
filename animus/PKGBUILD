pkgname=animus
pkgver=3.12_04.08192014
pkgrel=1
pkgdesc="Animus is a smooth Jade theme."
arch=('any')
url="http://fav.me/d77o811"
license=('GPLv3')
depends=('gtk-engines' 'gnome-themes-standard>=3.12.0' 'gtk-engine-murrine>=0.98.2')

replaces=('animus')
conflicts=('animus')
source=("https://dl.dropboxusercontent.com/u/330352/animus-3.12_04.08192014.zip")

md5sums=('0de2f4178e509e177f6873a941981031')


package() {
  # install themes
  
  #cd Animus

  find Animus-3.12/ -type f \
      -exec install -Dm644 "{}" "$pkgdir/usr/share/themes/{}" \;          
}

