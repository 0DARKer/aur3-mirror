pkgname=jpegtran_crop_bin
pkgver=1
pkgrel=1
pkgdesc="Stable jpegtran version with the crop feature"
arch=('i686')
url="http://sylvana.net/jpegcrop/jpegtran/"
license=('other')
source=("http://sylvana.net/jpegcrop/croppatch.tar.gz")
md5sums=('45d76e4226232439308e2129b64c4ea1')

build() {
	cd ${startdir}/src/
	install -D -m 755 jpegtran $pkgdir/usr/bin/jpegtran_crop
	}
