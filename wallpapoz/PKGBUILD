pkgname=wallpapoz
pkgver=0.4.1
pkgrel=1
pkgdesc="Gnome Multi Backgrounds and Wallpapers Configuration Tool"
url="http://wallpapoz.akbarhome.com/"
license=('GPL')
depends=('pil')
arch=('i686')
source=(http://wallpapoz.akbarhome.com/files/$pkgname-$pkgver.tar.bz2)
md5sums=(''63a32917840f912648f9e01bfd1f178f'')

build() {
 cd $startdir/src/$pkgname-$pkgver
  python setup.py install --installdir=$startdir/pkg 
} 
