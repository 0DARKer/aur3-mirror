pkgname=lib32-libxaw
pkgver=1.0.8
pkgrel=1
pkgdesc="X11 Athena Widget library"
arch=('x86_64')
url="http://xorg.freedesktop.org"
license=('custom')
depends=('lib32-libxmu' 'lib32-libxp' 'lib32-libxpm')
source=("ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-}-$pkgver-$pkgrel-i686.pkg.tar.xz")
md5sums=('82f0313a1b40956d282b8e37eac1ff42')

package() {
 mkdir -p "$pkgdir/opt/lib32/usr"
 cp -rPf "$srcdir/usr/lib" "$pkgdir/opt/lib32/usr/lib"
}
