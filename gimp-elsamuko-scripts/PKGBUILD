pkgname=gimp-elsamuko-scripts
_pkgname=elsamuko
pkgver=0.1
pkgrel=1
pkgdesc='Varoius Gimp scripts made by elsamuko'
arch=(any)
url='https://sites.google.com/site/elsamuko/gimp'
license=('GPL')
groups=()
depends=(gimp)
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=(!strip)
install=
source=(
"${_pkgname}-scripts.zip::https://sites.google.com/site/elsamuko/gimp/elsamuko.zip"
)
noextract=()

package(){
  _scriptsdir="$pkgdir"/usr/share/gimp/2.0/scripts
  cd "$srcdir"/$_pkgname

  # scripts
  install -dm755 "${_scriptsdir}"
  cp *.scm "${_scriptsdir}"
  
}
md5sums=('ee3da0b99fbbfd8a77886efd8578b3ac')
