# Maintainer: archtux <antonio.arias99999 at gmail.com>

pkgname=pasaffe
pkgver=0.10
pkgrel=1
pkgdesc="Easy to use password manager for Gnome with a Password Safe 3.0 compatible database."
url="https://launchpad.net/pasaffe"
license=('GPL3')
arch=('any')
depends=('gtk2')
makedepends=('gettext' 'python-distutils-extra')
source=(http://launchpad.net/pasaffe/trunk/$pkgver/+download/$pkgname-$pkgver.tar.gz)
md5sums=(8dac379cebfdaf6dbfb09245aa247212)

build() {
    cd $srcdir/$pkgname-$pkgver
  
    python2 setup.py install --root $pkgdir
}