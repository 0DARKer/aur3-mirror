# Maintainer: Aliaksandr Stelmachonak (mail.avatar@gmail.com)

pkgname=oxygencolors
pkgver=3.3
pkgrel=2
pkgdesc="Oxygencolors is a full customizable iconset based on original oxygen"
arch=('i686' 'x86_64')
url="http://www.kde-look.org/content/show.php/oxygencolors?content=104048"
license=('GPL')
source=(http://dl.dropbox.com/u/4514366/oxygencolors\(v3.3\).tar.bz2)
md5sums=('1f10fe160f26bc3ef822a5a43c624128')

build() {
  cd "$srcdir/$pkgname(v$pkgver)"
  mkdir -p $pkgdir/usr/share/icons/oxygencolors || return 1
  cp -r *  "$pkgdir/usr/share/icons/oxygencolors" || return 1
} 
