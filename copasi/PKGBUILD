pkgname=copasi
pkgver=4.6.33
pkgrel=1
pkgdesc="COPASI is used for simulation and analysis of biochemical networks and their dynamics"
arch=(i686 x86_64)
url="http://www.copasi.org/"
license=(Artistic-2.0)
depends=(glibc java-runtime)
makedepends=()
optdepends=()
provides=()
source=(http://www.comp-sys-bio.org/mirror.copasi.org/Copasi-33-Linux-Dynamic.tar.gz)
md5sums=(9887993e7f13c08c643602c8bcc8bd7a)
#options=(!strip)

build() {
  mkdir $pkgdir/usr

  cd $srcdir/copasi
  cp -R bin $pkgdir/usr/
  cp -R share $pkgdir/usr/
}

