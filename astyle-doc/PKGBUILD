pkgname=astyle-doc
pkgver=2.02
pkgrel=2
pkgdesc="Documentation of Astyle, A Automatic Formatter for C, C++, C-sharp, and Java Source Code"
arch=('any')
url="http://sourceforge.net/projects/astyle/"
license=('LGPL')
source=(http://downloads.sourceforge.net/sourceforge/astyle/astyle_${pkgver}_linux.tar.gz)
md5sums=('6ce1f5c766ba142f152dab4ddd1ee3b7')

build() {
    mkdir $pkgdir/usr/share/doc/ -p
    mv $srcdir/astyle/doc/ $pkgdir/usr/share/doc/astyle
}

