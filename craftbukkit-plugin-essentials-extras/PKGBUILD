pkgname=craftbukkit-plugin-essentials-extras
pkgver=2.9.5
pkgrel=1
pkgdesc="Extra plugins for CraftBukkit Essentials"
arch=(any)
url="http://dev.bukkit.org/server-mods/essentials"
license=("GPLv3")
depends=("craftbukkit-plugin-essentials=$pkgver")
source=("EssentialsExtra.zip::http://tiny.cc/EssentialsExtraZip")
md5sums=('13511e100ef3002eba07bb56bf58e522')

package() {
  cd $srcdir
  find . -name '*.jar' | xargs -rtl1 -I {} install -Dm644 {} "$pkgdir/srv/craftbukkit/"{}
}

