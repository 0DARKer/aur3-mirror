license=('GPLv3')
depends=('ruby')
source=(gtr)
md5sums=('d1757ce99a306f86281eb7c4ff905c0f')
package() {

    install -D -m755 $srcdir/$pkgname "${pkgdir}/usr/sbin/${pkgname}"
}

