pkgname=wippien
pkgver=2.3.2
pkgrel=8
pkgdesc="Establish direct P2P (peer to peer) VPN between each jabber-contact"
arch=('i686' 'x86_64')
url="http://wippien.com/"
license=('GPL')
depends=('lib32-openssl-compatibility')
source=(http://wippien.com/download/wiplinux.tgz)
md5sums=('36ba39c6f42ab56ded954ea5be150562')

build() {
  cd $srcdir

  mkdir -p $pkgdir/usr/bin
  cp wipclnt $pkgdir/usr/bin/
  cp wiptun $pkgdir/usr/bin/
}

