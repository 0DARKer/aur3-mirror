#contributor: Mateusz Kwiatkowski <kwiat3k(at)panic.pl> 

pkgname=cutleaves
pkgver=1.0
pkgrel=1
pkgdesc="Removing packages that are not installed as dependencies but no longer required by any package"
url="http://www.panic.pl/cutleaves/"
license=('GPL')
arch=('i686')
depends=()
install=()
source=(
http://www.panic.pl/cutleaves/cutleaves
)
md5sums=(
'bbc6e46dbb4ea098c0e84700596a94f4'
)

build() {
	mkdir -p $startdir/src/
	cd $startdir/src/
	install -d $startdir/pkg/usr/bin
	install $startdir/$pkgname $startdir/pkg/usr/bin
}

