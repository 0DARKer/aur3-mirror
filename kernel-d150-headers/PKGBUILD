pkgname=kernel-d150-headers
pkgver=2.6.30.5
pkgrel=1
pkgdesc="Kernel headers 2.6.30-5 ottimizzato per Acer Aspire D150"
url="http://blog.liberailvoip.it/2009/09/08/acer-aspire-one-kernel-2-6-30-5-ottimizzato-ubuntu-debian/"
depends=('kernel-d150')
makedepends=('wget' 'tar')
arch=('i686')
license=('GPL')
install=${pkgname}.install
source=('http://blog.liberailvoip.it/wp-content/uploads/linux-headers-2.6.30.5-skumpic-aa1-r2.deb')
md5sums=('eba9c8e48bcd7ac69a84117cc24e1df5')

build() {

	echo -e "
\033[1;31mInstallazione kernel headers\033[0m"
	sleep 1
	cd $startdir/
	ar vx linux-headers-2.6.30.5-skumpic-aa1-r2.deb
	tar -xzvf data.tar.gz -C / > /dev/null
}
