# Contributor: Gour <gour@gour-nitai.com>
pkgname=guitone  
pkgver=1.0rc4
pkgrel=1 
pkgdesc="frontend for monotone"
url="http://guitone.thomaskeller.biz"
arch=('i686' 'x86_64')
license=('GPL3')
depends=('monotone' 'qt')
makedepends=()
conflicts=()
replaces=()
backup=()
install=
source=(http://guitone.thomaskeller.biz/count.php/from=default/$pkgver/$pkgname-$pkgver.tgz)
md5sums=('8a5c809d0be52ff9e4e033d91a3a04b3')

build() {
  cd $startdir/src/$pkgname-$pkgver
  qmake -config release || return 1
  make || return 1
  make INSTALL_ROOT=$startdir/pkg install
}