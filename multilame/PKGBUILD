# Contributor: graysky <graysky AT archlinux dot us>
arch=('any')
license=('GPL2')
pkgname=multilame
pkgver=2.0
pkgrel=3
pkgdesc="Pseudo multi-threaded bash script to parallel encode .wav to .mp3 via lame."
depends=('lame')
url=http://aur.archlinux.org/packages.php?ID=35133
source=($pkgname.sh $pkgname.conf)
md5sums=('50b578681be73b16b39a7fbfa9b152bc' '3fa76fa490cb25329837ff1107bc92b6')
install=('readme.install')
backup=(etc/$pkgname.conf)

build() {
install -D $pkgname.sh $pkgdir/usr/bin/$pkgname.sh
install -D $pkgname.conf /$pkgdir/etc/$pkgname.conf
}
