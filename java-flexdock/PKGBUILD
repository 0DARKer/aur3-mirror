pkgname=java-flexdock
pkgver=1.2.3
pkgrel=1
pkgdesc="Docking framework for Swing"
arch=('any')
url="http://forge.scilab.org/index.php/p/flexdock/"
license=('MIT')
depends=('java-runtime')
privides=('flexdock')
conflicts=('flexdock')
source=("http://forge.scilab.org/index.php/p/flexdock/downloads/get/flexdock-${pkgver}.jar")
noextract=("flexdock-${pkgver}.jar")
md5sums=('9cbf7e8af45ea900c8927152e7d17a05')

package() {
  cd "${srcdir}"
  install -Dm644 "flexdock-${pkgver}.jar" \
    "${pkgdir}/usr/share/java/flexdock/flexdock.jar"
}
