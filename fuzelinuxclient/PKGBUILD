pkgname=fuzelinuxclient
pkgver=14.02.14587
pkgrel=1
pkgdesc='Fuze for Linux'
arch=('x86_64')
url='http://www.fuzebox.com/'
license=('unknown')
depends=('c-ares' 'libjpeg6' 'curl' 'qt4')
source=("http://apt.fuzebox.com/apt/pool/saucy/main/f/fuzelinuxclient/fuzelinuxclient_14.02.14587.saucy_amd64.deb")
md5sums=("fc089d8bf23bf7769f419d8f3d0cc7f1")
build() {
        cd "$srcdir"
        tar zxf ./data.tar.gz
}

package() {
        msg "Moving package files into pkg/ directory..."
        mv "${srcdir}/usr" "${pkgdir}/"
}
