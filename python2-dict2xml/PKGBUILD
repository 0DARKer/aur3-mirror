pkgname=python2-dict2xml
pkgver=0.7
pkgrel=2
pkgdesc="Simple library to convert a native Python dictionary into XML"
arch=('any')
url="https://github.com/quandyfactory/dict2xml/"
license=('GPL2')
depends=('python2')
conflicts=()
replaces=()
source=("https://github.com/quandyfactory/dict2xml/raw/master/dist/dict2xml-$pkgver.tar.gz")

package() {
  cd "${srcdir}/dict2xml-${pkgver}"
  python2 setup.py install --root ${pkgdir}
}

md5sums=('9bd60b170a66ad4406b6292dcf91a8ad')
