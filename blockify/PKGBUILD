pkgname=blockify
pkgver=0.1
pkgrel=1
pkgdesc="mute spotify tracks you don't want to hear"
arch=('any')
url="https://github.com/mikar/blockify"
license=('WTFPL')
depends=('wmctrl')
source=(https://github.com/mikar/blockify/raw/master/blockify)

build() {
  install -d	${pkgdir}/usr/bin
  install -m755 ${srcdir}/${pkgname} ${pkgdir}/usr/bin/
}


