pkgname=ginn
pkgver=0.2.3
pkgrel=1
pkgdesc=""
url="http://launchpad.net/ginn"
arch=('i686' 'x86_64')
license=('GPL')
depends=('utouch-geis' 'bamf' 'libxtst')
source=(http://launchpad.net/ginn/0.x/0.2.3/+download/ginn-0.2.3.tar.gz)

build() {
  cd ${srcdir}/${pkgname}-${pkgver}

  ./configure --prefix=/usr
  make || return 1
  make DESTDIR=${pkgdir} install || return 1
}

md5sums=('593a7222e3c414107adcc6c64b8c7387')
