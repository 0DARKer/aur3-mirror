pkgname=python-openid
pkgver=2.2.5
pkgrel=1
pkgdesc="OpenID library for Python"
arch=('any')
url="http://www.openidenabled.com/openid/libraries/python"
license=('Apache')
depends=('python2')
source=(https://github.com/openid/$pkgname/tarball/$pkgver)
md5sums=('7bb24dc6abd941ebff6a83c1ac7e4370')

build() {
    cd openid-python-openid-b666238
    python2 setup.py build || return 1
    python2 setup.py install --root=$pkgdir || return 1
}
