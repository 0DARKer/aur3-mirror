pkgname=python2-rtmapi
pkgver=0.5.3
pkgrel=1
pkgdesc='API package for rememberthemilk.com'
arch=('any')
url='http://pypi.python.org/pypi/RtmAPI'
license=('MIT')
depends=('python2' 'python2-httplib2')
options=(!emptydirs)
source=("http://pypi.python.org/packages/source/R/RtmAPI/RtmAPI-${pkgver}.zip")
md5sums=('c23c9ddf2b831ce462e4825bfb9412a2')

package() {
  cd "${srcdir}/RtmAPI-${pkgver}"
  python2 setup.py install --root="${pkgdir}/" --optimize=1
}

