pkgname=lib32-libbonoboui
pkgver=2.24.4
pkgrel32=1
pkgrel=1
pkgdesc="User Interface library for Bonobo"
arch=('x86_64')
url="http://www.gnome.org"
depends=('lib32-libgnome' 'lib32-libgnomecanvas')
license=('GPL' 'LGPL')
groups=lib32
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('48bb7b989cf6fd2a5f08d30e92f5bbca')

build() {
  mkdir -p ${startdir}/pkg/opt/lib32/usr/lib
  cp -rPf ${startdir}/src/usr/lib/*.so* ${startdir}/pkg/opt/lib32/usr/lib
}
