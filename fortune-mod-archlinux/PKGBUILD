# Contributer: Benjamin Andresen <benny(at)klapmuetz(dot)org>
pkgname=fortune-mod-archlinux
pkgver=1
pkgrel=1
pkgdesc="#archliux fortune cookie file"
url="irc://irc.freenode.org/archlinux"
depends=('fortune-mod')
groups=('fortune-mods')
source=('archlinux')
md5sums=('5036e8a3e6477b14e730a8e43e9abc15')

build() {
  cd $startdir/src

  strfile archlinux archlinux.dat

  install -D -m644 archlinux $startdir/pkg/usr/share/fortune/archlinux
  install -D -m644 archlinux.dat $startdir/pkg/usr/share/fortune/archlinux.dat
}
# vim: set ft=sh ts=2 et:
