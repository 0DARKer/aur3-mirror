post_install() {
  ln -s /usr/lib/libmfp.so.1.0.1 /usr/lib/libmfp.so.1
  ln -s /usr/lib/libmfp.so.1.0.1 /usr/lib/libmfp.so
  ln -s /usr/lib/libnetsnmp.so.10.0.2 /usr/lib/libnetsnmp.so.10
  ldconfig
}

post_upgrade() {
  ldconfig
}

post_remove() {
  rm /usr/lib/libmfp.so
  rm /usr/lib/libmfp.so.1
  rm /usr/lib/libnetsnmp.so.10
  ldconfig
}
