pkgname=aurvote
pkgver=1.1
pkgrel=1
pkgdesc="Tool to vote for favorite AUR packages"
url="http://gitweb.archlinux.fr/aurvote.git/" 
license="GPL" 
arch=('any')
depends=('curl') 
source=($pkgname) 

build() { 
  	install -D -m 755 "$srcdir/$pkgname" "$pkgdir/usr/bin/$pkgname"
}

md5sums=('ec98e577c4cc81966430d634053a837a')
