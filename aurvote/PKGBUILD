pkgname=aurvote
pkgver=0.4
pkgrel=2
pkgdesc="A tool to vote for favorite AUR's packages for inclusion in [community]" 
url="http://archlinux.fr/" 
license="GPL" 
arch=('any')
depends=('wget') 
source=($pkgname-$pkgver.src.tar.gz) 

build() { 
  	install -D -m 755 $startdir/src/$pkgname/$pkgname $startdir/pkg/usr/bin/$pkgname 
}
md5sums=('3559cbb0fec32dda2976cd1f54e26dc8')
