# PKGBUILD
# Author: jdeb
# Modified: 2011-04-18

pkgname='mixtray'
pkgver='77bddb0'
pkgrel=1
pkgdesc='Tray icon for Alsa mixer control'
arch=('i686' 'x86_64')
url='https://github.com/jdeb/mixtray'
optdepends=()
license=('GPL3')
depends=('qt')
source=("$url/tarball/master/jdeb-$pkgname-$pkgver.tar.gz")
md5sums=('7a3ef3587ad1c53cdf206cebcfb1850d')

build() {
    cd "$srcdir/jdeb-$pkgname-$pkgver"
    qmake
    make
}

package() {
    cd "$srcdir/jdeb-$pkgname-$pkgver"
    make install
}
