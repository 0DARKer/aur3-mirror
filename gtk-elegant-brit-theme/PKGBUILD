pkgname=gtk-elegant-brit-theme
pkgver=20110520
pkgrel=2
pkgdesc="Elegant Brit GTK theme, a mod of Elegant One."
arch=('any')
url="http://gnome-look.org/content/show.php/Elegant+Brit?content=74553"
license=('GPLv3')
depends=('gtk-engine-mist')
source=('http://trizen.go.ro/files/Elegant_Brit.tar.gz')
md5sums=('0d9a06015ca3fb3135ca07167116b9ce')

package() {
	mkdir -p $pkgdir/usr/share/themes/
	cp -r $srcdir/Elegant_Brit/ $pkgdir/usr/share/themes/
}
