pkgname=gtk-elegant-brit-theme
pkgver=1.1.13
pkgrel=2
pkgdesc="Elegant Brit GTK theme, a mod of Elegant One."
arch=('any')
url="http://gnome-look.org/content/show.php/Elegant+Brit?content=74553"
license=('GPL')
depends=('gtk-engine-mist')
source=('http://gnome-look.org/CONTENT/content-files/74553-ElegantBrit.tar.gz')
md5sums=('15f75e411a5168078baa5b5ae74cb04f')

build() {
	mkdir -p $pkgdir/usr/share/themes/
	cp -r $srcdir/Elegant\ Brit/ $pkgdir/usr/share/themes/
}
