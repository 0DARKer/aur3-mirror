# Maintainer: Ben Reedy <thebenj88 *AT* gmail *DOT* com>
pkgname=uif2iso
pkgver=0.1.7c
pkgrel=1
pkgdesc="Open source CLI tool for converting single/multi part UIF image files to ISO."
arch=(i686 x86_64)
url='http://aluigi.org/mytoolz.htm#uif2iso'
license=('GPL')
depends=('zlib' 'glibc')
makedepends=('glibc')
source=(http://download1us.softpedia.com/dl/328033778f7760fe940bc7f8cdc4df35/504c1f19/100094290/software/file_management/$pkgname.zip)
md5sums=('2eb9797ec463c38253014d45591a7043')

build() {
  cd "$srcdir/src"
  make || return 1
  make prefix="$pkgdir/usr" install
}

