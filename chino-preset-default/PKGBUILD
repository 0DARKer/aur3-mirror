# Maintainer: David Adler <david dot jo dot adler at gmail dot com>
pkgname=chino-preset-default
pkgver=0.10	
pkgrel=1
pkgdesc="default preset for chino"
arch=('any')
url="http://chino.tuxfamily.org/"
license=('custom: mixed/public_domain')
depends=('chino')
optdepends=('ams' 'amsynth' 'ardour2' 'gjackclock' 'gjacktransport' 'gjvidtimeline'
'inconcert' 'jaaa' 'jack-rack' 'japa' 'jconvolver' 'jnoisemeter' 'nekobee' 'non-sequencer'
'pd' 'phasex' 'qmidiarp' 'seq24' 'tapeutape' 'tranches' 'whysynth' 'yass' 'yoshimi' 'zita-rev1')
source=(http://download.tuxfamily.org/chino/$pkgname-$pkgver.tar.bz2)
md5sums=('c517ea679f3f933e79980ae552b837d8')

build() {
	cd "$srcdir/$pkgname-$pkgver"
	./configure --prefix=/usr
}

package() {
	cd "$srcdir/$pkgname-$pkgver"
	make DESTDIR=$pkgdir install
}

