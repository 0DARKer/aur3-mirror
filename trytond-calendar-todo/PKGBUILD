pkgname=trytond-calendar-todo
_pkgname=trytond_calendar_todo
pkgver=2.2.0
_pkgdir=2.2
pkgrel=1
pkgdesc="The calendar_todo module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-webdav>=2.2' 'trytond-calendar>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("7633d612da0c40e177d1b7939908456f")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}