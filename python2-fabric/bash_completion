# Fabric completion

have fab &&
{

_fabric()
{
    local cur prev

    COMPREPLY=()
    _get_comp_words_by_ref cur prev

    COMPREPLY=( "${COMPREPLY[@]}" $( compgen -W \
        "$( fab --shortlist )" \
        -- "$cur" ) )

    return 0
}

complete -F _fabric fab
}

