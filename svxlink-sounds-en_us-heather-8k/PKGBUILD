pkgname=('svxlink-sounds-en_us-heather-8k')
pkgver="13.12"
pkgrel=1
arch=('any')
url="http://sourceforge.net/projects/svxlink/"
license=('GPL')
pkgdesc="Sound Package en_US-heather from svxlink in medium quality"
depends=('svxlink')
replace=('sounds-en_us-heather')
source=("${pkgname}-${pkgver}.tar.bz2::http://downloads.sourceforge.net/project/svxlink/sounds/${pkgver}/svxlink-sounds-en_US-heather-8k-${pkgver}.tar.bz2")
sha256sums=('dbd21caa2042d24f15ba056e9dfc2a9938c42e2f57162b0820eee94876ccc538')

package(){
  mkdir -p "${pkgdir}/usr/share/svxlink/sounds/en_US-heather-8k"
  cp -a "${srcdir}/en_US-heather-8k" "${pkgdir}/usr/share/svxlink/sounds/"
}
