pkgname=treeline
pkgver=1.2.4
pkgrel=3
pkgdesc="Tree outliner and PIM which stores any kind of information within a tree structure"
arch=(any)
url="http://treeline.bellz.org"
license=('GPL')
depends=('python2-qt' 'ispell' 'pyxml')
conflicts=('treeline-unstable')
replaces=('treeline-unstable')
source=(http://download.berlios.de/${pkgname}/${pkgname}-$pkgver.tar.gz)
md5sums=('af51957700b68c4af36dc0b5cf0fee92')
sha256sums=('868e3b54cbec370ccbac66c2c525611768bebd07886552cc87c52891c27b3dbd')

build() {
  cd ${srcdir}/TreeLine
  python2 install.py -p /usr -b ${pkgdir}
}
