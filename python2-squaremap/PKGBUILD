pkgname=python2-squaremap
pkgver=1.0.0b25
pkgrel=1
pkgdesc="Hierarchic data-visualisation control for wxPython"
arch=('any')
url="https://launchpad.net/squaremap"
license=('GPL')
depends=('wxpython')
source=('http://pypi.python.org/packages/source/S/SquareMap/SquareMap-1.0.0b25.tar.gz')
md5sums=('47e77f1f6e98e0a1854b74feab78ed2d')

package() {
  cd "$srcdir/SquareMap-$pkgver"
  python2 setup.py install --root=$pkgdir/ --optimize=1
}

