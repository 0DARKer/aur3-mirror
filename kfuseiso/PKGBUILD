# Maintainer: Pilli <elpilli@gmail.com>
pkgname="kfuseiso"
pkgver=20061108
pkgrel=1
pkgdesc="Small set of modules to help access ISO image files"
arch=(i686 x86_64)
license="GPL"
url="http://www.kde-apps.org/content/show.php?content=46526"
depends=(fuseiso kdebase)
source=(http://ubiz.ru/dm/kfuseiso-20061108.tar.bz2)
md5sums=(c7ca433f482d399c399730f1ad7d361f)

build() {
	cd $startdir/src/kfuseiso-20061108
	./configure --prefix=/opt/kde/ --with-qt-dir=/opt/qt
	make || return 1
	make DESTDIR=$startdir/pkg install || return 1
}
