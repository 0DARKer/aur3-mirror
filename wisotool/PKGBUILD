# Maintainer: wido <widomaker2k7@gmail.com>

pkgname=wisotool
pkgver=20101106
pkgrel=1
pkgdesc="a handy winetricks-like script for automatically installing game. "
url="http://wiki.winehq.org/wisotool"
license=('LGPL')
arch=('any')
depends=('wine')
optdepends=('zenity: GUI for GNOME desktop')
provides=('wisotool')
source=(http://kegel.com/wine/wisotool)
md5sums=('12210ac4b1afe53fd6bd3c1c7faa9ef3')

build() {
  install -Dm755 $srcdir/$pkgname $pkgdir/usr/bin/$pkgname || return 1
}
