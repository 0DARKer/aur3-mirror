pkgname=fbless
pkgver=0.2.1
pkgrel=3
pkgdesc="fictionbook console viewer"
url="http://pybookreader.narod.ru/misc.html"
license="GPL"
depends=('python2' 'ncurses')
arch=('i686' 'x86_64')
source=(http://pybookreader.narod.ru/fbless-$pkgver.tar.gz)
md5sums=('83cf3a9a4cdcf85b381085ebbe2ee6e5')

build() {
	cd $startdir/src/fbless-$pkgver
	python2 setup.py install --root=$pkgdir 
}

