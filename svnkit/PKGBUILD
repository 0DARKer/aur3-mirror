pkgname=svnkit
pkgver=1.3.5
pkgrel=1
pkgdesc="Subversion library for java"
arch=('any')
url="http://svnkit.com"
license=('custom')
depends=('java-runtime')
source=(http://svnkit.com/org.tmatesoft.svn_${pkgver}.standalone.zip)
md5sums=(0c74d5373681e7e5094a217fab5757ed)

package()
{
  cd $srcdir/svnkit-${pkgver}*
  install -m644 -D svnkit.jar ${pkgdir}/usr/share/java/svnkit/svnkit.jar
}
