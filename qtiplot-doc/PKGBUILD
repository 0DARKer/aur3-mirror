pkgname=qtiplot-doc
pkgver=20091031
pkgrel=1
pkgdesc="QtiPlot documentations(html)"
url="http://soft.proindependent.com/qtiplot.html"
license=('GDL')
arch=('any')
source=('http://download.berlios.de/qtiplot/qtiplot-manual-en.tar.bz2')
md5sums=('1726b3f57fc29f6137173771dc8a0015')

build()
{	
	INSTALLDIR=${pkgdir}/usr/share/doc/qtiplot/manual

	mkdir -p $INSTALLDIR
	mv $srcdir/qtiplot-manual-en $INSTALLDIR/ 
}

