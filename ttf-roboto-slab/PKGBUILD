# Maintainer: Gavin Lloyd <gavinhungry@gmail.com>

pkgname=ttf-roboto-slab
pkgver=1.100262
pkgrel=1
pkgdesc='Serif Roboto font from Google Keep'
arch=('any')
license=('APACHE')
url='http://www.creativebloq.com/typography/roboto-slab-3132264'
depends=('fontconfig' 'xorg-fonts-encodings' 'xorg-font-utils')
source=('https://gavinhungry.com/pub/RobotoSlab.zip')
md5sums=('4f8e7c34512fd83770f1d38892ae64f1')
install=$pkgname.install

package() {
	install -d $pkgdir/usr/share/fonts/TTF/
	install -m644 *.ttf $pkgdir/usr/share/fonts/TTF/
}

