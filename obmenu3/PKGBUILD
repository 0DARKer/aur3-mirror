pkgname=obmenu3
pkgver=1.03
pkgrel=1
pkgdesc="Openbox menu editor - replacement of obmenu"
url="http://sourceforge.net/projects/obmenu3"
license=('GPL')
depends=('gtk2')
makedepends=('')
arch=('i686' 'x86_64')
source=(http://sourceforge.net/projects/obmenu3/files/$pkgver/obmenu3-$pkgver-$CARCH.tgz)
if [[ $CARCH == 'i686' ]]; then
  md5sums=('a3d2d128d69f13fa3b33acdfb3de6267')
else
  md5sums=('d681c49ae0f251274befdf77e0cc85d4')
fi
package() {
  cp -R "$srcdir/usr" "$pkgdir"
}
