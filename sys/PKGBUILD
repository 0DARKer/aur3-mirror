pkgname=sys
pkgver=0.1
pkgrel=1
pkgdesc='Friendly systemctl wrapper'
arch=('any')
url='http://github.com/xyproto/sys/'
license=('GPL2')
depends=('bash' 'sudo')
source=("$pkgname.zip::https://github.com/xyproto/$pkgname/archive/master.zip")
sha256sums=('597fd2051cd9d2a4f0a5bbb63f6b9bac2488374970bae1b862a28da210f9478b')

package() {
  install -Dm755 "$pkgname-master/$pkgname" "$pkgdir/usr/bin/$pkgname"
}

