pkgname=python-fudge
_realname=fudge
pkgver=0.9.4
pkgrel=1
pkgdesc="a Python module for using fake objects (mocks, stubs, etc) to test real ones."
arch=(any)
url="http://farmdev.com/projects/fudge/"
license=(MIT)
depends=('python')
makedepends=('setuptools')
provides=()
conflicts=()
replaces=()
backup=()
install=
source=(http://pypi.python.org/packages/source/f/fudge/$_realname-$pkgver.tar.gz)
md5sums=('5dd525ad3f8e138100e6f6f4f3a4c678')

build() {
  cd $srcdir/$_realname-$pkgver
  python setup.py install --root=$pkgdir/ --optimize=1 || return 1
}


