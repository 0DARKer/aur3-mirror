pkgname=trytond-stock
_pkgname=trytond_stock
pkgver=2.2.1
_pkgdir=2.2
pkgrel=1
pkgdesc="The stock module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-workflow>=2.2' 'trytond-party>=2.2' 'trytond-product>=2.2' 'trytond-company>=2.2' 'trytond-currency>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("b59172fe95a59e32ff7369e8c293cf4d")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}