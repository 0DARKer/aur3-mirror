pkgname=ssdw
pkgver=1.0
pkgrel=1
pkgdesc="An Script to count how much write of your SSD"
arch=('any')
url="https://github.com/yafengabc"
license=('GPLv3')
depends=('smartmontools')
makedepends=()
conflicts=("$pkgname")
provides=("$pkgname")
source=("ssdw")
#install=$pkgname.install


package() {
  	mkdir $pkgdir/usr
	mkdir $pkgdir/usr/bin
	cp ssdw  $pkgdir/usr/bin
}
