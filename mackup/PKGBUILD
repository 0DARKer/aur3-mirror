pkgname=mackup
pkgver=0.7.3
pkgrel=2
pkgdesc="a small Python utitlity to keep your application settings in sync."
arch=('any')
url="https://github.com/lra/mackup"
license=('GPL3')
depends=('python2')
        
makedepends=('python2')

source=('https://github.com/lra/mackup/archive/0.7.3.tar.gz')

sha256sums=('c9a135049fe5fbf5ce8342fbf3e04320246902672c0047ffd346fea25c604500')



package() {
   cd $srcdir/${pkgname}-${pkgver}
   python2 setup.py install --prefix=/usr --root="$pkgdir/" --optimize=1
}
