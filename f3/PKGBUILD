# Contributor: OldShatterhand <OldShatterhand at gmx-topmail dot de>
pkgname=f3
pkgver=1.1.3
pkgver_=1_1_3
pkgrel=1
pkgdesc="An alternative to h2testw"
url="http://oss.digirati.com.br/f3/"
license=('GPLv3')
arch=('i686' 'x86_64')
source=("http://oss.digirati.com.br/f3/f3v${pkgver_}.zip")
md5sums=('8e8ac000259f26409b7df8547100e1f9')

build() {
gcc -of3write f3write.c
gcc -of3read f3read.c
mkdir $pkgdir/usr
mkdir $pkgdir/usr/bin
cp f3write $pkgdir/usr/bin/f3write
cp f3read $pkgdir/usr/bin/f3read
}