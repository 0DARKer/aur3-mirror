pkgname=python2-everything
pkgver=1.0
pkgrel=1
pkgdesc="Includes all of the Python2 packages in the official repos"
arch=(x86_64 i686)
url="https://github.com/EvanTeitelman/pkgbuilds"
license=('custom')

package() {
  depends=($(pacman -Ssq | grep '^python2-'))
}
