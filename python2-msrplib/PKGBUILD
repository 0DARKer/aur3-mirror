# Maintainer: Andrea Scarpino <andrea@archlinux.org>

pkgname=python2-msrplib
_pkgname=python-msrplib
pkgver=0.12.1
pkgrel=1
pkgdesc="A python client library for the MSRP client"
license=('MIT')
arch=('any')
url="http://download.ag-projects.com/SipClient"
depends=('python2')
source=("http://download.ag-projects.com/SipClient/${_pkgname}-${pkgver}.tar.gz")
md5sums=('9b6aa3a511850010f06761f3512806fb')

package() {
  cd "$srcdir/${_pkgname}-${pkgver}"
  python2 setup.py install --root="${pkgdir}" --optimize=1
}
