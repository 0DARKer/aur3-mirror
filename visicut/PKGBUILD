# Maintainer: Thomas Oster
pkgname=visicut
pkgver=1.3.1
pkgrel=1
epoch=
pkgdesc="A userfriendly tool to generate, save and send Jobs to a Lasercutter"
arch=(any)
url="http://visicut.org"
license=('LGPL')
groups=()
depends=('java-runtime' 'bash')
makedepends=('apache-ant')
checkdepends=()
optdepends=()
provides=()
conflicts=('visicut-git')
replaces=()
backup=()
options=()
install=
changelog=
source=(https://github.com/t-oster/VisiCut/tarball/$pkgver)
md5sums=('0dc6d9e7bff7736e7f6a9db4b5b34ce3')
_gitdir=t-oster-VisiCut-10f9c0e

build() {
  cd "$srcdir/${_gitdir}"
  make
}

package() {
  cd "$srcdir/${_gitdir}"
  make DESTDIR="$pkgdir/" install
}

