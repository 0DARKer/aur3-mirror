# Maintainer: Stefan Husmann <stefan-husmann@t-online.de>

pkgname=emacs-ring+
pkgver=21r211
pkgrel=1
pkgdesc="Extensions to ring.el, needed by emacs-icicles"
arch=('any')
url="http://www.emacswiki.org/emacs/Icicles"
license=(GPL)
depends=('emacs')
source=(ring+.el::http://www.emacswiki.org/emacs/download/ring%2b.el)
md5sums=('ec5f388399cdfeafc3d0bf48258373c8')
install=ring+.install

build() {
  emacs -Q -batch -L .  -f batch-byte-compile ring+.el 
  install -Dm644 ring+.el $pkgdir/usr/share/emacs/site-lisp/ring+.el
  install -Dm644 ring+.elc $pkgdir/usr/share/emacs/site-lisp/ring+.elc
}

