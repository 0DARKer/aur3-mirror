pkgname=kaiketsu
pkgver=0.5
pkgrel=1
pkgdesc="A simple sudoku solver."
arch=('i686')
url="http://www.kde-apps.org/content/show.php?content=28255"
license="GPL"
depends=('kdelibs3')
makedepends=('kdelibs3')
conflicts=()
replaces=()
backup=()
install=
source=(http://web.tiscali.it/gulbros/sito_nuovo/$pkgname.$pkgver.tar.bz2)
md5sums=('81ff0076dc550bc0f9c5aa5d35aff01f')

build() {
  cd $startdir/src/$pkgname.$pkgver
  ./configure --prefix=/opt/kde --without-arts
  make || return 1
  make DESTDIR=$startdir/pkg install
}