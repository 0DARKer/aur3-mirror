# Maintainer: Dan Serban

pkgname=qtmindmap-git
pkgver=latest
pkgrel=1
pkgdesc="MindMap written in QT, aims to provide the features of Freemind"
arch=(i686 x86_64)
url="http://qt-apps.org/content/show.php/QtMindMap?content=143083"
license=(GPL)
depends=(qt)

build()
{
  git clone git://gitorious.org/qtmindmap/qtmindmap.git
  cd qtmindmap
  qmake qtmindmap.pro
  make
  install -Dm755 qtmindmap "${pkgdir}"/usr/bin/qtmindmap
}
