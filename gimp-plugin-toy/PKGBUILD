# $Id: pkgbuild-mode.el,v 1.23 2007/10/20 16:02:14 juergen Exp $
# Maintainer: Kevin Brubeck Unhammer <unhammer@fsfe.org>
pkgname=gimp-plugin-toy  
pkgver=1.0.3
pkgrel=1 
pkgdesc="GIMP plug-in that creates a toy effect or tilt-shift miniature faking on a selected layer, using blur"
url="http://registry.gimp.org/node/25803"
arch=('i686' 'x86_64')
license=('GPL3')
depends=('gimp')
makedepends=()
conflicts=()
replaces=()
backup=()
install=
source=(http://registry.gimp.org/files/$pkgname-$pkgver.tar.gz)
md5sums=('f4553668af4edd452a7ac0a447e980f7')
build() {
  cd $startdir/src/$pkgname-$pkgver
  ./configure --prefix=/usr
  make || return 1
  make DESTDIR=$startdir/pkg install
}