pkgname=cppreference
pkgver=20111225
pkgrel=1
pkgdesc="a complete reference for the features in the C++ Standard Library. "
arch=('any')
url="http://en.cppreference.com/"
license=('CCPL:cc-by-sa')
source=(http://ppa.launchpad.net/p12/ppa/ubuntu/pool/main/c/"$pkgname"-doc/"$pkgname"-doc-en_"$pkgver"-"$pkgrel"ppa1_all.deb)
depends=("ttf-dejavu")
optdepends=("devhelp")
makedepends=("file-roller")
md5sums=('c331d7005dfd09fbb972fc7b4c63f044')

build() {
	ls $srcdir
	tar xvzf $srcdir/data.tar.gz
}

package() {
	mv $srcdir/usr $pkgdir
	ls $pkgdir
}