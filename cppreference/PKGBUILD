pkgname=cppreference
pkgver=20120227
pkgrel=1
pkgdesc="a complete reference for the features in the C++ Standard Library. "
arch=('any')
url="http://en.cppreference.com/"
license=('CCPL:cc-by-sa')
source=("http://upload.cppreference.com/mwiki/images/8/85/html_book_20120227.zip")
md5sums=('40cc2ef661b172f4f3cb0989f42989ca')

build() {
    mkdir -p $pkgdir/usr/share/doc
    mv output $pkgdir/usr/share/doc/cppreference
}

