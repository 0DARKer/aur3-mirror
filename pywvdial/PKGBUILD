pkgname=pywvdial
pkgname_2=pyWvDial
pkgver=0.5
pkgrel=1
pkgdesc="Frontend for wvDial written in python"
arch=(i686)
url=(http://www.develia.org)
depends=(python wvdial pygtk)
license=GPLv2
source=(http://www.develia.org/files/$pkgname_2-$pkgver.tar.gz)
md5sums=('6ab2c0231583c4179f4fb6f9b6bbbe2c')

build(){
    cd $startdir/src/$pkgname_2-$pkgver
    python setup.py install --prefix=/usr --root=$startdir/pkg || return 1
}
