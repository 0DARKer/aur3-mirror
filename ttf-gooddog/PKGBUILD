pkgname=ttf-gooddog
_pkgname=gooddog
pkgver=1.0
pkgrel=1
depends=('fontconfig' 'xorg-font-utils')
pkgdesc="Hand writing like true type font"
arch=('any')
license=('EULA')
source=('http://www.fontsquirrel.com/fonts/download/GoodDog')
url="http://www.fontsquirrel.com/fonts/GoodDog"
install=$pkgname.install
md5sums=('ef747676ae526d77d9e0bac989af557d')

build()
{
  mkdir -p $pkgdir/usr/share/fonts/TTF
  cp $srcdir/*.otf $pkgdir/usr/share/fonts/TTF
}

