pkgname=openarch_skel
pkgver=0.2.11
pkgrel=1
pkgdesc="Skeleton files for OpenArch"
arch=('any')
url="http://ovsinc.narod.ru/"
license=('GPL')
depends=('openarch_openbox_scripts' 'openarch_dmenu_scripts' 'openarch_artwork_gray' 'pcmanfm' 'wbar')
source=('http://ovsinc.narod.ru/skel/skel_openarch.tar.gz')


build() {
	install -d "${pkgdir}"/etc
	cp -R "${srcdir}"/skel_openarch "${pkgdir}"/etc/
}

