pkgname=openarch_skel
pkgver=0.2.22
pkgrel=2
pkgdesc="Skeleton files for OpenArch"
arch=('any')
url="http://ovsinc.narod.ru/"
license=('GPL')
depends=('openarch_openbox_scripts'
 'openarch_dmenu_scripts' 
 'openarch_artwork_gray' 
 'pcmanfm' 
 'wbar' 
 'openarch_xconf' 
 'openarch_systemd'
 'openbox-xdgmenu'
 'gnome-menus2'
 'lxmenu-data')
source=('http://ovsinc.narod.ru/skel/skel_openarch.tar.gz' 'profile.tar.gz')


build() {
	install -d "${pkgdir}"/etc
	cp -R "${srcdir}"/skel_openarch "${pkgdir}"/etc/
	cp -R "${srcdir}"/profile.d "${pkgdir}"/etc/
}

md5sums=('da6a03c321eb1953a82b9ecd0de1c5e2'
         '7b81711e95bcf05aee3377926d95a0af')
