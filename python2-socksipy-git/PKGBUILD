pkgname=python2-socksipy-git
pkgver=1.04
pkgrel=1
pkgdesc="Routes sockets of any TCP service through a SOCKS4, SOCKS5 or HTTP proxy - Anorov fork"
arch=('any')
license=('BSD')
url="http://socksipy.sourceforge.net/"
depends=('python2')
provides=('socksipy')
conflicts=('socksipy')
source=("https://github.com/Anorov/PySocks/archive/master.zip")
md5sums=('0e5c5f4cb2508199229dc9e99e1ea2bf')

package() {
  cd "$srcdir/PySocks-master"
  python2 setup.py install --root="$pkgdir" --optimize=1
}
