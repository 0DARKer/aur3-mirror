# Mantainer: harry218

pkgname=ttf-weblysleekui
pkgver=2014.09.20
pkgrel=3
pkgdesc="Alternative fonts to Segoe UI family."
arch=('any')
url="http://fontpro.com/weblysleek-ui-font-9838"
license=('GPL3')
#depends=
source=(http://sourceforge.net/projects/numix-square-xfwm4/files/fonts/WeblySleekUI.zip/download)
md5sums=('d9cb514cd6b6be898399800ae3e8330e')

package() {
	mkdir -p ${pkgdir}/usr/share/fonts/TTF/
	rm ${srcdir}/download
	cp -r ${srcdir}/* ${pkgdir}/usr/share/fonts/TTF/
}