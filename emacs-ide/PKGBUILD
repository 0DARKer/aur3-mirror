    # Maintainer: examon <examon.mail[at]gmail[dot]com>
    pkgname=emacs-ide
    pkgver=1.7
    pkgrel=1
    pkgdesc="Emacs-IDE is an Integrated Development Environment (IDE) interface for Emacs (code browsing, compilation, debug...)."
    url="http://gna.org/projects/emacs-ide/"
    arch=('x86_64' 'i686')
    license=('GPLv3')
    depends=('emacs' 'ctags' 'cscope')
    optdepends=()
    makedepends=()
    conflicts=()
    replaces=()
    backup=()
    install=emacs-ide.install
    source=("http://download.gna.org/emacs-ide/${pkgname}_${pkgver}.tar.bz2")
    md5sums=('ec3a77ff6a273e8b123d5fc75220c9c7')
     
    build() {
        tar xf ${pkgname}_${pkgver}.tar.bz2
        cd ${pkgname}_${pkgver}
   	./install
    }

