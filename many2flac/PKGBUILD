# Maintainer and contributor: m0rph < m0rph.mailbox (at) gmail.com >

pkgname=many2flac
pkgver=1.0
pkgrel=1
pkgdesc="ALAC/APE to FLAC converter script"
arch=(any)
depends=('ffmpeg' 'flac')
license=('GPLv3')
url=('http://localhost')
source=(many2flac)
md5sums=('f8e131677bd529b3cd2dda6cf3a4fdba')

build() {
    cd ${startdir}/pkg || return 1
    mkdir -p usr/bin || return 1
    cp ${startdir}/many2flac usr/bin/
    chmod +x usr/bin/many2flac
}
