
pkgname=lib32-libsexymm
pkgname32=${pkgname/lib32-/}
pkgver=0.1.9
pkgrel=4
_pkgrel32=3
pkgdesc="C++ bindings for libsexy"
arch=('x86_64')
depends=('gtkmm>=2.18.2' 'libsexy>=0.1.11')
makedepends=()
url=(http://oss.sgi.com/projects/xfs/)
groups=lib32
license=('GPL')
source=(ftp://mirrors.kernel.org/archlinux/extra/os/i686/libsexymm-0.1.9-3-i686.pkg.tar.gz)
md5sums=('8529295d96b9d21655b29800fc1bb434')

build() {
  mkdir -p $startdir/pkg/opt/lib32/lib/
  mkdir -p $startdir/pkg/opt/lib32/usr/lib/
  cp -R $startdir/src/lib/*.so* $startdir/pkg/opt/lib32/lib/
  cp -R $startdir/src/usr/lib/*.so* $startdir/pkg/opt/lib32/usr/lib/
}
