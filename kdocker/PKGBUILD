# Maintainer: DonVla <donvla@users.sourceforge.net>

pkgname=kdocker
pkgver=4.5
pkgrel=1
pkgdesc="dock any application into the system tray"
arch=('i686' 'x86_64')
url="https://launchpad.net/kdocker"
license=(GPL)
depends=('qt')
source=(http://launchpad.net/${pkgname}/trunk/${pkgver}/+download/${pkgname}-${pkgver}.tar.gz)
md5sums=('f08b9e04a1846616dcd46877062ede3a')
sha256sums=('84277100a8875b7b1cc074500e1838c379184d5f4807d61e0f2622d05b7cf347')

build() {
    cd "${srcdir}/${pkgname}-${pkgver}"
    qmake
    make
}

package(){
    cd "${srcdir}/${pkgname}-${pkgver}"
    make INSTALL_ROOT=${pkgdir} install
}
