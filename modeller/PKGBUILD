pkgname=modeller
pkgver=9.9
pkgrel=1
pkgdesc="3D Structure Homology Modeller"
arch=('i686' 'x86_64')
url="http://salilab.org/modeller/"
license=('custom')
depends=('python2' 'glib2')
install=modeller.install
source=(http://www.salilab.org/modeller/$pkgver/modeller-$pkgver.tar.gz Install_Arch_Linux)
md5sums=('d6654581c8d449e7defb5bc36296a871'
         'c04a42f203c2ddd4c8dbf21365a2718d')
build() {
    cd $srcdir/$pkgname-$pkgver 
    install -m 755 ../Install_Arch_Linux . || return 1
    ./Install_Arch_Linux $pkgver $pkgdir || return 1
}
