build() {
  cd -- "$srcdir/$pkgname-$pkgver"
  install -D -m644 "en_XX@POSIX" "$pkgdir/usr/share/i18n/locales/en_XX@POSIX"
#   install -D -m755 "en_XX.sh" "$pkgdir/etc/profile.d/en_XX.sh"
}

