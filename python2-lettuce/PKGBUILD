pkgname=python2-lettuce
pkgver=0.1.32
pkgrel=1
pkgdesc="Behaviour Driven Development for python."
arch=(any)
url="http://lettuce.it/"
license=('GPL3')
depends=('python2')
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=(!emptydirs)
source=("http://pypi.python.org/packages/source/l/lettuce/lettuce-$pkgver.tar.gz")
noextract=()
md5sums=('2570bcf5952ad2e84232b32256a9c961')
build() {
    cd $srcdir
    python2 setup.py install --root=$pkgdir/ --optimize=1
}

