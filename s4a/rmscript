#! /bin/bash

rm -r /usr/share/s4a/

rm /usr/share/applications/s4a.desktop

rm /usr/share/icons/hicolor/128x128/apps/s4a.png
rm /usr/share/icons/hicolor/128x128/mimetypes/gnome-mime-application-x-s4a-project.png
rm /usr/share/icons/hicolor/48x48/apps/s4a.png
rm /usr/share/icons/hicolor/48x48/mimetypes/gnome-mime-application-x-s4a-project.png

rm /usr/share/mime/packages/s4a.xml

# Binaries
rm /usr/bin/s4a /usr/bin/s4a_squeak_vm
