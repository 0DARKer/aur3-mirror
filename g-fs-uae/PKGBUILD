pkgname=g-fs-uae
_name=G-FS-UAE
pkgver=0.0.53
pkgrel=1
pkgdesc="A SIMPLE GUI FOR FS-UAE AMIGA EMULATOR "
arch=('any')
url="http://sourceforge.net/projects/gfsuae"
license='GPL'
depends=('gambas3-runtime' 'fs-uae')

#source=(http://sourceforge.net/projects/gfsuae/files/Package/debian.tar.gz)
source=(http://heanet.dl.sourceforge.net/project/gfsuae/Package/debian.tar.gz)
md5sums='85e1d379c4d6a7fa3094d3198e0a9123'

build() {
  tar -xaf $srcdir/debian.tar.gz
  ar -x ${srcdir}/debian/${pkgname}_${pkgver}-${pkgrel}_all.deb
  tar -xaf ${srcdir}/data.tar.gz

 }

 package() {
 
 cp -r ${srcdir}/usr/ ${startdir}/pkg/ 
}