#makepkgopt = libtool
#makepkgopt = emptydirs
#makepkgopt = zipman
#makepkgopt = purge
#makepkgopt = !upx
source=("http://downloads.sourceforge.net/project/$pkgname/${pkgver}/Source/$pkgname-${pkgver}.tar.gz")

package () {
    cd "$srcdir/$pkgname-$pkgver"
	python2.7 setup.py install --root="$pkgdir/" --optimize=1
}

