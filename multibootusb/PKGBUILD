pkgname=multibootusb
pkgver=6.2.0
pkgrel=3
pkgdesc="Boot multiple live Linux distros from a usb flash drive"
arch=('i686' 'x86_64')
url="http://sourceforge.net/projects/multibootusb"
license=('gpl3')
depends=('gambas3-gb-image' 'gambas3-gb-gtk' 'gambas3-gb-form' 'syslinux')
sha512sums=('6ccfc9380469d8fc3f41da62cfbafdcb75baf39b8621c225d9aadc34b9aeff4e3768a74540b0770732921846e5f0d65193b7f6fd7fdf10518f27fc4834d91b06')
