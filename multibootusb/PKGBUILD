pkgname=multibootusb
pkgver=6.0.0
pkgrel=1
pkgdesc="Boot multiple live Linux distros from a usb flash drive"
arch=('i686' 'x86_64')
url="http://sourceforge.net/projects/multibootusb"
license=('gpl3')
sha512sums=('30ddfeae29f8b994e97fd7333090d565bfd9e4ca68d236be13b6ce942569ed04f072d88a22ae3364c28c5bd1ea4e32f01d8a306aeb67196c2f7f17752f4f5881')
source=("http://downloads.sourceforge.net/project/multibootusb/Linux/Archlinux/${pkgname}-${pkgver}-${pkgrel}-any.pkg.tar.xz")
package () {
    tar -C ${pkgdir} -xvf ${srcdir}/${pkgname}-${pkgver}-${pkgrel}-any.pkg.tar.xz
}

