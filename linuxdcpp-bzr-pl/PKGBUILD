# Contributor: 3ED <krzysztof1987@gmail.com>
pkgname=linuxdcpp-bzr-pl
pkgver=278
pkgrel=2.20081229
pkgdesc="Polish support for linuxdcpp"
arch=('any')
url="https://bugs.launchpad.net/linuxdcpp/+bug/311482/comments/1"
license=('GPL')
depends=('linuxdcpp-bzr')
makedepends=('gettext')
source=(linuxdcpp.pl.po)
md5sums=('fa1853110b76fef7892aec006409ceca')
build() {
  cd "$srcdir"

  msgfmt "linuxdcpp.pl.po" -o "linuxdcpp.mo" || return 1
  install -d "$pkgdir/usr/share/locale/pl/LC_MESSAGES/" || return 1
  install -m 644 "linuxdcpp.mo" \
    "$pkgdir/usr/share/locale/pl/LC_MESSAGES/linuxdcpp.mo" || return 1
}

#$cat=i18n
