pkgname=y-cruncher
pkgver=0.6.3.9415
pkgrel=1
pkgdesc="The first scalable multi-threaded Pi-benchmark for multi-core systems."
arch=('x86_64')
url="http://www.numberworld.org/y-cruncher/"
license=('unknown')
options=(!strip)
depends=('gcc-libs')
#y-cruncher%20v${pkgver}.tar.gz::
source=(http://www.numberworld.org/y-cruncher/y-cruncher%20v${pkgver}.tar.gz)
md5sums=('41206e83d0a90d694c4ea8e740e0fe2f')

package() {
  install -D -m755 ${srcdir}/"y-cruncher v${pkgver}"/"x64 SSE3 ~ Kasumi.out" \
                   ${pkgdir}/usr/bin/y-cruncher
}
