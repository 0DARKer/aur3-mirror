pkgname=y-cruncher
pkgver=0.6.5.9444b
pkgrel=1
pkgdesc="The first scalable multi-threaded Pi-benchmark for multi-core systems."
arch=('x86_64')
url="http://www.numberworld.org/y-cruncher/"
license=('unknown')
options=(!strip)
depends=('gcc-libs')
source=(y-cruncher-${pkgver}.tar.gz::"http://www.numberworld.org/y-cruncher/y-cruncher%20v${pkgver}%20(fix%202).tar.gz")
md5sums=('a77ad76e713c769401e8666b39900bf7')

package() {
  install -D -m755 ${srcdir}/"y-cruncher/Binaries"/"x64 SSE3 ~ Kasumi.out" \
                   ${pkgdir}/usr/bin/y-cruncher
}
