pkgname=dorian-theme-slate
pkgver=3.14_12_03162015
pkgrel=1
pkgdesc="A sleek dark theme for gtk2+3 Gnome-Shell Cinnamon MATE Xfwm4 Unity"
arch=('any')
url="http://fav.me/d86da4w"
license=('GPLv3')
depends=('gtk-engines' 'gnome-themes-standard>=3.14' 'gtk-engine-murrine>=0.98.2')

source=("https://dl.dropboxusercontent.com/u/330352/dorian-theme-slate-3.14.01_03162015.zip")


md5sums=('26d22fd1209b85925033a82abaac8993')


package() {
  # install themes

  find Dorian-theme-slate*/ -type f \
      -exec install -Dm644 "{}" "$pkgdir/usr/share/themes/{}" \;           
}

