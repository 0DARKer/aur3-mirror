# Contributor: Scott Garrett <Wintervenom@archlinux.us>

pkgname=libtifiles
pkgver=1.1.4
pkgrel=1
pkgdesc="TI File format library"
arch=(i686 x86_64)
url="http://tilp.info"
license="GPL"
depends=('zlib' 'libticonv' 'glib2')
source=("http://downloads.sourceforge.net/project/tilp/tilp2-linux/tilp2-1.15/${pkgname}2-${pkgver}.tar.bz2")

build() {
  cd $srcdir/${pkgname}2-$pkgver
  ./configure --prefix=/usr
  make
  make DESTDIR=$pkgdir install
}

md5sums=('81fa27bfe777f2c6baa6f56f95bed0c2')
