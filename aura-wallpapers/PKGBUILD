pkgname=aura-wallpapers
pkgver=1.0
pkgrel=1
pkgdesc="A variety of Chrome OS (Aura) Wallpapers"
arch=('any')
url="https://picasaweb.google.com/100132233764003563318/AuraWallpapers?noredirect=1"
license=('unknown')
install=${pkgname}.install
source=(https://dl.dropboxusercontent.com/u/4776753/${pkgname}.tar.bz2)
md5sums=('b2a4ce6fac76ca792e57070551d340b2')

package() {
    cd "${srcdir}/${pkgname}"
    install -d "${pkgdir}/usr/share/backgrounds"
    install -m644 *.jpg "${pkgdir}/usr/share/backgrounds/"
}
