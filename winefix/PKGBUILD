# Contributor: Daenyth <Daenyth+Arch AT gmail DOT com>
pkgname=winefix
pkgver=1.02
pkgrel=4
pkgdesc="An advanced wine wrapper script"
arch=(i686)
url="http://ubuntuforums.org/showthread.php?t=533257"
license=('GPL')
depends=(wine zenity)
source=(http://download.softpedia.ro/dl/9138a15a2e425b91ac40b1717be4ddc8/4909a2b4/500031532/linux/${pkgname}.tar.gz)
# Note: I use the download.com source because ubuntuforums requires you to be logged in to download an attachment.

build() {
  cd "$srcdir"

  install -Dm755 $pkgname "$pkgdir/usr/bin/$pkgname"
}

# vim:set ts=2 sw=2 et:
md5sums=('dc9128774642d8725c5e750b67a87f29')
