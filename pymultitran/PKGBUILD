pkgname=pymultitran
pkgver=0.0
pkgrel=1
pkgdesc="CLI client for http://multitran.ru written in python."
arch=('any')
url="http://example.com"
license=(BSD)
depends=('python3')
source=('pymultitran.py')
md5sums=('22a2309e508a05047c934b54b6e701f5')
sha256sums=('920710ef3b2090e5bd4a8d5e296dcde794baf110bf61d899e06948344216f24b')

package() {
	install -Dm755 pymultitran.py "${pkgdir}/usr/bin/pymultitran"
}

