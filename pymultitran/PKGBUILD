pkgname=pymultitran
pkgver=0.1
pkgrel=3
pkgdesc="CLI client for http://multitran.ru written in python."
arch=('any')
url="http://example.com"
license=(BSD)
depends=('python3')
source=('pymultitran.py')
md5sums=('4ddb62340080caed8db85ee06e1a5209')
sha256sums=('b6e5eaa6caa9dbdbbd6301d76429e96f059dbcaaacf2e8b48a95cc6e43a0718b')

package() {
	install -Dm755 pymultitran.py "${pkgdir}/usr/bin/pymultitran"
}

