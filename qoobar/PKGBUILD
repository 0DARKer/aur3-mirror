pkgname=qoobar
pkgver=1.3.1
pkgrel=1
pkgdesc="Qoobar - Audio Tagger for Classical Music. It can - Edit ID3v2.4, Xiph.org, APE, ASF tags."
arch=('i686' 'x86_64')
url="http://qoobar.sourceforge.net"
license=('GPL')
depends=('qt' 'taglib' 'taglib-extras' 'librcd' 'libdiscid')
source=(http://freefr.dl.sourceforge.net/project/qoobar/qoobar%20${pkgver}/src/qoobar-${pkgver}-src.tar.gz)
md5sums=('1a096e9eb47f8ee2fefe848d18e98f34')

build() {
cd  $startdir/src/${pkgname}-${pkgver}/ 
    qmake qoobar.pro || return 1
    make || return 1
    make INSTALL_ROOT=${pkgdir} install || return 1
}
