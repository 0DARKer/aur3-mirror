# Mantainer: M0Rf30

pkgname=jdk7-docs
_prefix=u21
_build=b11
pkgver=${_prefix}${_build}
pkgrel=1
pkgdesc="Java SE Development Kit Documentation 7"
arch=('any')
url="http://java.sun.com/"
license=('custom')
options=('docs' '!strip')
source=("http://download.oracle.com/otn-pub/java/jdk/7${_prefix}-${_build}/jdk-7${_prefix}-apidocs.zip")
conflicts=('jdk-docs')

package(){
  cd "$srcdir/docs"
  mkdir -p "$pkgdir/usr/share/doc/java7" 
  cp -rf * "$pkgdir/usr/share/doc/java7" 
}


md5sums=('6479fea256742d048f36ed1077e0da95')
