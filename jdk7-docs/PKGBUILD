# Mantainer: M0Rf30

pkgname=jdk7-docs
pkgver=u15b02
pkgrel=1
pkgdesc="Java SE Development Kit Documentation 7"
arch=('any')
url="http://java.sun.com/"
license=('custom')
options=('docs' '!strip')
source=('http://download.oracle.com/otn-pub/java/jdk/7u15-b02/jdk-7u15-apidocs.zip')
conflicts=('jdk-docs')

build(){
  cd "$srcdir/docs"
  mkdir -p "$pkgdir/usr/share/doc/java7" 
  cp -rf * "$pkgdir/usr/share/doc/java7" 
}

md5sums=('76b852c1a88a36749c8ef816a2cc9ce6')
