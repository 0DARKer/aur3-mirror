# Mantainer: M0Rf30

pkgname=jdk7-docs
pkgver=u2b13
pkgrel=1
pkgdesc="Java SE Development Kit Documentation 7"
arch=('any')
url="http://java.sun.com/"
license=('custom')
options=('docs' '!strip')
source=('http://download.oracle.com/otn-pub/java/jdk/7u2-b13/jdk-7u2-apidocs.zip')
conflicts=('jdk-docs')

build(){
  cd "$srcdir/docs" || return 1
  mkdir -p "$pkgdir/usr/share/doc/java" || return 1
  cp -rf * "$pkgdir/usr/share/doc/java" || return 1
}

md5sums=('d8b0f663f1f11c432de3bffd9399e82e')
