# Mantainer: M0Rf30

pkgname=jdk7-docs
pkgver=u11b21
pkgrel=1
pkgdesc="Java SE Development Kit Documentation 7"
arch=('any')
url="http://java.sun.com/"
license=('custom')
options=('docs' '!strip')
source=('http://download.oracle.com/otn-pub/java/jdk/7u11-b21/jdk-7u11-apidocs.zip')
conflicts=('jdk-docs')

build(){
  cd "$srcdir/docs"
  mkdir -p "$pkgdir/usr/share/doc/java7" 
  cp -rf * "$pkgdir/usr/share/doc/java7" 
}

md5sums=('e953632be4ac21cfeda756f263a9e193')
