# Mantainer: M0Rf30

pkgname=jdk7-docs
pkgver=b147
pkgrel=2
pkgdesc="Java SE Development Kit Documentation 7"
arch=('any')
url="http://java.sun.com/"
license=('custom')
options=('docs' '!strip')
source=('http://download.oracle.com/otn-pub/java/jdk/7/jdk-7-fcs-bin-b147-apidocs-27_jun_2011.zip')
conflicts=('jdk-docs')

build(){
  cd "$srcdir/docs" || return 1
  mkdir -p "$pkgdir/usr/share/doc/java" || return 1
  cp -rf * "$pkgdir/usr/share/doc/java" || return 1
}
md5sums=('3e1868a1276cdfb9998fbceb5d2afaa8')
