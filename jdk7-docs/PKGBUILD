# Mantainer: M0Rf30

pkgname=jdk7-docs
pkgver=u6b24
pkgrel=2
pkgdesc="Java SE Development Kit Documentation 7"
arch=('any')
url="http://java.sun.com/"
license=('custom')
options=('docs' '!strip')
source=('http://download.oracle.com/otn-pub/java/jdk/7u6-b24/jdk-7u6-apidocs.zip')
conflicts=('jdk-docs')

build(){
  cd "$srcdir/docs"
  mkdir -p "$pkgdir/usr/share/doc/java7" 
  cp -rf * "$pkgdir/usr/share/doc/java7" 
}

md5sums=('91d8188362c70a834697a58121b2d2a6')
