# Mantainer: M0Rf30

pkgname=jdk7-docs
pkgver=u4b20
pkgrel=1
pkgdesc="Java SE Development Kit Documentation 7"
arch=('any')
url="http://java.sun.com/"
license=('custom')
options=('docs' '!strip')
source=('http://download.oracle.com/otn-pub/java/jdk/7u4-b20/jdk-7u4-apidocs.zip')
conflicts=('jdk-docs')

build(){
  cd "$srcdir/docs" || return 1
  mkdir -p "$pkgdir/usr/share/doc/java" || return 1
  cp -rf * "$pkgdir/usr/share/doc/java" || return 1
}

md5sums=('d1f9217f6a8e3ec394b651b3b7ef58c3')
