pkgname=trytond-project
_pkgname=trytond_project
pkgver=1.4.0
pkgrel=2
pkgdesc="Project module of the Tryton application platform"
arch=('any')
url="http://www.tryton.org/"
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=1.4' 'trytond-timesheet>=1.4' 'trytond-party>=1.4'
'trytond-company-work-time>=1.4')
makedepends=('setuptools')
source=("http://downloads.tryton.org/1.4/$_pkgname-$pkgver.tar.gz")
md5sums=('8c1e4227987784bf2c19b03fc50dd71a')

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}
