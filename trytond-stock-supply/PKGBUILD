arch=('any')
url="http://www.tryton.org/"
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=1.4' 'trytond-product>=1.4' 'trytond-stock>=1.4'
'trytond-purchase>=1.4' 'trytond-party>=1.4' 'trytond-account>=1.4')
