pkgname=trytond-stock-supply
_pkgname=trytond_stock_supply
pkgver=2.2.0
_pkgdir=2.2
pkgrel=1
pkgdesc="The stock_supply module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-product>=2.2' 'trytond-stock>=2.2' 'trytond-purchase>=2.2' 'trytond-party>=2.2' 'trytond-account>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("f035e51c0b2896ae984c5c1edde4d2d0")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}