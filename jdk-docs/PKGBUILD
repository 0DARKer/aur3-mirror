pkgname=jdk-docs
pkgver=7.21
pkgrel=1
pkgdesc="Java SE Development Kit Documentation 7"
arch=('any')
url="http://java.sun.com/"
license=('custom')
options=('docs' '!strip')
source=('http://download.oracle.com/otn-pub/java/jdk/7u21-b11/jdk-7u21-apidocs.zip')
md5sums=('6479fea256742d048f36ed1077e0da95')

build(){
  cd "$srcdir/docs"
  mkdir -p "$pkgdir/usr/share/doc/java"
  cp -rf * "$pkgdir/usr/share/doc/java"
}
