pkgname=jdk-docs
pkgver=7.25
pkgrel=1
pkgdesc="Java SE Development Kit Documentation 7"
arch=('any')
url="http://java.sun.com/"
license=('custom')
options=('docs' '!strip')
source=('http://download.oracle.com/otn-pub/java/jdk/7u25-b15/jdk-7u25-apidocs.zip')
md5sums=('956d3784f361426dbb53f72b39aa8a57')

build(){
  cd "$srcdir/docs"
  mkdir -p "$pkgdir/usr/share/doc/java"
  cp -rf * "$pkgdir/usr/share/doc/java"
}
