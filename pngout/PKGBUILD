pkgname=pngout
pkgver=20110415
pkgrel=1
pkgdesc="Lossless PNG compressor (static build)"
arch=('x86_64')
url="http://www.jonof.id.au/kenutils"
license=('custom')
source=(http://static.jonof.id.au/dl/kenutils/pngout-$pkgver-linux-static.tar.gz)
md5sums=('2449a30944f0199789547bfd8b09af95')

build() {
  cd ${srcdir}/pngout-$pkgver-linux-static/x86_64
  install -D -m755 pngout-static ${pkgdir}/usr/bin/pngout
}
