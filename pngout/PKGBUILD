# Contributor: gueek <cyttam[at]gmail[dot]com>

pkgname=pngout
pkgver=20110722
pkgrel=2
pkgdesc="Lossless PNG compressor (static build)"
arch=('x86_64')
url="http://www.jonof.id.au/kenutils"
license=('custom')
source=(http://static.jonof.id.au/dl/kenutils/pngout-$pkgver-linux-static.tar.gz
	pngout.completion)
md5sums=('f19ac933fc9efc20814d5472e3343ad3'
	 '99a75d19ff6278f04c66358ce67e8eed')

package() {
  cd ${srcdir}/${pkgname}-${pkgver}-linux-static/x86_64
  install -Dm755 pngout-static ${pkgdir}/usr/bin/pngout
  install -Dm644 ${srcdir}/pngout.completion ${pkgdir}/etc/bash_completion.d/pngout
}
