pkgname=gnome-shell-theme-dark-shine
pkgver=20111103
pkgrel=1
pkgdesc="Dark variant of the shine theme by satya164." 
url="http://satya164.deviantart.com/art/Gnome-Shell-Dark-Shine-266329752"
license=('GPL')
arch=('any')
depends=('gnome-shell')
optdepends=('gnome-shell-extension-user-theme: User Theme extension for GNOME Shell'
            'gnome-tweak-tool: A tool to customize advanced GNOME 3 options.')
source=("http://www.deviantart.com/download/266329752/gnome_shell___dark_shine_by_satya164-d4ekdco.zip")
md5sums=('d9072692c75b2318d9a07d34f5b5ce3c')

package() {
  mkdir -p ${pkgdir}/usr/share/themes/
  mv ${srcdir}/Dark\ Shine ${pkgdir}/usr/share/themes/  

}


