pkgname=rutorrent
pkgver=3.3
pkgrel=1
pkgdesc="Yet another web front-end for rTorrent"
arch=('any')
url="http://code.google.com/p/rutorrent/"
license=('GPL')
depends=('rtorrent' )
source=(http://rutorrent.googlecode.com/files/$pkgname-$pkgver.tar.gz)
md5sums=('ef678b8209e68a036ebf82c7a982aad6')

package() {
  cd ${srcdir}

  install -d ${pkgdir}/srv/http
  cp -r $pkgname ${pkgdir}/srv/http
} 
