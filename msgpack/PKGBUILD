# $Id: PKGBUILD,v 1.12 2003/11/06 08:26:13 dorphell Exp $
# Contributor: Leen Jewel <leenjewel@gmail.com>
pkgname=msgpack
pkgver=0.5.2
pkgrel=1
pkgdesc="MessagePack is a binary-based efficient object serialization library."
arch=(i686 x86_64)
url="http://msgpack.org/"
license=('GPL')
source=(http://downloads.sourceforge.net/project/msgpack/msgpack/cpp/$pkgname-$pkgver.tar.gz)
md5sums=('bb796446d4c24170a14bad0594c70786')

build() {
  cd $startdir/src/$pkgname-$pkgver
  ./configure
  make || return 1
  make install
  ldconfig
}

