# Maintainer: Simon Conseil <contact+aur at saimon dot org>
pkgname=python2-jedi
pkgver=0.6.0
pkgrel=1
pkgdesc="An autocompletion tool for Python that can be used for text editors."
arch=('any')
url="https://github.com/davidhalter/jedi"
license=('LGPL')
depends=('python2')
source=("http://pypi.python.org/packages/source/j/jedi/jedi-${pkgver}.tar.gz")
md5sums=('2e2d953bf8213e56478e5df886ea7c02')

package() {
  cd "$srcdir/jedi-$pkgver"
  python2 setup.py install --root="$pkgdir/" --optimize=1
}
