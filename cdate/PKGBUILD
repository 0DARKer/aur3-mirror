# Maintainer: vinoca <supoingo@163.com>
# Contributor: vinoca <supoingo@163.com>

pkgname=cdate
pkgver=1.0.1
pkgrel=2
pkgdesc="Show Chinese lunisolar calender date"
arch=('i686' 'x86_64')
url="http://www.vinoca.org/"
license=('BSD')
depends=('')
source=(http://vincasrcfiles.googlecode.com/files/$pkgname-$pkgver.tar.gz)
sha1sums=('2eaa61a354988e9f57f6cb995afe7fc8b232178c')

build() {
  cd "$srcdir/$pkgname-$pkgver"

  ./configure --prefix=/usr
  make || return 1
  make DESTDIR="$pkgdir" install || return 1
}

