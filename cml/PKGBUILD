pkgname=cml
pkgver=1.0.2
pkgrel=1
pkgdesc="Configurable Math Library"
arch=('i686' 'x86_64')
url="http://cmldev.net"
license=('BOOST')
depends=()
source=(http://downloads.sourceforge.net/project/cmldev/Configurable%20Math%20Library/CML%201.0/cml-1_0_2.zip)
md5sums=('6584ba1ef71b8d1f031efdac5ac311b4')

build() {
  mkdir -p $pkgdir/usr/include
  cp -R $srcdir/cml-1_0_2/cml $pkgdir/usr/include/cml
}
