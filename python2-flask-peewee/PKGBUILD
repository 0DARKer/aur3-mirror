pkgname=python2-flask-peewee
pkgver=0.5.3
pkgrel=1
pkgdesc="Peewee integration for flask"
arch=(any)
url="http://pypi.python.org/pypi/flask-peewee"
license=("BSD")
groups=()
depends=(
    "python2"
    "python2-flask"
    "python2-wtf-peewee")
source=("http://pypi.python.org/packages/source/f/flask-peewee/flask-peewee-0.5.3.tar.gz")
noextract=()
md5sums=("57c6ec87873cd83e11ba0c62c5762212")

build() { 
    cd $srcdir/flask-peewee-0.5.3
    python2 setup.py install --root="$pkgdir" --prefix=/usr
}
