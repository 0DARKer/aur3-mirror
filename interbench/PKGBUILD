# Contributer: Mark Rosenstand <mark@borkware.net>

pkgname=interbench
pkgver=0.31
pkgrel=1
pkgdesc="Benchmark interactivity in Linux"
url="http://users.on.net/~ckolivas/interbench/"
license="GPL"
arch=('i686' 'x86_64')
depends=('glibc')
md5sums=('37f82cf34ebc5cc7a0b8df2c70dfa8c7')
source=(http://freshmeat.net/urls/559205f664780403645bc1a593b73c27)

build() {
  cd $startdir/src/$pkgname-$pkgver
  make || return 1
  install -D -m755 interbench $startdir/pkg/usr/bin/interbench
}
