# Maintainer: Michael Schubert <mschu.dev at gmail>
# Contributer: Mark Rosenstand <mark@borkware.net>

pkgname=interbench
pkgver=0.31
pkgrel=3
pkgdesc="Interbench is a benchmarking application designed to emulate the CPU scheduling behavior of interactive tasks and measure their scheduling latency and jitter"
url="http://users.on.net/~ckolivas/interbench/"
license=('GPLv2')
arch=('i686' 'x86_64')
depends=('glibc')
md5sums=('37f82cf34ebc5cc7a0b8df2c70dfa8c7')
source=('http://freshmeat.net/urls/559205f664780403645bc1a593b73c27')

build() {
  cd $startdir/src/$pkgname-$pkgver
  make || return 1
  install -D -m755 interbench $pkgdir/usr/bin/interbench
}
