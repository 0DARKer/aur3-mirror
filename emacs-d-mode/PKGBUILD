pkgname=emacs-d-mode  
pkgver=2.0.4
pkgrel=1 
pkgdesc="Emacs major mode for the D programming language"
url="http://launchpad.net/emacs-d-mode"
arch=('any')
license=('GPL2')
depends=()
install=$pkgname.install
source=(http://launchpad.net/emacs-d-mode/trunk/initial-release/+download/d-mode.el)
md5sums=('01702acac0853addc9b4ae07237005e9')

build() {
  mkdir -p $startdir/pkg/usr/share/emacs/site-lisp
  cp $startdir/src/d-mode.el $startdir/pkg/usr/share/emacs/site-lisp
}


