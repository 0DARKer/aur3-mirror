pkgname=python-pp
pkgver=1.5.7
pkgrel=1
pkgdesc="Parallel and distributed programming for Python"
arch=('i686' 'x86_64')
depends=(python)
url="http://www.parallelpython.com"
license=('BSD-like license')
source="http://downloads.sourceforge.net/downloads/pp/pp-$pkgver.tar.bz2"
md5sums=('dc26a848917e4d319f6e76646774f629')
build() {
	cd $srcdir/pp-$pkgver/
	./setup.py install --prefix=$pkgdir/usr
}
