# Contributor: Enrico Morelli <morelli@cerm.unifi.it>
pkgname=dictd-wn
pkgver=2.0
_mypkgname=dict-wn-${pkgver}-pre
pkgrel=3
pkgdesc="WordNet for dict"
arch=(i686 x86_64)
url="http://www.dict.org"
license=('GPL')
depends=(dictd)
source=("ftp://ftp.dict.org/dict/pre/$_mypkgname.tar.gz")
md5sums=('fcfedcc13815cde1e28103b61c05c168')

build() {
  cd $startdir/src/
  install -D -m 0644 $startdir/src/wn.dict.dz $startdir/pkg/usr/share/dictd/wn.dict.dz
  install -D -m 0644 $startdir/src/wn.index $startdir/pkg/usr/share/dictd/wn.index
}
