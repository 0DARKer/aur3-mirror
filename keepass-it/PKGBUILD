# Maintainer: Simone Sclavi 'Ito'  <darkhado@gmail.com>
pkgname=keepass-it
pkgver=2.14
pkgrel=1
pkgdesc="Italian locale for keepass"
license=('GPL2')
depends=('keepass>=2.14')
url="http://keepass.info/translations.html"
md5sums=('d2895d29e918e85a89e5d9b6a24107de')
source=(http://downloads.sourceforge.net/keepass/Italian-${pkgver}.zip)
install=keepass-it.install
arch=('any')

package(){
cd $srcdir
mkdir -p $pkgdir/usr/share/keepass
install -m644 Italian.lngx $pkgdir/usr/share/keepass
}
