pkgname=python25-pyparsing
_realname=pyparsing
pkgver=1.5.1
pkgrel=1
pkgdesc="pyparsing compiled with python25"
arch=('i686' 'x86_64')
url="http://pyparsing.wikispaces.com"
license=('other:"MIT"')
depends=('python25')
source=(http://downloads.sourceforge.net/$_realname/$_realname-$pkgver.tar.gz)
md5sums=('d0777821fc4c5f2474cda1081229d76b')

build() {
  cd "$srcdir/$_realname-$pkgver"
  python2.5 setup.py build
  python2.5 setup.py install --prefix=/usr/local --root=$pkgdir
}
