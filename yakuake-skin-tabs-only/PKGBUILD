pkgname=yakuake-skin-tabs-only
pkgver=1.1
pkgrel=1
pkgdesc="Yakuake skin without titlebar."
arch=('i686' 'x86_64')
url=''http://kde-look.org/content/show.php?content=146209''
license=('GPL')
depends=('yakuake')
source=('http://kde-look.org/CONTENT/content-files/146209-tabsonly.tar.gz')
md5sums=('6298a4dab92348ff046e317013e0a613')

package() {
  mkdir -p ${pkgdir}/usr/share/apps/yakuake/skins/
  cp -r ${srcdir}/onlytabs ${pkgdir}/usr/share/apps/yakuake/skins/
}
