pkgname=yakuake-skin-tabs-only
pkgver=1.1
pkgrel=1
pkgdesc="Yakuake skin without titlebar."
arch=('i686' 'x86_64')
url=''http://kde-look.org/content/show.php?content=146208''
license=('GPL')
depends=('yakuake')
source=('http://kde-look.org/CONTENT/content-files/146208-146206-onlytabs.tar.gz')
md5sums=('46da6d8ca590ecdc26ac89bba32264a8')

package() {
  mkdir -p ${pkgdir}/usr/share/apps/yakuake/skins/
  cp -r ${srcdir}/onlytabs ${pkgdir}/usr/share/apps/yakuake/skins/
}
