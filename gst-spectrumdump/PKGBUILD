pkgname=gst-spectrumdump
pkgver=0.3_05102011
pkgrel=1
pkgdesc="Dumps spectrum or VU magnitudes from audio input to STDOUT or callback."
arch=('any')
url="http://wintervenom.mycrosoft.us/projects"
license=('WTFPL')
depends=('python2' 'gstreamer0.10-python' 'gstreamer0.10-good' 'pygobject')
optdepends=('python-gconf')
source=('gst-spectrumdump')
md5sums=('e4e0a921cfcd04c514ab0d1d6922f34b')

build() {
    return
}

package() {
    cd "${srcdir}"
    mkdir -p "${pkgdir}/bin"
    install ${pkgname} "${pkgdir}/bin"
}
