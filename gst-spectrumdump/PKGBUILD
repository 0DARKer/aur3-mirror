pkgname=gst-spectrumdump
pkgver=0.1_05092011
pkgrel=2
pkgdesc="Dumps spectrum analyzer magnitudes from audio input to STDOUT or callback."
arch=('any')
url="http://wintervenom.mycrosoft.us/projects"
license=('WTFPL')
depends=('python2' 'gstreamer0.10-python' 'gstreamer0.10-good' 'python2-gconf' 'pygobject')
source=('gst-spectrumdump')
md5sums=('1818647d724db45300c8338da0a83318')

build() {
    return
}

package() {
    cd "${srcdir}"
    mkdir -p "${pkgdir}/bin"
    install ${pkgname} "${pkgdir}/bin"
}
