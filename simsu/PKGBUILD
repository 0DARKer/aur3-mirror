# Contributor: Graeme Gott <graeme@gottcode.org>
pkgname=simsu
pkgver=1.2.1
pkgrel=1
pkgdesc="A basic Sudoku game"
arch=('i686' 'x86_64')
url="http://gottcode.org/simsu/"
license=('GPL')
depends=('qt')
source=(http://gottcode.org/simsu/$pkgname-$pkgver-src.tar.bz2)
md5sums=('a20a09930a7157cba7624b2eda960cb2')

build() {
  cd "$srcdir/$pkgname-$pkgver"

  qmake PREFIX=/usr
  make || return 1
  make INSTALL_ROOT=$pkgdir install
}
