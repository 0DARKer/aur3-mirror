# Maintainer: mmm
pkgname=imaplib2-git
_pkg=imaplib2
pkgdesc='IMAP mail protocol access library for Python. Ancestor of libgmail. Devel version'
url='http://sourceforge.net/projects/imaplib2/'
pkgver=1
pkgrel=3
arch=('any')
license=('Python license')
makedepends=('git')
depends=('python')
provides=('imaplib' 'python2-imaplib')
source=('git+git://imaplib2.git.sourceforge.net/gitroot/imaplib2/imaplib2')
md5sums=('SKIP')

package() {
	cd $pkgdir
	install -D -m755 $srcdir/$_pkg/imaplib2.py $pkgdir/usr/bin/imaplib2.py
}
