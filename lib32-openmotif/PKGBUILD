pkgname=lib32-openmotif
pkgname32=openmotif
pkgver=2.3.3
pkgrel=3
pkgrel32=1
pkgdesc="Openmotif"
arch=('x86_64')
url="http://www.opengroup.org/openmotif/"
license=('GPL')
groups=('lib32')
source=(http://mirrors.kernel.org/archlinux/community/os/i686/${pkgname32}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('9bfaa4d9a3fda8fa871d9bc75cc7ee5d')

build()
{
  install -d "${pkgdir}"/usr/lib32
}

package()
{
  install -d "${srcdir}"/usr/lib "${pkgdir}"/usr/lib32
}
