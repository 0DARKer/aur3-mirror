pkgname=lib32-openmotif
pkgname32=openmotif
pkgver=2.3.4
pkgrel=1
pkgrel32=1
pkgdesc="Openmotif"
arch=('x86_64')
url="http://www.opengroup.org/openmotif/"
license=('GPL')
groups=('lib32')
source=("http://ftp5.gwdg.de/pub/linux/archlinux/community/os/i686/${pkgname32}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz")
md5sums=('db838ebb7d3004d12ab987ed1aa79e56')

build()
{
  install -d "${pkgdir}"/usr/lib32
}

package()
{
  cp -Rp "${srcdir}"/usr/lib/* "${pkgdir}"/usr/lib32
}
