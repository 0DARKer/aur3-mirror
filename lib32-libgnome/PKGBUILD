pkgname=lib32-libgnome
pkgver=2.32.0
pkgrel32=1
pkgrel=1
pkgdesc="Common libraries for GNOME"
arch=('x86_64')
url="http://www.gnome.org"
depends=('lib32-esound' 'lib32-gnome-vfs' 'lib32-libbonobo')
license=('LGPL')
groups=lib32
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('7ad03650bc99197993b0744e30c5bfaa')

build() {
  mkdir -p ${startdir}/pkg/opt/lib32/usr/lib
  cp -rPf ${startdir}/src/usr/lib/*.so* ${startdir}/pkg/opt/lib32/usr/lib
}
