pkgname=trytond-production
_pkgname=trytond_production
pkgver=2.4.0
_pkgdir=2.4
pkgrel=2
pkgdesc="The production module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.4' 'trytond-product>=2.4' 'trytond-company>=2.4' 'trytond-stock>=2.4')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("965525ef8f205ae115ca7d550e22d559")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}