# Maintainer: Alucryd <alucryd@gmail.com>
pkgname=gnome-shell-extension-noa11y
pkgver=1.0
pkgrel=1
pkgdesc="A gnome-shell extension to remove the Accessibility icon from the panel"
source=('http://www.fpmurphy.com/gnome-shell-extensions/noa11y.tar.gz')
md5sums=('bbfc8ef8c59e72e5cb4dd3249d560a7d')
arch=('any')
url="http://www.fpmurphy.com/gnome-shell-extensions/"
license=('GPL3')
depends=('gnome-shell')

package() {
    cd "$srcdir"
    mkdir -p "$pkgdir/usr/share/gnome-shell/extensions/"
    cp -R "noa11y@fpmurphy.com" "$pkgdir/usr/share/gnome-shell/extensions"
}

