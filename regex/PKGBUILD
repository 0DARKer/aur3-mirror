pkgname=regex
pkgver=0.1c.rev24
pkgrel=1
pkgdesc="regular expression generator"
arch=('i686' 'x86_64')
url="https://bitbucket.org/hav3lock/pub_rel/src"
license=('GPL3')
depends=('gcc-libs')
source=("$pkgname-${pkgver}.tar.gz")
md5sums=('38b0924fd83e48de215ec7ff28f6a79e')


build() {

    cd $srcdir/$pkgname-$pkgver
    
    make

}

package() {

    cd $srcdir/$pkgname-$pkgver

    make DESTDIR=$pkgdir install


}
