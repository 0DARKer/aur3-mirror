pkgname=mint-themes-gtk3-git
pkgver=1.0
pkgrel=1
pkgdesc="The Mint-X theme for Linux Mint (git version)"
arch=('any')
conflicts=('mint-x-theme')
url="https://github.com/linuxmint/mint-themes-gtk3/"
license=('unknown')
depends=('mint-x-icons' 'git' 'mint-backgrounds-qiana' 'mint-cinnamon-themes')
source=('git+https://github.com/linuxmint/mint-themes-gtk3.git')
md5sums=('SKIP')
package() {
   cd $srcdir/mint-themes-gtk3
   cp -r usr/ $pkgdir/
   chmod 644 $pkgdir/usr/share/themes/Mint*
}
