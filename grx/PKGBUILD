pkgname=grx
pkgver=2.4.8
pkgrel=1
pkgdesc="2D graphics library."
arch=('i686')
url="http://grx.gnu.de/"
#depends=('' '')
#makedepends=('')
license=('GPL2')
source=(http://grx.gnu.de/download/${pkgname}248.tar.gz)
md5sums=('c4fbeeef428a9dce6c3dc84ab8f574cb')

build() {
	cd ${startdir}/src/grx248
	./configure -prefix=/usr
	make || return 1
	make DESTDIR=$startdir/pkg install 
}
