# Maintainer: Alfred Krohmer <devkid@devkid.net>
pkgname=python-mutagenx
pkgver=1.23
pkgrel=1
pkgdesc="read and write audio tags for many formats in Python 2/3"
url="https://github.com/LordSputnik/mutagen/"
depends=('python' )
makedepends=('python3' )
license=('GPL2')
arch=('any')
source=('https://pypi.python.org/packages/source/m/mutagenx/mutagenx-1.23.tar.gz')
md5sums=('3157b08e0585f8af3619ffbc18838b7c')

build() {
    cd $srcdir/mutagenx-1.23
    python setup.py build
}

package() {
    cd $srcdir/mutagenx-1.23
    python setup.py install --root="$pkgdir" --optimize=1 
}
