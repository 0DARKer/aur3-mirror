# Maintainer: Alfred Krohmer <devkid@devkid.net>
pkgname=python-mutagenx
pkgver=1.23.2
pkgrel=2
pkgdesc="read and write audio tags for many formats in Python 3"
url="https://github.com/LordSputnik/mutagen/"
depends=('python3')
conflicts=('mutagen')
license=('GPL2')
arch=('any')
source=('https://pypi.python.org/packages/source/m/mutagenx/mutagenx-1.23.2.tar.gz')
md5sums=('ee3d9db0b81ef6d6a6020c3cb7ff031c')

build() {
    cd $srcdir/mutagenx-1.23.2
    python setup.py build
}

package() {
    cd $srcdir/mutagenx-1.23.2
    python setup.py install --root="$pkgdir" --optimize=1 
}
