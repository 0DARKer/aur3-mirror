# $Id: pkgbuild-mode.el,v 1.23 2007/10/20 16:02:14 juergen Exp $
# Maintainer: Alexandre Bique <bique.alexandre@gmail.com>
pkgname=re2  
pkgver=20130115
pkgrel=1 
pkgdesc="efficient, principled regular expression library"
url="http://code.google.com/p/re2/"
arch=('i686' 'x86_64')
license=('BSD')
depends=()
makedepends=()
conflicts=()
source=(http://re2.googlecode.com/files/$pkgname-$pkgver.tgz)
md5sums=('ef66646926e6cb8f11f277b286eac579')

build() {
  cd $startdir/src/$pkgname
  make || return 1
  make prefix=/usr DESTDIR=$startdir/pkg install
}
