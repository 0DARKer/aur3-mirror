# Contributor: Angel 'angvp' Velasquez <angvp[at]archlinux.com.ve> 
pkgname=volleyball-data
pkgver=0.8.5
pkgrel=1
pkgdesc="data for the volleyball game"
url="http://www.losersjuegos.com.ar/juegos/volleyball"
license=('GPL')
depends=('sdl_image' 'sdl_mixer')
arch=('i686' 'x86_64')
source=(http://archmirror.de/aur/volleyball/$pkgname-$pkgver.tar.gz )
md5sums=('f17bd48538ae9bdf6ec1e3fdae707375')
build() {
  cd $srcdir/$pkgname-$pkgver
  ./configure --prefix=/usr || return 1
  make || return 1
  make DESTDIR=$pkgdir install || return 1
}
