# Contributor: Eric Le Bras <eric.lebras@gmail.com>

pkgname=aaphoto
pkgver=0.41
pkgrel=1
pkgdesc="Automatic photo adjusting"
url="http://log69.com/aaphoto_en.html"
license=('GPL3')
depends=('jasper' 'libpng')
arch=('i686' 'x86_64')
source=(http://log69.com/downloads/aaphoto_sources_v${pkgver}.tar.gz)
md5sums=('e15db522d8fc1915c939aaef8b31b187')

build() {
   cd ${srcdir}/aaphoto-${pkgver}
   ./configure --prefix=/usr
   make || return 1
   make DESTDIR=${pkgdir} install
}
