# Maintainer: Pierrick BRUN <pierrick.brun gmail.com>
pkgname=libreoffice-faenza-mod
pkgver=0.3
pkgrel=1
pkgdesc="An icons pack for libreoffice inspired by Faenza icons"
arch=(any)
url="http://gnome-look.org/content/show.php/Libreoffice+Icons+Faenza+Mod+?content=143474"
license=('GPL')
depends=('libreoffice-common')
install=${pkgname}.install
source=('http://dl.dropbox.com/u/1229628/libreoffice_faenzaMODV03.zip')
noextract=('libreoffice_faenzaMOD03.zip')
md5sums=('6141dd616a0083c95ff12a33f6603963') 

build() {
LO_DIR=/usr/lib/libreoffice/basis-link/share/config/

mv libreoffice_faenzaMODV03.zip images_faenza.zip
mkdir -p $pkgdir$LO_DIR
cp images_faenza.zip $pkgdir$LO_DIR

}

