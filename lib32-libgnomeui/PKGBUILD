# AUR packages should not be built from binaries. This is a workaround.
# If you manage to built this lib32 package from source, let the maintainer know
# and you will be helping the AUR community.

_pkgbasename=libgnomeui
pkgname=lib32-$_pkgbasename
pkgver=2.24.4
pkgrel32=1
pkgrel=1
pkgdesc="User Interface library for GNOME (32 bits)"
arch=('x86_64')
url="http://www.gnome.org/"
license=('LGPL')
depends=('lib32-gnome-keyring' 'lib32-libbonoboui' "${_pkgbasename}=${pkgver}-${pkgrel32}")
groups=lib32
source=(ftp://ftp.archlinux.org/extra/os/i686/${_pkgbasename}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('80f0714ef9464e169d2e6707d962b76f')

build() {
  return 0
}

package() {
  mkdir -p ${pkgdir}/usr/lib32
  cp -rPf ${srcdir}/usr/lib/*.so* ${pkgdir}/usr/lib32
}
