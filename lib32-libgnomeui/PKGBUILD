pkgname=lib32-libgnomeui
pkgver=2.24.4
pkgrel32=1
pkgrel=1
pkgdesc="User Interface library for GNOME"
arch=('x86_64')
url="http://www.gnome.org/"
license=('LGPL')
depends=('lib32-gnome-keyring' 'lib32-libbonoboui')
groups=lib32
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('80f0714ef9464e169d2e6707d962b76f')

build() {
  mkdir -p ${startdir}/pkg/opt/lib32/usr/lib
  cp -rPf ${startdir}/src/usr/lib/*.so* ${startdir}/pkg/opt/lib32/usr/lib
  #cp -rPf ${startdir}/src/usr/lib/gtk-2.0 ${startdir}/pkg/opt/lib32/usr/lib
}
