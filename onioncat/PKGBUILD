pkgname=onioncat
_ver=0.2.2
pkgver=0.2.2.r554
pkgrel=1
pkgdesc="An IP-Transparent Tor Hidden Service Connector"
arch=('i686' 'x86_64')
url="http://www.cypherpunk.at/onioncat/"
license=('GPL3')
depends=('tor')
#install=$pkgname.install
source=(http://www.cypherpunk.at/ocat/download/Source/$_ver/$pkgname-$pkgver.tar.gz)

build() {
	cd "$srcdir/$pkgname-${pkgver}"
	./configure --prefix=/usr
	make || return 1
}
package() {
	cd "$srcdir/$pkgname-${pkgver}"
	make DESTDIR="$pkgdir/" install
}
md5sums=('3c47eefe0ad3980035c09e85edc9ebf0')
sha256sums=('f44686696a9dda53ebad9f4fe06bdd0d313a2536f04703e2a277c3c174e5dbb7')
