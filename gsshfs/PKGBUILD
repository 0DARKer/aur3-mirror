# $Id: PKGBUILD,v 1.12 2003/11/06 08:26:13 dorphell Exp $
# Maintainer: marty <>
# Contributor: marty <>
pkgname=gsshfs
pkgver=1.0.5
pkgrel=1
pkgdesc="GUI for SSHFS"
arch=(i686 x86_64)
url="http://sallu.tuxfamily.org/Home"
license=('GPL')
#groups=
#provides=
depends=('gtk2' 'fuse' 'sshfs')
makedepends=('cmake' 'make')
#conflicts=()
#replaces=()
#backup=()
#install=()
source=(http://download.tuxfamily.org/sallu/utilities/${pkgname}-${pkgver}.tar.bz2)
md5sums=('290d638dcbce07d60a4aeb080694578e')
build() {
  cd "${srcdir}/${pkgname}-${pkgver}"
  cmake -DCMAKE_INSTALL_PREFIX=/usr .
  make || return 1
  make DESTDIR=$pkgdir install
}