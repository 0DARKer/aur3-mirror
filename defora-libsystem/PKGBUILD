# Contributor: Calimero <calimeroteknik@free.fr>
pkgname=defora-libsystem
pkgver=0.1.4
pkgrel=1
pkgdesc="The common lib to all Defora apps"
arch=('i686' 'x86_64')
url="http://defora.org/os"
license=('GPL3')
depends=('openssl')
source=(http://www.defora.org/os/download/download/3384/libSystem-${pkgver}.tar.gz)
md5sums=('1ea3cbed698cdecf42ecfe995c44650c')

build() {
  cd "$srcdir"/libSystem-$pkgver
  make || return 1
  make DESTDIR="$pkgdir" PREFIX="/usr" install
}
