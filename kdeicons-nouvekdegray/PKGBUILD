pkgname=kdeicons-nouvekdegray
_pkgname=nouveKDEGray
pkgver=0.2
pkgrel=1
pkgdesc="This is a version of the KDE4 port of the "Reflektions" icons."
arch=('any')
url="http://kde-look.org/content/show.php/nouveKDEGray?content=157454"
license=('CCPL')

depends=('kdebase-workspace')
source=("http://fc02.deviantart.net/fs70/f/2013/073/7/6/nouvekdegray_by_tsujan-d5y0sw4.7z")
md5sums=('6bd6dd7899cf71765c7189ed3ca36aa2')

package() {
  install -d ${pkgdir}/usr/share/icons
  cp -ar ${srcdir}/${_pkgname} ${pkgdir}/usr/share/icons/
}
