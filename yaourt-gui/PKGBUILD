pkgname=yaourt-gui
pkgver=0.2
pkgrel=1
pkgdesc="A bash GUI for yaourt"
arch=('any')
url="http://vkteam.altervista.org/yaourtgui/yaourtgui.html"
license=('GPL')
depends=('yaourt')
source=("http://vkteam.altervista.org/yaourtgui/$pkgname-$pkgver.tar.gz")


package() {
	cd $srcdir/$pkgname-$pkgver
	make DESTDIR=${pkgdir} install
}


md5sums=('33899b9c6776e334023ad0d50b9948b7')
