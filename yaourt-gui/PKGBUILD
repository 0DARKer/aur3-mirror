pkgname=yaourt-gui
pkgver=0.3
pkgrel=1
pkgdesc="A bash GUI for yaourt"
arch=('any')
url="http://vkteam.altervista.org/yaourtgui/yaourtgui.html"
license=('GPL')
depends=('yaourt')
source=("http://vkteam.altervista.org/yaourtgui/$pkgname-$pkgver.tar.gz")

package() {
	cd $srcdir/$pkgname-$pkgver
	make DESTDIR=${pkgdir} install
}


md5sums=('5c52486e787f359c604047de13af9926')
