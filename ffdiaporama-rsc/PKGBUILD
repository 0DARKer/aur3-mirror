# Maintainer: boenki <boenki at gmx dot de>

pkgname=ffdiaporama-rsc
pkgver=2.1.devel.20140125
pkgrel=2
pkgdesc="Recource-files for ffDiaporama"
arch=('any')
url="http://ffdiaporama.tuxfamily.org"
license=('GPL2')
conflicts=('ffdiaporama') #until 2.1 is stable
source=(http://download.tuxfamily.org/ffdiaporama/Packages/ffdiaporama_rsc_2.1.devel.2014.0125.tar.gz)
md5sums=('SKIP')

build() {
  cd ffDiaporama_rsc
  qmake ffDiaporama_rsc.pro
  make
}

package() {
  cd ffDiaporama_rsc
  make install INSTALL_ROOT=$pkgdir
}