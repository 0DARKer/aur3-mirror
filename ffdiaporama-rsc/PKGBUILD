# Maintainer: boenki <boenki at gmx dot de>

pkgname=ffdiaporama-rsc
pkgver=2.1.devel.20140119
pkgrel=1
pkgdesc="Recource-files for ffDiaporama"
arch=('any')
url="http://ffdiaporama.tuxfamily.org"
license=('GPL2')
conflicts=('ffdiaporama') #until 2.1 is stable
source=(https://launchpad.net/$pkgname/2.1-devel/2.1-devel.20140119/+download/ffdiaporama_rsc_2.1.devel.2014.0119.tar.gz)
md5sums=('161a4ad274d90f38723de0d53c4a655d')

build() {
  cd ffDiaporama_rsc
  qmake ffDiaporama_rsc.pro
  make
}

package() {
  cd ffDiaporama_rsc
  make install INSTALL_ROOT=${pkgdir}
}