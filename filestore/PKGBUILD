pkgname=filestore
pkgver=11.08.0
pkgrel=1
pkgdesc="A dmedia hashing protocol and file layout."
arch=('any')
url="http://www.kickstarter.com/projects/novacut/novacut-pro-video-editor"
license=('AGPL3')
depends=('python' 'pyskein')
source=("https://launchpad.net/$pkgname/trunk/11.08/+download/$pkgname-$pkgver.tar.gz")
md5sums=('59c27c1e61138cf214ddc62606962656')

package() {
  cd "$srcdir/$pkgname-$pkgver"
  python setup.py install --prefix=/usr --root="$pkgdir" -O1
}
