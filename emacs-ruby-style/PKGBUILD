
# Maintainer: Nathan Owe <ndowens.aur at gmail dot com>
pkgname=emacs-ruby-style
pkgver=1.9.2_rc2
pkgrel=1
pkgdesc="Emacs ruby style plugin"
arch=('any')
url="http://ruby-lang.org"
license=('GPL3')
depends=('emacs')
source=("http://svn.ruby-lang.org/repos/ruby/tags/v1_9_2_rc2/misc/ruby-style.el")
md5sums=('3f1b7730c9e14bce2138363c2e9a110b')

package() {
  cd "$srcdir/"
  install -d $pkgdir/usr/share/emacs/site-lisp/ruby-style
  install -Dm644 ruby-style.el \
$pkgdir/usr/share/emacs/site-lisp/ruby-style/ruby-style.el
}
