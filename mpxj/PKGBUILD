# Maintainer: Jameson Pugh <jameson.pugh@gmail.com>

pkgname=mpxj
pkgver=4.3.0
pkgrel=1
pkgdesc="A Java based project file reader/writter."
arch=('any')
url="http://mpxj.sourceforge.net"
license=('LGPL')
depends=('apache-poi' 'java-environment')
makedepends=('java-environment')
source=("http://dl.sourceforge.net/mpxj/$pkgname-$pkgver.zip")
sha256sums=('e32f747ec36fa6e4714a0a1be935b2633d0982340dfed81e72b1e03369547887')

build() {
  cd "$srcdir/$pkgname"

  install -d $pkgdir/usr/share/java
  install -m644 mpxj.jar $pkgdir/usr/share/java
}

# vim:set ts=2 sw=2 et:
