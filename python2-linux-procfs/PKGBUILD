pkgname=python2-linux-procfs
pkgver=0.4.6
pkgrel=3
pkgdesc="Python package that allows making use of procfs"
arch=('any')
url="https://git.fedorahosted.org/git/python-linux-procfs.git"
license=('GPL')
depends=('python2' 'python2-numpy' 'git')
options=(!emptydirs)
#provides=('python2-linux-procfs')
#conflicts=('python2-linux-procfs')
#replaces=('python2-linux-procfs')
source=("git://git.fedorahosted.org/git/python-linux-procfs.git")
sha256sums=('SKIP')

package() {
  cd "$srcdir/python-linux-procfs"
  python2 setup.py install --root="$pkgdir/"

}

