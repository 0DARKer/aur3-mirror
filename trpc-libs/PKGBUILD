pkgname=trpc-libs
pkgver=20101001
pkgrel=1
pkgdesc="The TreeP programming language compiler"
url="http://fsinapsi.altervista.org"
license=("GPL")
depends=("gmp" "zlib" "gc")
makedepends=("libid3tag" "libjpeg" "curl" "imlib2" "sqlite3" "gtk2" "file" "libmysqlclient" "mathgl")
source=("http://fsinapsi.altervista.org/code/treep/trp-${pkgver}.zip")
arch=('i686' 'x86_64')
md5sums=('aae9fde2a1885cec816d62de97eed72f')


build() {
        cd $srcdir/trp-$pkgver
	make rts|| return 1
        install -d -m 755 ${pkgdir}/usr/lib
        cp libs/* $pkgdir/usr/lib/
}
