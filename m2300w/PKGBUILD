pkgname=m2300w
pkgver=0.51
pkgrel=2
pkgdesc="CUPS driver for the Minolta/QMS magicolor2300w"
url="http://m2300w.sourceforge.net"
license="GPL"
depends=('cups' 'foomatic-filters' 'ghostscript')
install=
source=(http://mesh.dl.sourceforge.net/sourceforge/m2300w/$pkgname-$pkgver.tar.gz)
md5sums=(ba340a6ea545052aa2d1b634e9de1919)

build() {
	  cd $startdir/src/$pkgname-$pkgver
	    ./configure
	      make || return 1
	        make INSTROOT=$startdir/pkg install
	}
