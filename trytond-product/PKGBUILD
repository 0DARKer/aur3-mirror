pkgname=trytond-product
_pkgname=trytond_product
pkgver=1.4.0
pkgrel=2
pkgdesc="Product module of the Tryton application platform"
arch=('any')
url="http://www.tryton.org/"
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=1.4')
makedepends=('setuptools')
