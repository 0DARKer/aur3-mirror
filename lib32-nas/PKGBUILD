# Maintainer: Gadget3000 <gadget3000 at msn dot com>

pkgname=lib32-nas
pkgver=1.9.2
pkgrel=2
pkgdesc="Free peer-reviewed portable C++ source libraries - Runtime (32 bit)"
arch=('x86_64')
url="http://radscan.com/nas.html"
license=('custom')
groups=('lib32')
depends=('lib32-libxaw')
source=(ftp://mirrors.kernel.org/archlinux/community/os/i686/nas-${pkgver}-${pkgrel}-i686.pkg.tar.xz)
md5sums=('b456b86b8313857e4c6d809f88effbd7')

package() 
{
	mkdir -p ${pkgdir}/usr/lib32
	cp -Pr ${srcdir}/usr/lib/* ${pkgdir}/usr/lib32

	mkdir -p ${pkgdir}/usr/share/licenses/${pkgname}
	install ${srcdir}/usr/share/licenses/nas/license ${pkgdir}/usr/share/licenses/${pkgname}
}
