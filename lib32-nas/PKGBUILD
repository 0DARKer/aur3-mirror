# Maintainer: Gadget3000 <gadget3000 at msn dot com>

pkgname=lib32-nas
pkgver=1.9.2
pkgrel=2
pkgdesc="Network Audio System is a network transparent, client/server audio
transport system (32 bit)"
arch=('x86_64')
url="http://radscan.com/nas.html"
license=('custom')
groups=('lib32')
depends=('lib32-libxt')
source=(http://downloads.sourceforge.net/project/cherimoya/repo/defense/i686/nas-1.9.2-2-i686.pkg.tar.xz)
md5sums=('5e62918f9535b47c6a4a442f0559169f')

package() 
{
   mkdir -p ${pkgdir}/usr/lib32
   cp -Pr ${srcdir}/usr/lib/* ${pkgdir}/usr/lib32

   mkdir -p ${pkgdir}/usr/share/licenses/${pkgname}
   install ${srcdir}/usr/share/licenses/nas/license
   ${pkgdir}/usr/share/licenses/${pkgname}
}
