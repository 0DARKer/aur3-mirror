# Maintainer:  trashstar <trash@ps3zone.org>

pkgname=etm_qt-qt4
pkgver=2.3.20
pkgrel=1
pkgdesc="Manage events and tasks using simple text files."
arch=('any')
url="http://people.duke.edu/~dgraham/etmqt/"
license=('GPL')
depends=('python2-pyqt4' 'python2-dateutil' 'python2-yaml')
optdepends=('python2-icalendar: iCalendar support')
conflicts=('etm_qt')
makedepends=('setuptools')
source=("http://people.duke.edu/~dgraham/etmqt/etm_qt-$pkgver.tar.gz")
md5sums=('a35d565b279f0f6f6f4a047e12630f8e')

package() {
    cd "$srcdir/etm_qt-$pkgver"
    python2 setup.py install --prefix=/usr --root=$pkgdir 
}
