pkgname=trytond-google-maps
_pkgname=trytond_google_maps
pkgver=1.4.0
pkgrel=2
pkgdesc="The google_maps module of the Tryton application platform"
arch=('any')
url="http://www.tryton.org/"
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=1.4' 'trytond-party>=1.4')
makedepends=('setuptools')
source=("http://downloads.tryton.org/1.2/$_pkgname-$pkgver.tar.gz")
md5sums=('2b5038755c53ddb8b57129317f6e17b9')

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}

