pkgname=cinnamon-theme-zukitwo
pkgver=1.3
pkgrel=1
pkgdesc="Zukitwo theme for cinnamon"
arch=('any')
url="http://cinnamon-spices.linuxmint.com/themes/view/88"
license=('unknown')
depends=('cinnamon')
optdepends=('zukitwo-themes: zukitwo gtk themes')
source=("http://cinnamon-spices.linuxmint.com/uploads/themes/UM17-HOJW-1M89.zip")
md5sums=('52b51a9d79ce74e66c46920855c70af3')

package() {
  mkdir -p ${pkgdir}/usr/share/themes/
  cp -r ${srcdir}/Zukitwo* ${pkgdir}/usr/share/themes/
}

