# Contributor: Scott Garrett <Wintervenom@archlinux.us>

pkgname=libticalcs
pkgver=1.1.6
pkgrel=1
pkgdesc="TI Calculator library"
arch=(i686 x86_64)
url="http://tilp.info"
license="GPL"
depends=('libticables' 'libtifiles')
source=("http://downloads.sourceforge.net/project/tilp/tilp2-linux/tilp2-1.15/${pkgname}2-${pkgver}.tar.bz2")

build() {
  cd $srcdir/${pkgname}2-$pkgver
  ./configure --prefix=/usr
  make
  make DESTDIR=$pkgdir install
}
md5sums=('0be42d34f63a671925bfdef25032428e')
