# Contributor: piojo

pkgname=astrill
pkgver=2.9.1
pkgrel=1
pkgdesc="VPN configuration tool for Astrill's servers"
arch=('x86_64' 'i686')
url="http://astrill.com/"
license=('custom')
depends=()
if [ "${CARCH}" = 'x86_64' ]; then
    rpmpkg='astrill-setup-linux64.rpm'
    md5sums=('86667fe12c727fb77b3c779a5a535dbd')
else
    rpmpkg='astrill-setup-linux.rpm'
    md5sums=('8b1e98174dc66749f111f6a396d6c0a2')
fi
source=("https://www.astrill.com/downloads/$rpmpkg")

build() {
    mv "${srcdir}/usr" "${pkgdir}"
}
