post_install() {
  ln -s '/usr/lib/libssl.so.1.0.0'  '/usr/lib/libssl.so.10' 
  ln -s '/usr/lib/libcrypto.so.1.0.0' '/usr/lib/libcrypto.so.10' 
  ln -s /usr/lib/libjpeg.so  /usr/lib/libjpeg.so.62
  ln -s /usr/local/lib/* /usr/lib
  ln -s /usr/loal/share /usr/share/*
  ln -s /usr/local/bin/* /usr/bin
  ln -s /usr/local/share/applnk/Internet/ayttm.desktop /usr/share/applications/ayttm.desktop
}
