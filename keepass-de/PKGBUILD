pkgname=keepass-de
pkgver=2.16
pkgrel=1
pkgdesc="German locale for keepass"
license=('GPL2')
depends=("keepass>=${pkgver}")
makedepends=('unzip')
url="http://keepass.info/translations.html"
source=(http://downloads.sourceforge.net/keepass/German-${pkgver}.zip)
arch=('any')
md5sums=('105f59d89381dfb721d1df26c908d7c9')

package(){
   cd $srcdir
   #unzip German-${pkgver}.zip
   mkdir -p $pkgdir/usr/share/keepass
   install -m644 German.lngx $pkgdir/usr/share/keepass
}
