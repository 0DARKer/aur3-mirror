pkgname=deimos
pkgver=0.8.26
pkgrel=1
pkgdesc="Client for The Deimos Project"
arch=('x86_64')
url="http://deimos-project.net/"
license='Freeware'
source=(http://deimos-project.net/downloads/releases/deimos-${pkgver}-linux-binary-x64.tar.gz)
md5sums='976d7a224a85532d4f7343ac89f9ca02'

build() {
  cd "$srcdir"
  rm AUTHORS README
  mv etc/init.d etc/rc.d
}

package() {
  cd "$srcdir"
  cp -rfp etc "$pkgdir"
  cp -rfp usr "$pkgdir"
}