pkgname=serviio-twitch-plugin
pkgver=4
pkgrel=1
pkgdesc="Twitch.tv/Justin.tv plugin for Serviio"
arch=('any')
url="http://forum.serviio.org/viewtopic.php?f=20&t=9216"
license=('unknown')
depends=('serviio')
source=(twitch.groovy::http://forum.serviio.org/download/file.php?id=3760)
sha256sums=('ba35474bb47ed6a4087254a72144f65ce0367480473471dd9838e757b16e4baf')

package() {
  cd "$srcdir"

  install -m 644 -D twitch.groovy "$pkgdir/usr/share/java/serviio/plugins/twitch.groovy"
}

