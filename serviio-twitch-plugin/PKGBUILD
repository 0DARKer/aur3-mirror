pkgname=serviio-twitch-plugin
pkgver=9
pkgrel=1
pkgdesc="Twitch.tv/Justin.tv plugin for Serviio"
arch=('any')
url="http://forum.serviio.org/viewtopic.php?f=20&t=9216"
license=('unknown')
depends=('serviio')
source=(twitch.groovy::http://forum.serviio.org/download/file.php?id=4449)
sha256sums=('a9c513d2700d892f4d2244264dccb57509ec08c64f639ba4c58baeced3d3768a')

package() {
  cd "$srcdir"

  install -m 644 -D twitch.groovy "$pkgdir/usr/share/java/serviio/plugins/twitch.groovy"
}

