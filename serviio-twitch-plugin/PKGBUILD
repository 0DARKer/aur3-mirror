pkgname=serviio-twitch-plugin
pkgver=7
pkgrel=1
pkgdesc="Twitch.tv/Justin.tv plugin for Serviio"
arch=('any')
url="http://forum.serviio.org/viewtopic.php?f=20&t=9216"
license=('unknown')
depends=('serviio')
source=(twitch.groovy::http://forum.serviio.org/download/file.php?id=4433)
sha256sums=('c0bcdc6db27ee2701f00b9f02969a7e8e28a2164167811447fa6e0ca86d8d53e')

package() {
  cd "$srcdir"

  install -m 644 -D twitch.groovy "$pkgdir/usr/share/java/serviio/plugins/twitch.groovy"
}

