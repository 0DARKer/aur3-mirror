pkgname=serviio-twitch-plugin
pkgver=8
pkgrel=1
pkgdesc="Twitch.tv/Justin.tv plugin for Serviio"
arch=('any')
url="http://forum.serviio.org/viewtopic.php?f=20&t=9216"
license=('unknown')
depends=('serviio')
source=(twitch.groovy::http://forum.serviio.org/download/file.php?id=4438)
sha256sums=('1f9a6c5b06a06e9cdf2bdca2c11f40164e782c2497e7fd944372f4bca9184880')

package() {
  cd "$srcdir"

  install -m 644 -D twitch.groovy "$pkgdir/usr/share/java/serviio/plugins/twitch.groovy"
}

