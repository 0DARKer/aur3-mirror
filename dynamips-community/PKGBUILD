pkgname=dynamips-community
_gitname=dynamips-community
pkgver=0.2.8RC7
pkgrel=1
pkgdesc="Dynamips version develloped by gns3 team"
arch=('i686' 'x86_64')
url="http://www.gns3.net/dynamips"
license=('GPLv2')
makedepends=('git')
depends=('elfutils')
conflicts=('dynamips','dynamips-community-git')
source=('git://github.com/GNS3/dynamips-community.git#tag=v0.2.8-RC7-community')
sha1sums=('SKIP')

build() {
  if test "$CARCH" == x86_64; then
    export DYNAMIPS_ARCH=amd64
  fi
  cd "$_gitname"
  make
}

package() {
  cd "$_gitname"
  make DESTDIR="$pkgdir/usr" install
}
