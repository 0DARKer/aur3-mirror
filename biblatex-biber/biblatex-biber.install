# arg 1:  the new package version
post_install() {
    echo ">>> Remember that biber 1.3 *needs* biblatex 2.3!"
    echo ">>> Get it e.g. from "
    echo ">>> http://sourceforge.net/projects/biblatex/files/biblatex-2.3/biblatex-2.3.tds.tgz/download"
}

# arg 1:  the new package version
# arg 2:  the old package version
post_upgrade() {
    post_install $1
}



op=$1
shift

$op $*
