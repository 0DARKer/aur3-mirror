_all_aur="n"
# If you want to use the possibly same packages from official repo, keep _all_aur="n".
# To use packages from AUR only, edit it as _all_aur="y".

pkgname=xfce4-devel-meta
pkgver=4.8
pkgrel=1
pkgdesc="Latest applications of Xfce (Meta package)"
arch=('any')
license=('GPL')
url="http://www.xfce.org/"

depends=(
    "exo-devel>=0.7.1"
    "garcon>=0.1.10"
    "gtk-xfce-engine>=2.99.1"
    "libxfce4ui-devel>=4.9.0"
    "libxfce4util>=4.8.2"
    "thunar-devel>=1.3.0"
    "xfce-utils>=4.8.3"
    "xfce4-panel-devel>=4.9.0"
    "xfce4-session>=4.8.3"
    "xfce4-settings-devel>=4.9.2"
    "xfconf>=4.8.1"
    "xfdesktop-devel>=4.9.0"
    "xfwm4-devel>=4.8.3"
)

if [ $_all_aur = "y" ]; then
    true && depends=(
        "exo-devel>=0.7.1"
        "garcon-devel>=0.1.10"
        "gtk-xfce-engine-devel>=2.99.1"
        "libxfce4ui-devel>=4.9.0"
        "libxfce4util-devel>=4.8.2"
        "thunar-devel>=1.3.0"
        "xfce-utils-devel>=4.8.3"
        "xfce4-panel-devel>=4.9.0"
        "xfce4-session-devel>=4.8.3"
        "xfce4-settings-devel>=4.9.2"
        "xfconf-devel>=4.8.1"
        "xfdesktop-devel>=4.9.0"
        "xfwm4-devel>=4.8.3"
    )
fi

optdepends=(
    "terminal"
    "tumbler"
    "xfce4-appfinder"
    "xfce4-mixer"
    "xfwm4-themes"
)

build() {
    return 0
}

