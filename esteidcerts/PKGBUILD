	"https://installer.id.ee/media/sources/$pkgname-$pkgver.tar.gz.asc")
sha256sums=('3f27ffe7080270b158b365413f26148eeedd3e0cfea3300d5e65c97f9aa812e9'
            'eb2b353a42c9cd3c4ff083828c67d439a2bdf10d0a5468b9239cf0841faaf4bc')

package() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R usr $pkgdir/
