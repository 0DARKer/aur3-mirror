Subject: [PATCH 2/4] Setting default Qt4 backend to Cairo

---
 qt4/src/poppler-private.cc |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/qt4/src/poppler-private.cc b/qt4/src/poppler-private.cc
index d6e3265..280bab0 100644
--- a/qt4/src/poppler-private.cc
+++ b/qt4/src/poppler-private.cc
@@ -242,7 +242,7 @@ namespace Debug {
     void DocumentData::init(GooString *ownerPassword, GooString *userPassword)
     {
         m_fontInfoIterator = 0;
-        m_backend = Document::SplashBackend;
+        m_backend = Document::CairoBackend;
         m_outputDev = 0;
         paperColor = Qt::white;
         m_hints = 0;
