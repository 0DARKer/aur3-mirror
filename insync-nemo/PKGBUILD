# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-nemo
pkgver=1.0.27
pkgrel=1
pkgdesc="Nemo integration for insync"
url="https://support.insynchq.com/forums/discussion/2599/new-insync-version-1-0-27"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "nemo" "nemo-python-git")
makedepends=('gtk-doc')
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}.31715_all.deb")
sha256sums=("e970b50464a8e53281a250e5b464e671931bfedc6d517bb28679c171ca1cf3b0")
noextract=("${pkgname}-${pkgver}.deb")

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
