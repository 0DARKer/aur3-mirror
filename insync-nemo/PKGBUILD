# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-nemo
pkgver=1.0.30
pkgrel=1
pkgdesc="Nemo integration for insync"
url="https://support.insynchq.com/forums/discussion/3242/new-insync-version-1-0-30"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "nemo" "nemo-python-git")
makedepends=('gtk-doc')
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}.31757_all.deb")
sha256sums=("621ed4c1ac7ccdcd01c6f75e063f023ebba9eb1986dc1373e64d69df04ffd7f9")
noextract=("${pkgname}-${pkgver}.deb")

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
