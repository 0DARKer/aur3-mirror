# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-nemo
pkgver=1.1.0
pkgrel=1
pkgdesc="Nemo integration for insync"
url="https://support.insynchq.com/forums/discussion/3375/new-insync-version-1-1-0"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "nemo" "nemo-python-git")
makedepends=('gtk-doc')
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}.32001-precise_all.deb")
sha256sums=("a7f33671099882e0997e172b263f9af096a1148b64d52d5f0745c920f2f4d12d")
noextract=("${pkgname}-${pkgver}.deb")

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
