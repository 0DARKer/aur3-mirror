# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-nemo
pkgver=1.0.25
pkgrel=1
pkgdesc="Nemo integration for insync"
url="https://support.insynchq.com/forums/discussion/2416/new-insync-version-1-0-25"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "nemo" "nemo-python-git")
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}_all.deb")
sha256sums=("f851c44fd07a463cf64a83cd5ddcf88df71f3d2badbbd3e1b73c307e4b39e25f")
noextract=("${pkgname}-${pkgver}.deb")

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
