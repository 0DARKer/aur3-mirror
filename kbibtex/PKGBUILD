# Contributor: cmorlok <christianmorlok@web.de>

pkgname=kbibtex
pkgver=0.2.3
pkgrel=1
pkgdesc="A BibTeX editor for KDE"
url="http://www.unix-ag.uni-kl.de/~fischer/kbibtex/index.html"
depends=('qt3' 'kdelibs3')
arch=('i686' 'x86_64')
license=('GPL')
source=(http://www.unix-ag.uni-kl.de/~fischer/$pkgname/download/$pkgname-$pkgver.tar.bz2)

build() {
    cd $startdir/src/$pkgname-$pkgver
	msg "Compiling..."
	./configure --without-arts
    make || return 1
    make DESTDIR=$startdir/pkg install
}
md5sums=('211075c5e096e71f1bc311acb5777eff')
sha1sums=('b65709fdba3d992bb147b329ffc2ea762bb24f3d')
