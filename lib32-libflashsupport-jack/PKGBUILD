

pkgname="lib32-libflashsupport-jack"
pkgver="20100626"
pkgrel=1
pkgdesc="JACK-audio-connection-kit support for Flash"
arch=('x86_64')
url="http://sourceforge.net/users/torbenh/"
license=('GPL')
depends=('lib32-jack')
conflicts=('lib32-libflashsupport-oss' 'lib32-libflashsupport-pulse')
source=("http://morfeo.altervista.org/libflashsupport-jack-$pkgver-1-i686.pkg.tar.xz")
md5sums=('b67de4ceee94213604a7e304617b418c')

build() {
  mkdir -p $pkgdir/opt/lib32/usr/lib
  install -Dm755 $srcdir/usr/lib/libflashsupport.so $pkgdir/opt/lib32/usr/lib
}
