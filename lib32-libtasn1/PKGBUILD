pkgname=lib32-libtasn1
pkgver=2.6
pkgrel=1
pkgdesc="The ASN.1 library used in GNUTLS"
arch=('x86_64')
url="http://www.gnu.org/software/libtasn1/"
license=('GPL3' 'LGPL')
depends=('lib32-glibc' 'libtasn1=2.6')
source=('http://mir.archlinux.fr/extra/os/i686/libtasn1-2.6-1-i686.pkg.tar.xz')
md5sums=('83fa43d291f420c9468239c721297c18')

build() {
	cd $pkgdir
	mkdir usr
	cd usr
	mkdir lib32
	cp $srcdir/usr/lib/*.so* $pkgdir/usr/lib32
}
 