# Maintainer: SpepS <dreamspepser at yahoo dot it>

pkgname=cacaoweb-bin
_pkgname=${pkgname/-*/}
pkgver=1.2.0.3
pkgrel=6
pkgdesc="A free and opensource software handling the broadcasting of multimedia content such radio or live sport events."
arch=('i686' 'x86_64')
url="http://www.cacaoweb.org/"
license=('GPL')
depends=('glibc')
provides=("$_pkgname")
conflicts=("$_pkgname")
[ $CARCH = "x86_64" ] && _arc=64 && \
md5sums=('a2c4b4cf8beb6e77f37ecebf773fd77a') || md5sums=('adae7b4a153067adaf3c65d8c257866a')
source=("http://downloads.sourceforge.net/project/$_pkgname/$_pkgname.linux$_arc")

build() {
  return 0
}

package() {
  install -Dm 755 "$srcdir/$_pkgname.linux$_arc" "$pkgdir/usr/bin/$_pkgname"
}
