# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-dolphin
pkgver=1.0.34
pkgrel=1
pkgdesc="Dolphin integration for insync"
url="https://support.insynchq.com/forums/discussion/3300/new-insync-version-1-0-34"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "kdebindings-python2" "kdebase-workspace")
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}.31801-precise_all.deb")
sha256sums=("b6e5c8e4c6b6c1513ff256827bff8893f69368efcb789399fe5db94ecc5049f0")
noextract=("${pkgname}-${pkgver}.deb")
install=$pkgname.install

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
