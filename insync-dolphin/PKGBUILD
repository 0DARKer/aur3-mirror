# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-dolphin
pkgver=1.0.29
pkgrel=1
pkgdesc="Dolphin integration for insync"
url="https://support.insynchq.com/forums/discussion/2789/new-insync-version-1-0-29/p1"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "kdebindings-python2" "kdebase-workspace")
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}.31750_all.deb")
sha256sums=("25dc80501e52dd0b322a9caf20eb3b223afa9b3793cdd2ee53a43b43c9e12043")
noextract=("${pkgname}-${pkgver}.deb")
install=$pkgname.install

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
