# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-dolphin
pkgver=1.0.32
pkgrel=1
pkgdesc="Dolphin integration for insync"
url="https://support.insynchq.com/forums/discussion/3270/new-insync-version-1-0-32"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "kdebindings-python2" "kdebase-workspace")
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}.31767-precise_all.deb")
sha256sums=("a79d2c6ddf17bab11637d591c7ad8538f11cedc56c925f3c6f1012cb41eef643")
noextract=("${pkgname}-${pkgver}.deb")
install=$pkgname.install

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
