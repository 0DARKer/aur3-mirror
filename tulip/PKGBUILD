# $Id: PKGBUILD,v 1.12 2003/11/06 08:26:13 dorphell Exp $
# Maintainer: Thomas Aynaud <taynaud at gmail dot com>
pkgname=tulip
pkgver=3.4.1 # note: if the pkgver had been '0.99-10' then use an underscore. like '0.99_10'
pkgrel=1
pkgdesc="a networks or graphs drawing software"
arch=(i686 x86_64)
url="http://www.tulip-software.org"
license=('GPL')
groups=
provides=
depends=('qt-assistant-compat' 'qt' 'libxml2' 'glew' 'freetype2')
makedepends=
conflicts=
replaces=
backup=
install=
source=("http://sourceforge.net/projects/auber/files/tulip/$pkgname-$pkgver/$pkgname-$pkgver.tar.bz2/download")
md5sums=('0aba11088e4b0ed93889ed5cefdabbe7')
 
build() {
  cd $srcdir/$pkgname-$pkgver
  export MOC=/usr/bin/moc
  export UIC=/usr/bin/uic
  sh gen-conf.sh
  ./configure --prefix=/usr
  make || return 1
  make prefix=$pkgdir/usr install
}
