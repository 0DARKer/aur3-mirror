# Maintainer: Nathan Owe <ndowens.aur at gmail dot com>
pkgname=emacs-auto-indent-mode
pkgver=0.35
pkgrel=1
pkgdesc="Auto indent mode for Emacs"
arch=('any')
url="http://emacswiki.org/emacs/"
license=('unknown')
depends=('emacs')
source=(http://www.emacswiki.org/emacs/download/auto-indent-mode.el)
md5sums=('7d9ac4726f443f29ba60ebb2229e927f')

package() {
  cd "$srcdir/"
  install -d $pkgdir/usr/share/emacs/site-lisp/$pkgname
  install -Dm644 auto-indent-mode.el \
$pkgdir/usr/share/emacs/site-lisp/$pkgname/auto-indent-mode.el
}

