# Maintainer: Raizner Evgeniy <razrfalcon@gmail.com>
pkgname=itmages-dolphin-extension
pkgver=1.00
pkgrel=1
pkgdesc="ITmages dolphin context menu extension."
arch=('i686' 'x86_64')
url="https://launchpad.net/itmages"
license=('LGPL')
depends=('kdebase-dolphin' 'itmages-service')
makedepends=('bzr' 'qt')
md5sums=() #generate with 'makepkg -g'

build() {
  bzr branch lp:itmages/${pkgname}
  cd "$srcdir/$pkgname"
  qmake -config release
  make
  make INSTALL_ROOT="$pkgdir" install
}  
