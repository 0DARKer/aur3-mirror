# Author: Stefano 'jht' <stefano@inventati.org>
pkgname=pyclips
pkgver=1.0.5.327
pkgrel=1
pkgdesc="Python binding for Clips" 
url="http://pyclips.sourceforge.net/" 
license="GPL" 
arch=(i686)
depends=('python' 'pacman>=3.0.0') 
source=(http://downloads.sourceforge.net/sourceforge/$pkgname/$pkgname-$pkgver.tar.gz
	http://www.ghg.net/clips/download/source/CLIPSSrc.zip) 
md5sums=('b87c8936ff8f2f8714c95340cd93fe92'
         'c5dbcb997656a74727c308c0289da27f')

build() { 
	mv ${startdir}/CLIPSSrc.zip ${startdir}/src/pyclips/
	cd ${startdir}/src/pyclips
	python setup.py install --root=$startdir/pkg
}
