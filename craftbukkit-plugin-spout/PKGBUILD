pkgname=craftbukkit-plugin-spout
pkgver=1443
pkgrel=1
pkgdesc="Spout plugin for CraftBukkit"
arch=(any)
url="http://www.spout.org"
license=("LGPLv3")
depends=("craftbukkit>=1.5.1r0.2")
source=("http://get.spout.org/spoutplugin.jar")
