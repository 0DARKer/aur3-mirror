pkgver=1444
pkgrel=1
pkgdesc="Spout plugin for CraftBukkit"
arch=(any)
url="http://www.spout.org"
license=("LGPLv3")
depends=("craftbukkit>=1.5.2r0.1")
source=("http://get.spout.org/spoutplugin.jar")
