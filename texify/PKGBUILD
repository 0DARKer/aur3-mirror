# Contributor: rickyc85
pkgname=texify
pkgver=1.19_all
pkgrel=1
pkgdesc="A perl script that translates source code written in any of several different languages to LaTeX source files (eg. c, vhdl, java...)"
license=('GPL')
depends=('perl')
arch=('i686' 'x86_64')
url="http://packages.debian.org/unstable/utils/texify"
source=(ftp://ftp.ncnu.edu.tw/Linux/Debian/debian/pool/main/t/texify/$pkgname\_$pkgver.deb)
md5sums=('2dd2fc32784ec07a4912d8fa31c9bba0')
build() {
    ar -x $pkgname\_$pkgver.deb
    gzip -d data.tar.gz
    tar -xvf data.tar
    mkdir -p $startdir/pkg/usr
    cp -aR usr/* $startdir/pkg/usr/
}
