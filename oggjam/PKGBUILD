pkgname=oggjam
pkgver=0.9.2
pkgrel=1
pkgdesc="A script to directly download ogg/vorbis files from Jamendo.com"
arch=(i686 x86_64)
url="http://leorockway.com.ar/oggjam"
license=('GPL')
depends=('python')
source=('http://leorockway.com.ar/oggjam-0.9.2')
md5sums=('aa0f28b529741be0bb8f3d3cbeaf621b')

build() {
  cd "$srcdir"

  install -Dm755 ${pkgname}-${pkgver} $pkgdir/usr/bin/$pkgname

}
