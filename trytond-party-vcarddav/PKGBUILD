pkgname=trytond-party-vcarddav
_pkgname=trytond_party_vcarddav
pkgver=2.8.0
_pkgdir=2.8
pkgrel=1
pkgdesc="The party_vcarddav module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.8' 'trytond-party>=2.8')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("8240d10ea899fa835911452bb8ac352a")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py install --root=$pkgdir
}