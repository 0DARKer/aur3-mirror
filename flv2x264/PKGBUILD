# Contributor: 3ED <kas1987@o2.pl>

arch=('i686' 'x86_64')
license=('GPL2')
pkgname=flv2x264
pkgver=0.1
pkgrel=1
pkgdesc="flv to avi converter"
depends=('x264' 'mplayer' 'lame' 'bash')
url="http://3ed.jogger.pl/2007/07/03/flv2x264-wersja-finalna-0-1/"
source=($pkgname.sh)
md5sums=('059076fb8067ec8b672ad30afd3fa5e6')

build() {
  cd $startdir/src
  mkdir -pv $startdir/pkg/usr/bin
  install -v -m 0755 flv2x264.sh $startdir/pkg/usr/bin/flv2x264
}
