# Maintainer: archtux <antonio.arias99999 at gmail.com>

pkgname=photo
pkgver=0.5.0.1
pkgrel=1
pkgdesc="A fast, simple, good looking, yet powerfull and highly configurable image viewer."
arch=('i686' 'x86_64')
url="http://qt-apps.org/content/show.php/Photo?content=147453"
license='GPL'
depends=('qt>4.6')
source=(http://qt-apps.org/CONTENT/content-files/147453-$pkgname-$pkgver.tar.gz)
md5sums='c1c0481a3e01979d588b3a538dc4c48d'

build() {
  cd $srcdir/$pkgname-$pkgver

   # Build
   qmake
   make
   
   # Install
   install -Dm755 photo $pkgdir/usr/bin/photo
}