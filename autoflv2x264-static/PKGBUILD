arch=('any')
license=('GPL2')
pkgname=autoflv2x264-static
pkgver=1.0
pkgrel=1
pkgdesc="Mass flv to avi converter written by graysky and based on 3ed_0's script. Does not inspect source files for vbitrate, uses a static one."
depends=('x264' 'mplayer' 'lame' 'bash')
url=http://dev/null
source=($pkgname.sh)
md5sums=('ddd33da2a4c3647bf4873f4b5a9778f8')

build() {
install -D $pkgname.sh $pkgdir/usr/bin/$pkgname.sh
}
