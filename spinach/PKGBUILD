pkgname=spinach
pkgver=0.1.6
pkgrel=1
pkgdesc="A small AUR helper written in Bash."
arch=('any')
url="http://www.floft.net/wiki/Scripts"
license=('GPL')
depends=('bash' 'coreutils' 'curl' 'grep' 'sed' 'tar')
source=(spinach)
md5sums=('0a7005c472fa1e50622ed0fcd6593da4')

build() {
  install -Dm755 $srcdir/$pkgname $pkgdir/usr/bin/$pkgname
}
