pkgname=spinach
pkgver=0.1.1
pkgrel=1
pkgdesc="A small, slow (but functional) AUR helper written in bash."
arch=('any')
url="http://www.floft.net/wiki/Scripts.html"
license=('GPL')
depends=('bash' 'coreutils' 'curl' 'grep' 'sed' 'tar' 'wget')
source=(http://www.floft.net/uploads/spinach)
md5sums=('decf1ac9abe009cb70cbe98e9bb723ac')

build() {
  install -Dm755 $srcdir/$pkgname $pkgdir/usr/bin/$pkgname
}
