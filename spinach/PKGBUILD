pkgname=spinach
pkgver=0.1.3
pkgrel=2
pkgdesc="A small, slow (but functional) AUR helper written in bash."
arch=('any')
url="http://www.floft.net/wiki/Scripts"
license=('GPL')
depends=('bash' 'coreutils' 'curl' 'grep' 'sed' 'tar')
source=(spinach)
md5sums=('0b30eb194370645a9f5f7a39c77d9fbd')

build() {
  install -Dm755 $srcdir/$pkgname $pkgdir/usr/bin/$pkgname
}
