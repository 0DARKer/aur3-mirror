# Contributor: Garrett <http://floft.net/contact>
pkgname=spinach
pkgver=0.3.94
pkgrel=1
pkgdesc="A small AUR helper written in Bash."
arch=('any')
url="http://floft.net/wiki/Scripts/Spinach"
license=('GPL')
depends=('curl')
source=(spinach spinach.1)
md5sums=('1146ca5bcd08a73292ecfea53cde0a8f' 'a3bd71bfe68efdc88f717ff44ff2cc61')

build() {
  install -Dm755 $srcdir/$pkgname $pkgdir/usr/bin/$pkgname
  install -Dm644 $srcdir/$pkgname.1 $pkgdir/usr/share/man/man1/$pkgname.1
}
