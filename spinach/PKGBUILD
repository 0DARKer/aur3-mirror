pkgname=spinach
pkgver=0.1.7
pkgrel=1
pkgdesc="A small AUR helper written in Bash."
arch=('any')
url="http://floft.net/wiki/Scripts/Spinach"
license=('GPL')
depends=('bash' 'coreutils' 'curl' 'findutils' 'grep' 'sed' 'tar')
source=(spinach)
md5sums=('71b2667dd8ef7b3fbc71b3941f6998d0')

build() {
  install -Dm755 $srcdir/$pkgname $pkgdir/usr/bin/$pkgname
}
