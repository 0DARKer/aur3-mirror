pkgname=spinach
pkgver=0.1
pkgrel=1
pkgdesc="A small, slow AUR helper written in bash."
arch=('any')
url="http://www.floft.net/wiki/Scripts.html"
license=('GPL')
depends=('coreutils' 'wget' 'curl' 'sed' 'grep' 'tar')
source=(spinach)
md5sums=('6bbcdb6b064201304fe2be84ed1b6d28')

build() {
  install -Dm755 $srcdir/$pkgname $pkgdir/usr/bin/$pkgname
}
