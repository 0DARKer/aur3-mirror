pkgname=keepasshttp
pkgver=1.0.3.6
pkgrel=1
pkgdesc="KeePass plugin to expose password entries securely (256bit AES/CBC) over HTTP"
license=('GPL2')
depends=('keepass>=2.14')
url="http://github.com/pfn/keepasshttp"
md5sums=('428c56bfb6252ca806669f69adc33b31'
         '6dc7988bf02bdccb02c03ccd5cfc856e')
source=(https://www.github.com/pfn/keepasshttp/raw/master/KeePassHttp/KeePassHttp.dll https://www.github.com/pfn/keepasshttp/raw/master/KeePassHttp/Newtonsoft.Json.dll)
arch=('any')

package(){
   cd $srcdir
   mkdir -p $pkgdir/usr/share/keepass
   install -m644 {KeePassHttp.dll,Newtonsoft.Json.dll} $pkgdir/usr/share/keepass
}
