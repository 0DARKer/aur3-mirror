pkgname=keepasshttp
pkgver=1.0.3.8
pkgrel=1
pkgdesc="KeePass plugin to expose password entries securely (256bit AES/CBC) over HTTP"
license=('GPL2')
depends=('keepass')
url="http://github.com/pfn/keepasshttp"
md5sums=('8b767d5470fc3ff7aaf0c6fb3e8722f9'
         '6dc7988bf02bdccb02c03ccd5cfc856e')
source=(https://github.com/pfn/keepasshttp/raw/8b04e0f23d4e6e3ed3f096314a17f042b8c091b8/KeePassHttp/KeePassHttp.dll
        https://github.com/pfn/keepasshttp/raw/01bc6848cd5fa1db5766e254d5cc3e8ebfad0374/KeePassHttp/Newtonsoft.Json.dll)
arch=('any')

package(){
   cd $srcdir
   mkdir -p $pkgdir/usr/share/keepass
   install -m644 {KeePassHttp.dll,Newtonsoft.Json.dll} $pkgdir/usr/share/keepass
}
