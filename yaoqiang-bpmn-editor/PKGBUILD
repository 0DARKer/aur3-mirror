pkgname="yaoqiang-bpmn-editor"
pkgver="2.1.35"
pkgrel=1
epoch=
pkgdesc="BPMN 2.0 and UML Editor"
arch=(any)
url="http://bpmn.sourceforge.net/"
license=('GPL3')
groups=()
depends=(java-environment)
makedepends=()
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
changelog=
source=(yaoqiang-bpmn-editor.desktop
yaoqiang-bpmn-editor.png
$pkgname-${pkgver}.jar::http://sourceforge.net/projects/bpmn/files/2.1/$pkgname-${pkgver}.jar/download)
noextract=()
md5sums=(   '138f0ec0eb42a2dd07c7d5a1d60db103'
            '6672924faa8d9bd3fee21a9294dd1a21'
            '820dea270a7065bddf08caed299aa2fe')
package() {
    cd $srcdir
    install -D $pkgname-${pkgver}.jar $pkgdir/opt/$pkgname/$pkgname-${pkgver}.jar
    install -D $pkgname.desktop $pkgdir/usr/share/applications/$pkgname.desktop
    install -D $pkgname.png $pkgdir/usr/share/pixmaps/$pkgname.png
}
