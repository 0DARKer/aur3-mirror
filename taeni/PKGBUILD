pkgname=taeni
pkgver=hg_latest
pkgrel=2
pkgdesc="Terminal emulator based on GTK+ and VTE"
arch=('i686' 'x86_64')
url="http://alphaos.tuxfamily.org/"
license=('GPL')
makedepends=('vala')
depends=('vte3')
source=(https://bitbucket.org/simargl/alphaos/get/tip.tar.gz)
md5sums=('SKIP')
options=(!strip)

package() {
  cd "$srcdir/"simargl-alphaos-*/def-scripts/03_extra/$pkgname/$pkgname
  make DESTDIR="$pkgdir/" install
}