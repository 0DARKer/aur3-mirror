# Maintainer: Your Name <youremail@domain.com>
pkgname=jorgan
pkgver=3.13
pkgrel=1
#epoch=
pkgdesc="Lets you use your computer as a full featured virtual organ."
arch=('any')
url="http://jorgan.sourceforge.net"
license=('GPL3')
#groups=()
depends=('java-runtime')
#makedepends=()
#checkdepends=()
#optdepends=()
provides=()
#conflicts=()
#replaces=()
#backup=()
#options=()
#install=
#changelog=
source=(http://sourceforge.net/projects/$pkgname/files/$pkgname-package/$pkgver/"$pkgname"_"$pkgver"_i386.deb)
#noextract=()
md5sums=('24721324991e2285ee9e3e8a35796874')

build() {
  msg "Extracting ..."
  ar -xv "$pkgname"_"$pkgver"_i386.deb || return 1
  tar -xvf data.tar.gz || return 1
  msg2 "Done extracting."
  msg "Moving files ..."
  cd $srcdir
  mv -t $pkgdir usr
  msg2 "Done moving files."
}

