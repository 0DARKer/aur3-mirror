# Maintainer: nem <nem@ikitten.co.uk>

pkgname=speedstep-centrino
pkgver=3.3.4
pkgrel=1
pkgdesc="Kernel module for Pentium-M (centrino) speedstepping"
url="http://www.kernel.org"
arch=('i686')
license=('GPL2')
depends=('linux>=3.3.4')
source=('speedstep-centrino.ko.gz')
md5sums=('5713e29470a3fb24113470e690d2fae9')
noextract=('speedstep-centrino.ko.gz')

package_speedstep-centrino() {
  install -m644 -D "speedstep-centrino.ko.gz" "$pkgdir/lib/modules/$(uname -r)/kernel/drivers/cpufreq/speedstep-centrino.ko.gz"
}
