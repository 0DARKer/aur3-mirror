# Contributor: Graeme Gott <graeme@gottcode.org>
pkgname=tetzle
pkgver=1.2.1
pkgrel=1
pkgdesc="A jigsaw puzzle with tetrominoes for pieces."
arch=('i686' 'x86_64')
url="http://gottcode.org/tetzle/"
license=('GPL')
depends=('qt')
source=(http://gottcode.org/tetzle/$pkgname-$pkgver-src.tar.bz2)
md5sums=('81f0619f920ccf8ea0273bfde508825e')

build() {
  cd "$srcdir/$pkgname-$pkgver"

  qmake PREFIX=/usr
  make || return 1
  make INSTALL_ROOT=$pkgdir install
}
