pkgname=stoqlib
pkgver=0.8.9
pkgrel=1
pkgdesc="Library required by Stoq application"
arch=('i686')
url="http://www.stoq.com.br"
license=('GPL')
depends=('python>=2.4.2' 'pygtk>=2.8.1' 'postgresql>=8.1' 'python-psycopg2>=2.0.5' 'zope-interface>=3.0.0' 'pil>=1.1.5' 'python-reportlab>=1.2' 'stoqdrivers>=0.5.0' 'kiwi>=1.9.13' 'gazpacho>=0.6.6')
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=(http://download.stoq.com.br/sources/0.8.9/$pkgname-$pkgver.tar.gz)
noextract=()
md5sums=('e61c5ccc1f5bfcedba16beff1adcdbe7')
build() {
  cd $startdir/src/$pkgname-$pkgver
  python setup.py install --root=$startdir/pkg || return 1
}
