# Maintainer: 謝致邦 <Yeking@Red54.com>
 
pkgname=yozooffice
pkgver=2012
pkgrel=1
pkgdesc='Yozo(永中, Yongzhong) office 2012 personal'
arch=('i686' 'x86_64')
url='http://www.yozosoft.com/person/'
makedepends=('binutils')
license=('custom')
options=('!strip')
install=yozooffice.install
source=("http://www.yozosoft.com/download!download.do?productname=2012grbgzdeb"
yozooffice.install)
md5sums=('9ad8be00ee45a1a60ad52fac19a6a55a'
'aa70a3ef472d41e6aef71e159927fa40')
if [ "$CARCH" = "i686" ]; then
	depends=('gtk2' 'libxtst')
elif [ "$CARCH" = "x86_64" ]; then
	depends=('lib32-gtk2' 'lib32-libxtst')
fi

package() {
	cd "$srcdir"
	tar -zxf data.tar.gz -C $pkgdir
}
