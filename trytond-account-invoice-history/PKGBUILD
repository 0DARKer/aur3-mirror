pkgname=trytond-account-invoice-history
_pkgname=trytond_account_invoice_history
pkgver=2.2.0
_pkgdir=2.2
pkgrel=1
pkgdesc="The account_invoice_history module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-account_invoice>=2.2' 'trytond-party>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("f2546ab4730cb3257b363d9bfffdaa4d")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}