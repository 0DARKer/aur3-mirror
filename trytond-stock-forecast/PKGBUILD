_pkgname=trytond_stock_forecast
pkgver=1.4.0
pkgrel=1
pkgdesc="The stock_forecast module of the Tryton application platform"
arch=('any')
url="http://www.tryton.org/"
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=1.4' 'trytond-stock>=1.4' 'trytond-product>=1.4'
'trytond-company>=1.4')
makedepends=('setuptools')
source=("http://downloads.tryton.org/1.4/$_pkgname-$pkgver.tar.gz")
md5sums=('230aa9fd3a35559415f4ae48092fbd35')

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}

