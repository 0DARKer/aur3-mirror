pkgname=trytond-stock-forecast
_pkgname=trytond_stock_forecast
pkgver=2.2.1
_pkgdir=2.2
pkgrel=1
pkgdesc="The stock_forecast module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-workflow>=2.2' 'trytond-stock>=2.2' 'trytond-product>=2.2' 'trytond-company>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("4558996f49b09fe84aacddf8030d99cf")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}