pkgname=libudev.so.0
pkgver=0.1.0
pkgrel=1
pkgdesc="Creates /usr/lib/libudev.so.0 -> /usr/lib/libudev.so symlink"
arch=(x86 x86_64)
url="http://www.dartlang.org/dartium/"
license=('BSD')
depends=()
optdepends=()
makedepends=()
provides=()
conflicts=()
replaces=()
install=
changelog=
noextract=()

package(){
	mkdir -p ${pkgdir}/usr/lib
	ln -s /usr/lib/libudev.so ${pkgdir}/usr/lib/libudev.so.0
}
