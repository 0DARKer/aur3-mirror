# Maintainer: TesX <tesfabpel@gmail.com>
pkgname=craftbukkit-plugin-craftbook
pkgver=3.6.9
pkgrel=1
pkgdesc="Plugin that allows you to build interactive mechanisms, integrated circuits and more..."
arch=(any)
url="http://dev.bukkit.org/bukkit-plugins/craftbook/"
license=('GPL')
depends=('craftbukkit' 'craftbukkit-plugin-worldedit')
source=('http://dev.bukkit.org/media/files/712/274/CraftBook_3.6.9.zip')
md5sums=('7d84f05372f0c6ee6756b77312e1e749')

package() {
  mkdir -p "$pkgdir/srv/craftbukkit/plugins"

  install -m 644 -g craftbukkit -o craftbukkit "$srcdir/CraftBook.jar" "$pkgdir/srv/craftbukkit/plugins/"
}

