pkgname=yle-dl
pkgver=2.1.1
pkgrel=1
_gitid=5b1fcbb
pkgdesc="Download video and audio from YLE Areena."
arch=("any")
url="http://users.tkk.fi/~aajanki/rtmpdump-yle/index.html"
license=("GPL2")
depends=('python2-crypto' 'rtmpdump')
conflicts=('rtmpdump-yle')
replaces=('rtmpdump-yle')
source=("yle-dl-${pkgver}.tar.gz::https://github.com/aajanki/yle-dl/tarball/${pkgver}")

package() {
  cd "${srcdir}/aajanki-${pkgname}-${_gitid}"

  make DESTDIR="${pkgdir}" install
}

md5sums=('2eb8be74725fe6449fcc0a6423f8a896')
