pkgname=mint-cinnamon-themes
pkgver=2014.09.30
pkgrel=1
pkgdesc="Linux Mint Cinnamon Themes"
arch=("any")
url="http://packages.linuxmint.com/pool/main/c/cinnamon-themes/"
license=('unknown')
source=("http://packages.linuxmint.com/pool/main/c/cinnamon-themes/cinnamon-themes_2014.09.30.tar.gz")
md5sums=('17b67f537e777ee4a2ba9cbc21643a9a')

package() {
   cd "$srcdir/cinnamon-themes"
   cp -r usr/ $pkgdir/
   chmod 755 $pkgdir/usr/share/themes/
   chmod 755 $pkgdir/usr/share/themes/*
}
