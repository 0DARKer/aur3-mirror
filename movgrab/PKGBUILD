# Maintainer: archtux <antonio.arias99999@gmail.com>

pkgname=movgrab
pkgver=1.0.3
pkgrel=1
pkgdesc="Movgrab is a command-line movie downloader."
arch=('i686' 'x86_64')
url="http://sites.google.com/site/columscode/home/movgrab"
license=('GPL3')
source=(http://sites.google.com/site/columscode/files/$pkgname-$pkgver.tgz)
md5sums=('2b0b733bb1019e3a200589d667e9ced1')

build() {
  cd $srcdir/$pkgname-$pkgver

  install -Dm755 movgrab $pkgdir/usr/bin/movgrab
}
