#Maintainer: lordwelch
pkgname=mac-ithemes-v3-noobslab
pkgver=3.12
pkgrel=1
pkgdesc="ithemes (MAC OS) Themes GTK+/Cinnamon"
arch=('any')
url="https://launchpad.net/~noobslab/+archive/themes/"
license=('unknown')
source=('https://launchpad.net/~noobslab/+archive/themes/+files/mac-ithemes-v3_3.12-b%7Etrusty%7ENoobsLab.com.tar.gz')
md5sums=('5cac7b6622a27ea2f96c4a7c9ad6d896')

package() {
	cd "$srcdir/Mbuntu-themes"
	mkdir -p "$pkgdir/usr/share/themes"
	cp -r M* "$pkgdir/usr/share/themes"
	chmod 755 -R "$pkgdir/usr/share/themes/"
}
