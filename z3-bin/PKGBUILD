
# Contributor: Paolo Herms

pkgname=z3-bin
pkgver=2.2
pkgrel=1
pkgdesc="An automatic theorem prover"
arch=('any')
url="http://research.microsoft.com/en-us/um/redmond/projects/z3"
license=('custom')
source=(http://why.lri.fr/z3-2.2.tar.gz 'http://research.microsoft.com/en-us/downloads/0a7db466-c2d7-4c51-8246-07e25900c7e7/Z%203%20MSR-LA%20(2007-07-07).txt')
md5sums=('8afce49e0eac1d0ddc28ee77a0bac5f2' 'c5f05e7f2f95c796d5101069276d326f')

build() {
  install -D "$srcdir"/z3/bin/z3 "$pkgdir"/usr/bin/z3
  install -m644 -D "$srcdir/Z%203%20MSR-LA%20(2007-07-07).txt" "$pkgdir"/usr/share/licenses/$pkgname/LICENSE
}
