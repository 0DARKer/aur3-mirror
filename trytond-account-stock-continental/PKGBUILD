pkgname=trytond-account-stock-continental
_pkgname=trytond_account_stock_continental
pkgver=2.2.0
_pkgdir=2.2
pkgrel=1
pkgdesc="The account_stock_continental module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-account>=2.2' 'trytond-account_product>=2.2' 'trytond-stock>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("711ead8a62a1503496cd4a50383a3ee0")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}