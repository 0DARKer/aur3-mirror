# Maintainer: realitygaps <realitygaps AT yahoo DOT com>

pkgname=fengoffice
pkgver=2.4.0.6
pkgrel=1
pkgdesc="OpenGoo is an easy to use Open Source Web Office"
arch=('i686' 'x86_64')
url="http://www.fengoffice.com/"
license=('GPL')
depends=('php')
install=${pkgname}.install
source=(http://downloads.sourceforge.net/opengoo/$pkgname/${pkgname}_${pkgver}/${pkgname}_$pkgver.zip)
md5sums=('32167e770f74a4cc8c2b6b6821b1fdcc')

build() {
  mkdir -p $startdir/pkg/${pkgname}/srv/http/fengoffice
  cp -r $startdir/src/* $startdir/pkg/${pkgname}/srv/http/$pkgname
}
