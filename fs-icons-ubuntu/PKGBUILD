# Maintainer: crazyelf5

pkgname=fs-icons-ubuntu
pkgver=0.2
pkgrel=1
pkgdesc="An icon theme based on possible new icons for Ubuntu by Yellowicon"
arch=('any')
url="http://franksouza183.deviantart.com/art/FS-Icons-Ubuntu-288407674"
license=('GPL')
source=("http://www.deviantart.com/download/288407674/fs_icons_ubuntu_by_franksouza183-d4rpksa.zip")
md5sums=('7d01cf395219a67279a32b3b29a45ccc')

package() {
  install -dm755 "$pkgdir"/usr/share/icons/fs-icons-ubuntu{,-mono-dark,-mono-light}

  tar -xf fs-icons-ubuntu.tar.gz -C "$pkgdir/usr/share/icons/fs-icons-ubuntu/" 2>/dev/null
  tar -xf fs-icons-ubuntu-mono-dark.tar.gz -C "$pkgdir/usr/share/icons/fs-icons-ubuntu-mono-dark/"
  tar -xf fs-icons-ubuntu-mono-light.tar.gz -C "$pkgdir/usr/share/icons/fs-icons-ubuntu-mono-light/"
}
