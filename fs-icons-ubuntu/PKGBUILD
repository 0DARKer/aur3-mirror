# Maintainer: crazyelf5

pkgname=fs-icons-ubuntu
pkgver=0.1
pkgrel=1
pkgdesc="An icon theme based on possible new icons for Ubuntu 12.04 by Yellow Icon"
arch=('any')
url="http://franksouza183.deviantart.com/art/FS-Icons-Ubuntu-288407674"
license=('cc-by-nc-sa-3.0')
source=("http://www.deviantart.com/download/288407674/fs_icons_ubuntu_by_franksouza183-d4rpksa.zip")
md5sums=('0c4dede029a39fe12db9d409921098c9')

package() {
  tar -xf fs-icons-ubuntu.tar.gz
  find . -type f -not -name *.tar.gz -exec install -Dm644 '{}' "$pkgdir/usr/share/icons/$pkgname/{}" \;
}
