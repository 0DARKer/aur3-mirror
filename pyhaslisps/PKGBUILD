pkgname=pyhaslisps
pkgver=1.06_7
pkgrel=1
pkgdesc="A python program that returns the available implementations of common lisp."
arch=('any')
url="https://bitbucket.org/agalog/releases/downloads"
license=('BSD')
depends=('python2')
source=("$pkgname-${pkgver}.tar.gz")
md5sums=("0f930689a847d086b229a80d8c4c579f")

prepare() {

    cd $srcdir/$pkgname-$pkgver

    make DESTDIR=. ready

}



package() {

    cd $srcdir/$pkgname-$pkgver
    make DESTDIR=$pkgdir REALNAME=$pkgname install
}

