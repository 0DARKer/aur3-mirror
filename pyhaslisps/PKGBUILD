pkgname=pyhaslisps
pkgver=1.05
pkgrel=3
pkgdesc="A python program that returns the available implementations of common lisp."
arch=('any')
url="https://bitbucket.org/agalog/releases/downloads"
license=('BSD')
depends=('python2')
source=("pyhaslisps.py" "pyhaslisps.1" "license")
md5sums=('9c46cd7c23bc60a12fc4ac594988760f' 'c28a12b04a8929189f513b5fb86fb916' '444e9abd91f36de2aa2e8a565cc639aa')

package() {

    cd "$srcdir"
    install -m 644 -D "${pkgname}.1" "$pkgdir/usr/share/man/man1/${pkgname}.1"
    install -m 755 -D "${pkgname}.py" "$pkgdir/usr/bin/${pkgname}"
    install -m 644 -D "license" "$pkgdir/usr/share/licenses/${pkgname}/license.txt"
}

