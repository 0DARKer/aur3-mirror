pkgname=pyhaslisps
pkgver=1.04
pkgrel=1
pkgdesc="A python program that returns the available implementations of common lisp."
arch=('any')
url="https://bitbucket.org/agalog/releases/downloads"
license=('BSD')
groups=('')
depends=('python2')
makedepends=('')
provides=()
conflicts=()
replaces=()
backup=()
options=()
source=("https://bitbucket.org/agalog/releases/downloads/${pkgname}-${pkgver}.tar.gz")
md5sums=('0fa4d7d11c9bf941a5dddfc22c8b38ca')


build() {

    cd "$srcdir/$pkgname"
    install -m 644 -D "pyhaslisps.1" "$pkgdir/usr/share/man/man1/pyhaslisps.1"
    install -m 755 -D "pyhaslisps" "$pkgdir/usr/bin/pyhaslisps"
    install -m 644 -D "license.txt" "$pkgdir/usr/share/licenses/pyhaslisps/license.txt"
}

