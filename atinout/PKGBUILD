pkgname=atinout
pkgver=0.9
pkgrel=1
pkgdesc='AT commands as input are sent to modem and responses given as output.'
arch=(i686 x86_64)
url='http://atinout.sourceforge.net/'
license=('GPL')
makedepends=('')
conflicts=('')
 source=(http://garr.dl.sourceforge.net/project/atinout/atinout-$pkgver.tar.gz)
md5sums=('9beb0dd202b9acc40d8dabbee3f2a033')


 build() {
    cd ${srcdir}/atinout-$pkgver
#    make
   gcc atinout.c -o atinout
 }

 package() {
    cd ${srcdir}/atinout-$pkgver
    install -Dm 755 atinout $pkgdir/usr/bin/atinout
}

