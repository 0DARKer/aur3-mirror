pkgname=dexter
pkgver=0.16
pkgrel=1
pkgdesc="Address Book that goes hand in hand with Postler"
arch=('i686' 'x86_64')
url="https://launchpad.net/dexter-rolodex"
license=('GPL')
depends=('enchant' 'python-distutils-extra' 'python-vobject' 'python-pyenchant' 'python2-storm' 'python2-pyenchant')
makedepends=('python2')
conflicts=('dexter-bzr')
source=(http://launchpad.net/$pkgname-rolodex/trunk/$pkgver/+download/dexter_$pkgver.tar.gz)
md5sums=('5980b1bed169c8fc97338f3145759f30')

build() {
   cd $srcdir/$pkgname-rolodex
   python2 setup.py install --prefix=$pkgdir/usr
}
