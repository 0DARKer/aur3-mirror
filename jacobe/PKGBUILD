pkgname=jacobe
pkgver=7.3.14
pkgrel=1
pkgdesc="Jacobe is a configurable code beautifier for the Java programming language"
arch=(i686 x86_64)
url="http://www.tiobe.com/index.php/content/products/jacobe/Jacobe.html"
license=("custom")
source=("http://www.tiobe.com/content/products/jacobe/jacobe.linux.tar.gz"
"jacobe.sh")
md5sums=('fe4e3544094878af85c84f5236b9bc4b'
         'f3b451e738e281c9397f496acc8f9ebe')

package() {
	cd "$srcdir"
	install -Dm755 jacobe "$pkgdir/usr/share/jacobe/jacobe"
	install -m644 sun.cfg "$pkgdir/usr/share/jacobe/"
	install -Dm755 jacobe.sh "$pkgdir/usr/bin/jacobe"
}
