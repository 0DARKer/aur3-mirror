# Maintainer: Marat Mukhametshin <mukhametshin[at]yandex.ru>
pkgname=tomboy-export-to-latex
pkgver=0.18
pkgrel=1
pkgdesc="A tomboy plugin to export whole notebooks to a LaTeX file which can be used to make a PS or PDF file"
arch=(any)
url="http://notendur.hi.is/pth8/"
license=(freeware)
depends=('tomboy')

source=(http://notendur.hi.is/pth8/binaries/ExportToLaTeX_${pkgver}.zip)
md5sums=(9826a68a0a1233ed0512c6fd4539a15a)

build() {
  true
}

package() {
  install -D "$srcdir/ExportToLaTeX.dll" \
  "$pkgdir/usr/lib/tomboy/addins/ExportToLaTeX.dll"
}

