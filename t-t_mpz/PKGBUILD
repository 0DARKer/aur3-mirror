# Contributor: T-T

pkgname='t-t_mpz'
pkgver='1.0'
pkgrel=0
pkgdesc="T-T | MPZ"
arch=('i686' 'x86_64')
url="http://sourceforge.net/projects/mpz/files/"
license=('BSD')
depends=(bash mpc zenity)
makedepends=()
conflicts=()
optdepends=()

source=("https://downloads.sourceforge.net/project/mpz/mpz-$pkgver")
md5sums=('d58efd3600ca47bd34b6041500e47201')

build() {
	cd $srcdir/
	
	mkdir $pkgdir/usr
	mkdir $pkgdir/usr/bin
	
	mv $srcdir/mpz-$pkgver $pkgdir/usr/bin/mpz
}
