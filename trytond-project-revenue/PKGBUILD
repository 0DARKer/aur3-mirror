pkgname=trytond-project-revenue
_pkgname=trytond_project_revenue
pkgver=2.2.1
_pkgdir=2.2
pkgrel=1
pkgdesc="The project_revenue module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-project>=2.2' 'trytond-timesheet>=2.2' 'trytond-company>=2.2' 'trytond-product>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("60ec35d7010ef4856b6198b93bb95ecd")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}