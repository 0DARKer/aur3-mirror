pkgname=keepassx-zh_cn
pkgver=0.4.0
pkgrel=1
pkgdesc="zh_CN Locale of keepassx"
license=('GPL2')
depends=('keepassx')
url="http://www.keepassx.org"
md5sums=('5ddc8bf58a10214c6338abf8440586b9')
source=(http://code.google.com/p/axr/keepassx-zh_cn.tar.gz)
arch=('any')

build() {
	cd $startdir/src/keepassx-zh_cn
	mkdir -p $startdir/pkg/usr/share/keepassx/i18n
	install -m 644 keepassx-zh_CN.qm   $startdir/pkg/usr/share/keepassx/i18n/
}
