pkgname=python2-ipaddress
pkgver=1.0.6
pkgrel=2
pkgdesc="IPv4/IPv6 manipulation library"
arch=(any)
url="https://github.com/phihag/ipaddress"
license=('unknown')
depends=('python2')
source=("http://pypi.python.org/packages/source/i/ipaddress/ipaddress-1.0.6.tar.gz")
md5sums=('a869a0c03f03961c714046a579e38e68')

package() {
    cd $srcdir/${pkgname/python2-/}-$pkgver
    python2 setup.py install --root="$pkgdir/" --optimize=1
}
