pkgname=python2-ipaddress
pkgver=1.0.3
pkgrel=1
pkgdesc="IPv4/IPv6 manipulation library"
arch=(any)
url="https://github.com/phihag/ipaddress"
license=('unknown')
depends=('python2')
source=(http://pypi.python.org/packages/source/i/ipaddress/ipaddress-1.0.3.tar.gz)
md5sums=('e30ca46929f34c20da4cea4ece7910fb')

package() {
    cd $srcdir/${pkgname/python2-/}-$pkgver
    python2 setup.py install --root="$pkgdir/" --optimize=1
}
