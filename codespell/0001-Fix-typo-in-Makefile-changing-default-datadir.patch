From: Lucas De Marchi <lucas.demarchi@profusion.mobi>
Date: Mon, 5 Mar 2012 16:31:30 -0300
Subject: [PATCH] Fix typo in Makefile changing default datadir

---
 Makefile |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/Makefile b/Makefile
index b508d15..6722451 100644
--- a/Makefile
+++ b/Makefile
@@ -1,6 +1,6 @@
 prefix ?= /usr
 bindir ?= ${prefix}/bin
-datadir ?= ${prefix}/share/codesell
+datadir ?= ${prefix}/share/codespell
 
 _VERSION := $(shell grep -e "VERSION = '[0-9]\.[0-9]" codespell.py | cut -f 3 -d ' ')
 VERSION = $(subst ',,$(_VERSION))
