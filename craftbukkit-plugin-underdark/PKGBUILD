pkgname=craftbukkit-plugin-underdark
pkgver=0.1.0
pkgrel=1
pkgdesc="Underdark world generator plugin for Bukkit"
arch=(any)
url="http://dev.bukkit.org/server-mods/underdark"
license=("AGPLv3")
depends=("craftbukkit>=1.5.2r1.0")
optdepends=("craftbukkit-plugin-multiverse-core")
source=("http://dev.bukkit.org/media/files/712/121/Underdark.jar")
noextract=("Underdark.jar")
md5sums=('820cfad963510d5ea4122bf4a3626f41')

package() {
  install -Dm644 "$srcdir/Underdark.jar" "$pkgdir/srv/craftbukkit/plugins/Underdark.jar"
}

