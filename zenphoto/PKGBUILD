pkgname=zenphoto
pkgver=1.4.3.5
pkgrel=1
pkgdesc="A PHP-based photo gallery." 
arch=('i686' 'x86_64')
url="http://www.zenphoto.org/"
license=('GPL')
depends=('php' 'mysql')
source=(http://zenphoto.googlecode.com/files/$pkgname-$pkgver.tar.gz)
sha1sums=('215d41a95897654bb7777be0e4b10c7b30f4650a')

install=${pkgname}.install

package() {
  mkdir -p $pkgdir/srv/http
  tar xf $pkgname-$pkgver.tar.gz -C $pkgdir/srv/http 	
}
