pkgname=zenphoto
pkgver=1.3.1
pkgrel=1
pkgdesc="A PHP-based photo gallery" 
arch=('i686' 'x86_64')
url="http://www.zenphoto.org/"
license=('GPL')
groups=()
depends=('php>=5' 'mysql>=5')
provides=('zenphoto' 'zenphoto.install')
source=(http://www.zenphoto.org/files/$pkgname-$pkgver.tar.gz)
install=${pkgname}.install
build() {
	mkdir -p $pkgdir/srv/http
	tar xvf $pkgname-$pkgver.tar.gz -C $pkgdir/srv/http 	
	}
md5sums=('f88e7dc86805c7d7a1b18827c82aec4c')
