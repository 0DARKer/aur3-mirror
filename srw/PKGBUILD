# Contributor: Andreas B. Wagner <AndreasBWagner@gmail.com>
pkgname=srw
pkgver=0.6
pkgrel=1
pkgdesc="simple read wrapper"
arch=('i686' 'x86_64')
url="http://www.deepcube.net/code/srw/"
license=('MIT')
provides=('srw')
source=($pkgname-$pkgver.tar.gz)
md5sums=('6abe525794e7d849eadf638f3cda5bd9')

build() {
  cd "$srcdir/$pkgname-$pkgver"

  make
}

package() {
  cd "$srcdir/$pkgname-$pkgver"

  make PREFIX="$pkgdir/usr" install
}

