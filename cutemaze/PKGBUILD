# Contributor: Graeme Gott <graeme@gottcode.org>
pkgname=cutemaze
pkgver=1.1.0
pkgrel=1
pkgdesc="Top-down maze game"
arch=('i686' 'x86_64')
url="http://gottcode.org/cutemaze/"
license=('GPL')
depends=('qt')
source=(http://gottcode.org/cutemaze/$pkgname-$pkgver-src.tar.bz2)
md5sums=('ffe91cbd4e249393ef2474bf62846f2e')

build() {
  cd "$srcdir/$pkgname-$pkgver"

  qmake PREFIX=/usr
  make || return 1
  make INSTALL_ROOT=$pkgdir install || return 1
}
