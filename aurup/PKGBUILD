pkgname=aurup
pkgver=0.7.8
pkgrel=2
pkgdesc="A command line tool to upload AUR packages"
arch=('any')
url="http://pierloz.com/Aurup"
license=('GPL')
depends=('sh' 'html2text' 'links' 'curl')
source=('aurup')
md5sums=('1031a54212c41986ec7111c01ffa4040')
build() {
  install -Dm755 $srcdir/$pkgname $pkgdir/usr/bin/$pkgname
}
