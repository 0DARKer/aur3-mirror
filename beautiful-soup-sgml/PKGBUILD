pkgname=beautiful-soup-sgml
pkgver=3.0.7a
pkgrel=2
pkgdesc="Older version of Beautiful Soup using the deprecated SGMLParser"
arch=('i686' 'x86_64')
url="http://www.crummy.com/software/BeautifulSoup/index.html"
license=('PSF')
depends=('python')
conflicts=('beautiful-soup')
provides=('beautiful-soup')
source=(http://www.crummy.com/software/BeautifulSoup/download/3.x/BeautifulSoup-$pkgver.tar.gz)

build() {
    cd $startdir/src/BeautifulSoup-$pkgver
    python setup.py install --root=$startdir/pkg
}
md5sums=('d28c8148bb770e72762eb202fa797b56')

