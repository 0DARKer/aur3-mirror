pkgname=iscan-plugin-cx4400
pkgver=2.0.0_0.c2
pkgrel=1
pkgdesc="iscan plugin for Epson CX 4400 and similar scanners."
arch=('i686')
url="http://www.avasys.jp/lx-bin2/linux_e/spc/DL1.do"
license=('GPL' 'LGPL' 'EAPL')
depends=('iscan')
makedepends=('rpmextract')
source=($pkgname-${pkgver//_/-}.i386.rpm)
md5sums=('7e4b7b049797035303ffc687aca02f2f')
build() {
  cd "$startdir"
  rpmextract.sh "$pkgname-${pkgver//_/-}.i386.rpm"
  mv usr "$pkgdir"
}
