#Maintainer: Xyne <ac xunilhcra enyx, backwards>
pkgname=facadefs
pkgver=2012.02.16.57423
pkgrel=14
pkgdesc='A FUSE-based filesystem sandbox.'
arch=(any)
license=(GPL)
url="http://xyne.archlinux.ca/projects/facadefs"
depends=(python2 python-fuse)
source=(http://xyne.archlinux.ca/projects/facadefs/archives/facadefs-2012.02.16.57423.tar.xz)
sha512sums=(1d741fea9b73b18dfcd88c7559d18380c711dcec22016d61c312545231de6bef505d7b7bcce2ca2466f9996a1b9703380519cb5317c8c2e33f81f2829d5d094e)
md5sums=(4a2ba78bbedddf1739756af3d02f41ac)

build () 
{ 
  install -D -m755 "$srcdir/$pkgname-$pkgver/$pkgname" "$pkgdir/usr/bin/$pkgname";
  install -D -m644 "$srcdir/$pkgname-$pkgver/man/$pkgname.1.gz" "$pkgdir/usr/share/man/man1/$pkgname.1.gz"
}


