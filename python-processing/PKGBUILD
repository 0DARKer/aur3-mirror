pkgname=python-processing
pkgver=0.52
pkgrel=1
pkgdesc="Package for using processes which mimics the threading module"
url="http://developer.berlios.de/projects/pyprocessing"
arch=('i686')
license=("BSD")
depends=('python')
provides=('python-processing')
conflicts=('python-processing')
source=(http://download.berlios.de/pyprocessing/processing-${pkgver}.zip)
md5sums=('62772fa3002d003b2395ed669072d51d')


build() {
  cd $startdir/src/processing-${pkgver}
  python setup.py install --prefix=/usr --root=$startdir/pkg || return 1
}
