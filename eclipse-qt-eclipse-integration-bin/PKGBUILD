pkgname=eclipse-qt-eclipse-integration-bin
pkgver=1.6.1
pkgrel=3
pkgdesc="official binaries of the Qt Eclipse plugin"
arch=('i686' 'x86_64')
url="http://qt.nokia.com/developer/eclipse-integration"
license=('LGPL')
depends=('qt>=4.6.1' 'eclipse' 'eclipse-cdt')
install=eclipse-qt-eclipse-integration-bin.install
source=(http://get.qt.nokia.com/qteclipse/qt-eclipse-integration-linux.${CARCH}-${pkgver}.tar.gz)

md5sums=('3bf27fca4dd9051071f4bbf8ff761655')
[ "$CARCH" = "x86_64" ] && md5sums=('3b5ad955dd84608de07331509b4ddffb')

package() {
    install -dm755 ${pkgdir}/usr/share
    tar -xzf "${srcdir}/qt-eclipse-integration-linux.${CARCH}-${pkgver}.tar.gz" -C ${pkgdir}/usr/share
}
