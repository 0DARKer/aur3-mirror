pkgname=pyfprint 
pkgver=0.1
pkgrel=1
pkgdesc="Python bindings for libfprint "
arch=('i686' 'x86_64')
url="http://www.reactivated.net/fprint/wiki/Libfprint"
depends=('python' 'libfprint')
license=('GPL')
source=(http://chacara.homelinux.net/archlinux/pyfprint.tar.gz)
md5sums=('bcbfa0cfaf91dd60f3a379f5466bf867')

build() {
cd ${srcdir}/${pkgname}
	cp -rf  ./lib $pkgdir

}
