build() {
  mkdir -p "$pkgdir"/var/lib/asterisk/g72x-codecs/${pkgver} || return 1
  mkdir -p "$pkgdir"/var/lib/asterisk/g72x-codecs/${pkgver}/g729 || return 1
  mkdir -p "$pkgdir"/var/lib/asterisk/g72x-codecs/${pkgver}/g723.1 || return 1
  install codec_g723*.so "$pkgdir"/var/lib/asterisk/g72x-codecs/${pkgver}/g723.1 || return 1
  install codec_g729*.so "$pkgdir"/var/lib/asterisk/g72x-codecs/${pkgver}/g729 || return 1
  install README "$pkgdir"/var/lib/asterisk/g72x-codecs || return 1
}

