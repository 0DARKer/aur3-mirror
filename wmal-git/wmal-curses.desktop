#!/usr/bin/env xdg-open
[Desktop Entry]
Name=wMAL (ncurses)
Exec=wmal-curses
Terminal=true
Type=Application
Categories=Network;
Icon=/usr/lib/python2.7/site-packages/wmal/data/wmal_icon.png
StartupWMClass=wmal
Comment=wMAL Updater (ncurses frontend)

#comment out  the following line if you want to use the ncurses frontend
NoDisplay=true

Name[nl_NL]=wMAL (ncurses)
Name[en_EN]=wMAL (ncurses)
