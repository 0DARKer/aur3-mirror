name = eduroam-802
author = Oliver Wiedemann
version = 3
require anonymous *Anonymous identity *Identity password *Password cert *Cert
protected password *Password
network={
	ssid=$_ESSID
 	scan_ssid=$_SCAN 
	key_mgmt=IEEE8021X
	eap=TTLS          
	anonymous_identity=$ANONYMOUS
	identity="$_IDENTITY"
	password="$_PASSWORD"
	ca_cert="$_CERT"
	phase2="auth=PAP"
}
