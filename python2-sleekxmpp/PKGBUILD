pkgname=python2-sleekxmpp
pkgver=1.2.4
pkgrel=1
pkgdesc="A XMPP library written for Python 3.x (with 2.6 compatibility)"
arch=(any)
url="http://github.com/fritzy/SleekXMPP"
license=('MIT')
depends=('python2')
source=("https://pypi.python.org/packages/source/s/sleekxmpp/sleekxmpp-1.2.4.tar.gz")
md5sums=('e64f0cf806c65f94c624d5688d183a6b')

package() {
  cd "$srcdir/sleekxmpp-$pkgver"
  python2 setup.py install --root="${pkgdir}/" --optimize=1
}
