# Maintainer: RaK dimitri-b@orange.fr
pkgname=memwrite
pkgver=1.0.0
pkgrel=1
pkgdesc="memwrite is a cheat ArtMoney-like "
arch=(i686 x86_64)
url="http://fr.congelli.eu/"
license=('GPL')
groups=
provides=
depends=('')
makedepends=('wxwidgets')
source=(http://fr.congelli.eu/download/memwrite/$pkgname-$pkgver.tar.gz)
md5sums=('53a43b7710b1d633f3c479b5d042ed85')
 
build() {
	cd $srcdir/$pkgname-$pkgver
	./configure --prefix=/usr
	make || return 1
	make DESTDIR=$pkgdir install || return 1
	}
md5sums=('53a43b7710b1d633f3c479b5d042ed85')
