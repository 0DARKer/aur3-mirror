pkgname=commons-logging
pkgver=1.1.1
pkgrel=1
pkgdesc="A Java URL logging"
arch=('i686' 'x86_64')
url="http://commons.apache.org/logging/"
license="Apache"
depends=('java-runtime' )
conflicts=('')
source=(http://www.apache.org/dist/commons/logging/binaries/${pkgname}-${pkgver}-bin.zip)

package() {
	cd ${srcdir}/${pkgname}-${pkgver}
	mkdir -p ${pkgdir}/usr/share/java/${pkgname}
	cp -r *.jar ${pkgdir}/usr/share/java/${pkgname}
}


sha256sums=('734f22b6bc3ce0c67b6b98cadc9f46e34ddb0326f067de0bb6a53e79db316933')
md5sums=('f88520ed791673aed6cc4591bc058b55')
