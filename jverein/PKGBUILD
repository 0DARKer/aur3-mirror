pkgname=jverein
pkgver=1.3.3
pkgrel=1
pkgdesc="A club management plugin for jameica."
arch=('i686' 'x86_64')
url="http://www.jverein.de"
license=("GPL")
depends=('java-runtime>=1.5' 'jameica>=1.9' 'hibiscus>=1.11' 'libstdc++5')
makedepends=('unzip')
install=(jverein.install)
source=("http://www.jverein.de/phpATM/index.php?action=downloadfile&filename=jverein.$pkgver.zip&directory=Freigegebene%20Versionen&")
md5sums=('d8a4bf94d946fe4a5dd15feef4d7e271')

build() {
  mkdir -p $startdir/pkg/opt/jameica/plugins
  cd $startdir/src
  cp -r $pkgname $startdir/pkg/opt/jameica/plugins
}

