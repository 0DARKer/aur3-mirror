pkgname=python2-icalendar
pkgver=3.6.1
pkgrel=1
pkgdesc='A parser/generator of iCalendar files (RFC 2445)'
arch=('any')
url='http://pypi.python.org/pypi/icalendar/'
license=('GPL')
depends=('python2' 'python2-pytz' 'python2-dateutil')
makedepends=('python2-distribute')
source=(http://pypi.python.org/packages/source/i/icalendar/icalendar-${pkgver}.tar.gz)
md5sums=('b6d465c86dfb354f1c9dac93f4a8a1d0')
sha256sums=('ff7e6256515983fd2d10535a171fdaf8e0d5a1be814d6486e4f3c8438568b46c')

package() {
  cd "icalendar-${pkgver}"
  python2 setup.py install --root=${pkgdir} --optimize=1
}

