pkgname=python2-icalendar
pkgver=2.2
pkgrel=1
pkgdesc='A parser/generator of iCalendar files (RFC 2445)'
arch=('any')
url='http://pypi.python.org/pypi/icalendar/'
license=('GPL')
depends=('python2')
conflicts=('python-icalendar')
source=(http://pypi.python.org/packages/source/i/icalendar/icalendar-${pkgver}.tar.gz)
md5sums=('f019b8d21432305271f941c906197aca')
sha256sums=('ba3e4579dce0051f90b447c3170f830fb60d318279a7680662686f4fb0f292b9')

build() {
  cd "icalendar-${pkgver}"
  python2 setup.py install --root=${pkgdir} --optimize=1
}

