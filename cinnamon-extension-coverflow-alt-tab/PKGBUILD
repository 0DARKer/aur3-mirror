# Maintainer: Ner0

pkgname=cinnamon-extension-coverflow-alt-tab
pkgver=1.3
pkgrel=1
pkgdesc="CoverflowAltTab let's you Alt-Tab through your windows in a cover-flow manner."
arch=('any')
url="http://cinnamon-spices.linuxmint.com/extensions/view/3"
license=('GPL3')
depends=('cinnamon')
source=("http://cinnamon-spices.linuxmint.com/uploads/extensions/XTCT-1TQO-5YQ6.zip")
md5sums=('2f8475f423a89f27c71c43eeb2e8eaa4')

package() {
  find . -type f -exec install -Dm644 '{}' "$pkgdir/usr/share/cinnamon/extensions/{}" \;
}
