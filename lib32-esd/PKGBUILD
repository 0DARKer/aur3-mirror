pkgname=lib32-esd
pkgver=0.2.39
pkgrel32=1
pkgrel=1
pkgdesc="Enlightened Sound Daemon"
url="http://www.tux.org/~ricdude/EsounD.html"
license=('LGPL')
arch=('x86_64')
depends=('lib32-audiofile' 'lib32-alsa-lib')
groups=('lib32')
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.gz)
md5sums=('4fd4cc3e169bd45542b7462d6cd42f9a')

build() {
  mkdir -p $startdir/pkg/opt/lib32/usr/lib/
  cp -R $startdir/src/usr/lib/*.so* $startdir/pkg/opt/lib32/usr/lib
}
