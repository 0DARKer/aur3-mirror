pkgname=xfce4-xquisite-icon-theme
pkgver=0.4
pkgrel=1
pkgdesc="Xquisite icon theme for XFCE"
arch=('any')
url="http://xfce-look.org/content/show.php/Xquisite?content=69735"
license=('GPL')
source=('http://www.fileden.com/files/2008/12/27/2240700/Xquisite.tbz')
md5sums=('ff3f185c09f9358847820cfede06cbdd')

package() {
 mkdir -p "$pkgdir/usr/share/icons"
 cp -R "$srcdir/Xquisite" "$pkgdir/usr/share/icons"
 rm "$pkgdir/usr/share/icons/Xquisite/index.theme~"
}
