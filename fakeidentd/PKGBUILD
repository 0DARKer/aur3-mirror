pkgname=fakeidentd
pkgver=2.5
pkgrel=1
pkgdesc="Concurrent nonforking standalone fake identd"
arch=('i686')
url="http://www.guru-group.fi/~too/sw/"
license=('GPLv2')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
source=(http://www.guru-group.fi/~too/sw/releases/identd.c)
noextract=()
md5sums=('8bdeb74ae591a25de6d8e002fab0b541')

build() {
  cd $startdir/src/
  sh identd.c -s -DXXXMULTI
  install -m755 -D identd $startdir/pkg/usr/bin/identd
}
