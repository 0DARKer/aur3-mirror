post_install() {
  update-desktop-database -q /usr/share/applications

 cat <<-EndOfMessage

==> You can create your accout here : https://secure.ryzom.com/signup/?language=en Or in french https://secure.ryzom.com/signup/?language=fr'

}

post_upgrade() {
  post_install $1
}

post_remove() {
  post_install $1
}
