# Contributor: Maciej Sitarz <macieks@freesco.pl>

pkgname=bash-completion-netcfg
pkgver=0.1
pkgrel=1
pkgdesc="bash completion for netcfg"
arch=('i686' 'x86_64')
url="http://bbs.archlinux.org/viewtopic.php?id=49369"
license=('GPL')
depends=('bash-completion' 'netcfg>=2')
source=('netcfg')
md5sums=('6f91cf725b6116911979f2fa6faf29ef')

build() {
  install -D -m644 $startdir/src/netcfg $startdir/pkg/etc/bash_completion.d/netcfg || return 1
}
