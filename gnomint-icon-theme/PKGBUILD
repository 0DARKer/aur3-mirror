# Maintainer: GreenRaccoon23 <GreenRaccoon23@gmail.com>

pkgname=gnomint-icon-theme
pkgver=2
pkgrel=2
pkgdesc="GNOME icon theme with Mint-X folder icons"
arch=('any')
url="https://launchpad.net/gnomint-icon-theme"
license=('GPL3')
depends=('gnome-icon-theme')
source=('https://launchpad.net/gnomint-icon-theme/trunk/version2/+download/gnomint-source-2.tar.gz')
install='gnomint.install'
md5sums=('e02ecd107c6a0a0bcc9a511a1de69aae')

package() {
  cd "$srcdir/$pkgname"
  mkdir -p $pkgdir/usr/share/icons/$pkgname
  # Installing icons
  cp -R $srcdir/$pkgname/* $pkgdir/usr/share/icons/$pkgname/
}
