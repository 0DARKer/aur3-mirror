pkgname='vim-colorscheme-mustang'
pkgver=20080926
pkgrel=1
pkgdesc="Dark Vim colorscheme for coders. Supports GUI and terminal modes."
arch=('any')
url="http://hcalves.deviantart.com/art/Mustang-Vim-Colorscheme-98974484"
screenshot="http://fc03.deviantart.net/fs36/i/2008/269/1/d/Mustang_Vim_Colorscheme_by_hcalves.png"
license=('custom')
depends=('vim')
source=('mustang.vim'::"http://www.deviantart.com/download/98974484/Mustang_Vim_Colorscheme_by_hcalves.vim")
md5sums=('4ebb0d162c4e91638324dcf2fa52abfa')

package() {
  install -D -m644 "${srcdir}/mustang.vim" \
                   "${pkgdir}/usr/share/vim/vim73/colors/mustang.vim"
}

