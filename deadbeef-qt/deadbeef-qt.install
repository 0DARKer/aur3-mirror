pkgname=deadbeef-qt

post_install() {

  cat <<EOF
==> Please add blacklist_plugins gtkui to ~/.config/deadbeef/config
==> to disable default DeaDBeeF ui plugin.
EOF
}

post_upgrade() {
  post_install
}

post_remove() {

  cat <<EOF
==> Please remove blacklist_plugins gtkui to ~/.config/deadbeef/config
==> to enable default DeaDBeeF ui plugin.
EOF
}
