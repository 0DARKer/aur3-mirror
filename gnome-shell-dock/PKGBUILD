# Maintainer: Ryan Peters <sloshy45@sbcglobal.net>
pkgname=gnome-shell-dock
pkgver=0.1
pkgrel=2
pkgdesc="An unofficial dock extension for GNOME Shell"
arch=('any')
url="http://torb.blogspot.com/2010/08/dock-extension-for-gnome-shell.html"
license=('GPL')
depends=('gnome-shell')
source=(http://dl.dropbox.com/u/8599717/dock-0.1.tar.gz)
md5sums=('b5b16801893aa61974777bf319ac9e59')

build() {
  cd "$srcdir/"
  mkdir -p usr/local/share/gnome-shell/extensions
  mv "dock@torb.blogspot.com/" "usr/local/share/gnome-shell/extensions/"
  cp -R "usr/" "$pkgdir/"
}
