_pkgbasename=gstreamer0.10-good
pkgname=lib32-$_pkgbasename
pkgver=0.10.25
pkgrel=2
pkgdesc="GStreamer Multimedia Framework Good Plugins (gst-plugins-good)"
arch=(x86_64)
license=('GPL' 'LGPL')
depends=('lib32-gstreamer0.10-base' 'lib32-bzip2' 'lib32-orc')
url="http://www.gnome.org"
source=("ftp://ftp.archlinux.org/extra/os/i686/$_pkgbasename-$pkgver-$pkgrel-i686.pkg.tar.xz")
md5sums=('d6294e89e147d101a4bec8de262c486f')

build() {
  cd "$srcdir"

  install -d "$pkgdir/usr/lib32/gstreamer-0.10"
  install -Dm755 usr/lib/gstreamer-0.10/* "$pkgdir/usr/lib32/gstreamer-0.10"
}
