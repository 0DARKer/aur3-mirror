pkgname=trytond-analytic-invoice
_pkgname=trytond_analytic_invoice
pkgver=2.2.0
_pkgdir=2.2
pkgrel=1
pkgdesc="The analytic_invoice module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-account_invoice>=2.2' 'trytond-analytic_account>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("0c9707678e2a30e2201582069d9048fb")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}