# Maintainer: Vladimir Cerny <blackvladimir@gmail.com>
pkgname=asco
pkgver=0.4.8
pkgrel=1
pkgdesc="A SPICE Circuit Optimizer."
arch=('i686' 'x86_64')
url="http://asco.sourceforge.net"
license=('GPL')
groups=()
depends=(glibc)
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
source=("http://downloads.sourceforge.net/sourceforge/asco/ASCO-$pkgver.tar.gz")
noextract=()
md5sums=('e9642201ea14d2adc02992298b8ba267')

build() {
  cd "$srcdir/ASCO-$pkgver"

  make || return 1
  install -D asco "$pkgdir/usr/bin/asco" 
}

