pkgname=voracious
pkgver=2012.02.18.58470
pkgrel=13
pkgdesc='A customizable web feed aggregator for your browser.'
arch=(any)
license=(GPL)
url="http://xyne.archlinux.ca/projects/voracious"
depends=(python2 python2-feedparser python-xynehttpserver)
source=(http://xyne.archlinux.ca/projects/voracious/archives/voracious-2012.02.18.58470.tar.xz)
sha512sums=(ba39eba6a4bacd255d174b4682300a6da0b6a67211fbb2fb035e9ec51a1a7dced81fa6c5b9338e2d7626402a3c7b2424efd9ffc40005e36cfa8bf08238e9a1fe)
md5sums=(5517dbe5899487da1e860aed5f0605ff)

build () 
{ 
  install -D -m644 "$srcdir/$pkgname-$pkgver/man/$pkgname.1.gz" "$pkgdir/usr/share/man/man1/$pkgname.1.gz";
  install -D -m755 "$srcdir/$pkgname-$pkgver/$pkgname" "$pkgdir/usr/bin/$pkgname"
}


