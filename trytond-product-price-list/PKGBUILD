pkgname=trytond-product-price-list
_pkgname=trytond_product_price_list
pkgver=2.2.0
_pkgdir=2.2
pkgrel=1
pkgdesc="The product_price_list module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-product>=2.2' 'trytond-party>=2.2' 'trytond-company>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("fd6b8c359a88efaf4da694c73a53314f")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}