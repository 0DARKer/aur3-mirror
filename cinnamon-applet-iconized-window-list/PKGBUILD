pkgname=cinnamon-applet-iconized-window-list
pkgver=2.0
pkgrel=1
pkgdesc="A window list with window previews and win7 like favorites."
arch=('any')
url="http://cinnamon-spices.linuxmint.com/applets/view/16"
license=('GPL')
depends=('cinnamon')
source=("http://cinnamon-spices.linuxmint.com/uploads/applets/KWFH-NFGT-CQP2.zip")
md5sums=('dd1d49792739bd7829cddbeca86b3867')

package() {
  find WindowListGroup\@jake.phy@gmail.com/ -type f -not -name *~ -and -not -name CREDITS -and -not -name README -exec install -Dm644 '{}' "$pkgdir/usr/share/cinnamon/applets/{}" \;
}
