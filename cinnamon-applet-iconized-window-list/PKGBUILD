pkgver=2.4
pkgrel=1
pkgdesc="A window list with window previews and win7 like favorites."
arch=('any')
url="http://cinnamon-spices.linuxmint.com/applets/view/16"
license=('GPL')
groups=('cinnamon-applets')
depends=('cinnamon')
source=("http://cinnamon-spices.linuxmint.com/uploads/applets/61XC-Q48P-DA1V.zip")
md5sums=('3821d62b115b40e00136c3ae721ac0b9')

package() {
  find WindowListGroup\@jake.phy@gmail.com/ -type f -not -name *~ -and -not -name install.sh -exec install -Dm644 '{}' "$pkgdir/usr/share/cinnamon/applets/{}" \;
}
