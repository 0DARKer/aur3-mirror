pkgname=craftbukkit-plugin-multiverse-core
pkgver=2.5b641
pkgrel=1
pkgdesc="Multiple world plugin for Bukkit"
arch=(any)
url="http://dev.bukkit.org/server-mods/multiverse-core"
license=("BSD")
depends=("craftbukkit>=1.3.2r1.0-3")
source=("http://dev.bukkit.org/media/files/650/717/Multiverse-Core-2.5.jar")
noextract=("Multiverse-Core-${pkgver%b*}.jar")
md5sums=('7182e5297bc50144b3bc0ee29ef61b4c')

package() {
  install -Dm644 "$srcdir/Multiverse-Core-${pkgver%b*}.jar" "$pkgdir/srv/craftbukkit/plugins/Multiverse-Core-${pkgver%b*}.jar"
}

