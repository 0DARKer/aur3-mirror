pkgname=craftbukkit-plugin-multiverse-core
pkgver=2.3ab
pkgrel=1
pkgdesc="Multiple world plugin for Bukkit"
arch=(any)
url="http://dev.bukkit.org/server-mods/multiverse-core/"
license=('BSD')
depends=('craftbukkit')
source=("http://dev.bukkit.org/media/files/572/413/Multiverse-Core.jar")
md5sums=('bdf442ed3bc94b1783867b2e5bbece13')

package() {
  mkdir -p "$pkgdir/opt/craftbukkit/plugins"
  install -m 644 "$srcdir/Multiverse-Core.jar" "$pkgdir/opt/craftbukkit/plugins/"
}

