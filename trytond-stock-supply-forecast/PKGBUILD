# Maintainer: Robin Baumgartner <robin@baumgartners.ch>
pkgname=trytond-stock-supply-forecast
_pkgname=trytond_stock_supply_forecast
pkgver=3.2.0
_pkgdir=3.2
pkgrel=1
pkgdesc="The stock_supply_forecast module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=3.2' 'trytond-stock-supply>=3.2' 'trytond-stock-forecast>=3.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("8387350fb57046b943f6238743201698")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py build
}

package() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py install --root=$pkgdir
}