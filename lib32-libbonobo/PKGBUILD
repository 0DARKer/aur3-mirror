pkgname=lib32-libbonobo
pkgver=2.32.0
pkgrel32=1
pkgrel=1
pkgdesc="A set of language and system independant CORBA interfaces for creating reusable components"
arch=('x86_64')
url="http://www.gnome.org"
depends=('lib32-libxml2' 'lib32-orbit2')
license=('GPL' 'LGPL')
groups=lib32
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('9a32a4cd6d4bc251c6e55b8e4e0c199d')

build() {
  mkdir -p ${startdir}/pkg/opt/lib32/usr/lib
  cp -rPf ${startdir}/src/usr/lib/*.so* ${startdir}/pkg/opt/lib32/usr/lib
}
