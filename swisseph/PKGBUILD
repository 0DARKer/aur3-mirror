pkgname=swisseph
pkgver=1.77.00
pkgrel=1
pkgdesc="High precision ephemeris developed by Astrodienst, based on the DE406 ephemeris from the NASA JPL."
arch=('any')
license=('GPL')
url=("http://www.astro.com/swisseph/")
depends=()
makedepends=('wget')
optdepends=()
source=('http://arch.alwaysdata.net/swisseph.tar.gz')
md5sums=('101a6f9201e69f856a009b1be085f865')

build() {
    install -d -m 755 $pkgdir/usr/share/swisseph/
    install -m 755 $srcdir/swisseph/* $pkgdir/usr/share/swisseph/
}

