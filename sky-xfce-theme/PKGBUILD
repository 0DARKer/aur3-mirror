pkgname=sky-xfce-theme
pkgver=0.1
pkgrel=1
pkgdesc="gtk 3 theme for xfce"
url="http://golan77.deviantart.com/#/d5k51rk"
license=('GPL3')
arch=('any')
makedepends=()
options=()
source=("http://master.dl.sourceforge.net/project/archmind/$pkgname-$pkgver.tar.gz")

package() {
	cd ${srcdir}/$pkgname-$pkgver
	mkdir -p "${pkgdir}"/usr/share/themes
	cp -rf Sky "${pkgdir}"/usr/share/themes/

}

md5sums=('1cdabca412647bba0c7179f55bc5d261')
