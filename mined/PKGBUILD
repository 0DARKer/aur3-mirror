pkgname=mined
pkgver=2000.13.2
pkgrel=1
pkgdesc="a powerful text editor with good Unicode support"
url="http://towo.net/mined/"
license="GPL"
arch=(x86_64)
source=("http://towo.net/mined/$pkgname-$pkgver.tar.gz")

build() {
  cd $startdir/src/$pkgname-${pkgver}
  make install root=$startdir/pkg install
}
md5sums=('8c1741a7f15ca0330c3a43fe7df9991c')
