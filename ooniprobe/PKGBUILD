depends=('python2>=2.7' 'twisted>12.2.0' 'python2-pcapy' 'geoip-asndata'
                        'geoip-database' 'python2-yaml' 'python2-ipaddr>2.1.10' 'python2-pyopenssl>=0.13' 'python2-geoip' 'txtorcon>=0.7' 'txsocksx-git>=0.0.2' 'scapy')
