pkgname=trytond-project-plan
_pkgname=trytond_project_plan
pkgver=2.2.0
_pkgdir=2.2
pkgrel=1
pkgdesc="The project_plan module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-company>=2.2' 'trytond-project>=2.2' 'trytond-timesheet>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("e143116dca0087eb09a893f7bd4c733e")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}