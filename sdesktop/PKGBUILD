# Contributor: Tuxce <tuxce.net@gmail.com>
pkgname=sdesktop
pkgver=0.3
pkgrel=1
pkgdesc="Switch desktop with mouse wheel"
arch=(x86_64 i686)
url="http://tuxce.selfip.org/cgit/public/sdesktop/"
license=('GPL')
depends=(libx11)
makedepends=(xproto)
source=(http://tuxce.selfip.org/releases/$pkgname/$pkgname-$pkgver.tar.gz)

build() {
  cd "$srcdir/$pkgname-$pkgver"

  make DESTDIR="$pkgdir/" install
}

# vim:set ts=2 sw=2 et:
md5sums=('6c3da32da6b6a4c8f9b35b83fdf03371')
