# Contributor: Tuxce <tuxce.net@gmail.com>
pkgname=sdesktop
pkgver=0.4
pkgrel=2
pkgdesc="Switch desktop with mouse wheel"
arch=(x86_64 i686)
url="http://tuxce.selfip.org/cgit/public/sdesktop/"
license=('GPL')
depends=(libx11)
makedepends=(xproto)
source=(http://tuxce.no-ip.org/releases/$pkgname/$pkgname-$pkgver.tar.gz)

build() {
  cd "$srcdir/$pkgname-$pkgver"

  make DESTDIR="$pkgdir/" install
}

# vim:set ts=2 sw=2 et:
md5sums=('612cdbe018da2e983f0c57aea8145782')
