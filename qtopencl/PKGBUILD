pkgname=qtopencl
pkgver=1
pkgrel=0.0
pkgdesc="QT-based OpenCL framework"
url="http://mipt.ru"
license=('GPL')
arch=(i686)
depends=(libcl)
makedepends=(bzip2 gzip tar xz)
source=(http://dl.dropbox.com/u/4583032/mipt/qtopencl-$pkgver.$pkgrel.tar.xz)
md5sums=('937444ce3b0e12d281f8395cbf760835')




build() {
	cd $startdir/src
	for i in *
		do
			[ -d $i ] && cp -Rp $i $startdir/pkg/
		done
}


