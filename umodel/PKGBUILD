pkgname=umodel
pkgver=20130326
pkgrel=1
pkgdesc='Unreal engine resource viewer AKA UE Viewer'
arch=('i686' 'x86_64')
url='http://www.gildor.org/en/projects/umodel'
license=('unknown')
source=("http://www.gildor.org/down/37/${pkgname}/${pkgname}_linux.tar.gz")
md5sums=('ef1c0f1797c3f2a3c8052770fd632d9a')

package() {
    cd "$srcdir"

    install -d -m755 "${pkgdir}/usr/bin"
    install -m755 umodel "${pkgdir}/usr/bin/"
}
