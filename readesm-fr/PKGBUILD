      # Maintainer: lemust83 <lemust83420@gmail.com>
        pkgname=readesm-fr
        pkgver=2
        pkgrel=2
        pkgdesc="lire les fichiers Tachographs issus de Cardpeek ou autres  dans un format humainement compr√©hensible"
        arch=(i686 x86_64)
        url="http://readesm.sourceforge.net/help.html"
        license=('GPL')
        groups=()
        depends=('boost' 'gmp' 'libgcrypt' 'qt4' 'qtwebkit' 'python2')
        makedepends=('cmake' 'subversion')
        replaces=(readesm)
        source=(http://switch.dl.sourceforge.net/project/readesmfr/readesm-fr/readesm-fr.tar.gz)
        noextract=
        md5sums=('40601b5addda95e8334f158f5400a69a')




        build() {
          cd $srcdir/readesm-fr/fileformat
          python2 ./generate.py
          cd ..
          mkdir build
          cd build
          cmake ..
          make
         }

        package ()
       {
        cd "$srcdir/$pkgname/build"
        make DESTDIR="$pkgdir" install
       }

