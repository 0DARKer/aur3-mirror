pkgname=lib32-cmt
pkgname32=cmt
pkgver=1.16
pkgrel=1
pkgrel32=2
pkgdesc="CMT for LADSPA"
arch=('x86_64')
url="http://www.ladspa.org/"
license=('GPL')
depends=('lib32-gcc>=4.0.2' 'ladspa' 'lib32-ladspa')
source=("ftp://ftp.archlinux.org/extra/os/i686/$pkgname32-$pkgver-$pkgrel32-i686.pkg.tar.gz")
md5sums=('28bbfa7dd9e63e7e0e9e59265eb10f85')

build() {
  install -Dm755 $startdir/src/usr/lib/ladspa/cmt.so $startdir/pkg/usr/lib/ladspa/lib32-cmt.so
}

