pkgname=trytond-company
_pkgname=trytond_company
pkgver=2.2.1
_pkgdir=2.2
pkgrel=1
pkgdesc="The company module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.2' 'trytond-party>=2.2' 'trytond-currency>=2.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("cdbef5828fce8404ab2c201d34a0765a")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}