# Maintainer: Damien Churchill <damoxc@gmail.com>
pkgname=connect
pkgver=1.101
pkgrel=1
pkgdesc="makes socket connection using SOCKS4/5 and HTTP tunnel."
arch=('i686' 'x86_64')
url="https://bitbucket.org/gotoh/connect"
license=('GPL')
depends=()
makedepends=()
source=(https://bitbucket.org/gotoh/connect/get/${pkgver}.tar.bz2)
sha256sums=('99ece53518fd3c9aa8c9a1bf6700e9b36ba19b0dc11779f857a3924933756b26')

build() {
  cd "${srcdir}/${pkgname}"
  make
}

package() {
  cd "${srcdir}/${pkgname}"
  mkdir -p ${pkgdir}/usr/bin
  cp connect ${pkgdir}/usr/bin
}
