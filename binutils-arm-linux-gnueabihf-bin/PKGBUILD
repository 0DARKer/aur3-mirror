pkgname=binutils-arm-linux-gnueabihf-bin
pkgver=2.23.2
pkgrel=1
pkgdesc="GNU binary utilities, for arm-linux-gnueabihf target"
arch=('i686' 'x86_64')
url="http://packages.ubuntu.com/binutils-arm-linux-gnueabihf"
license=('GPL2')
provides=('binutils-arm-linux-gnueabihf')
conflicts=('binutils-arm-linux-gnueabihf')
if [ "$CARCH" = "i686" ]; then
	source=("http://archive.ubuntu.com/ubuntu/pool/universe/b/binutils-armhf-cross/binutils-arm-linux-gnueabihf_2.23.2-1ubuntu2cross1.92_i386.deb")
	md5sums=('9677e2bfb7242075419e9729e5aa888d')
elif [ "$CARCH" = "x86_64" ]; then
	source=("http://archive.ubuntu.com/ubuntu/pool/universe/b/binutils-armhf-cross/binutils-arm-linux-gnueabihf_2.23.2-1ubuntu2cross1.92_amd64.deb")
	md5sums=('eedd8a3e186eb8155ced536940c13ff2')
fi

package() {
	cd $pkgdir
	tar xf $srcdir/data.tar.gz
}
