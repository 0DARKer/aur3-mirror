pkgname=mint-x-icons
pkgver=1.1.1
pkgrel=1
pkgdesc="Mint-X Icon themes"
arch=('any')
url="http://packages.linuxmint.com/pool/main/m/mint-x-icons"
license=('GPL')
depends=()
optdepends=("mint-x-theme")
source=("${url}/${pkgname}_${pkgver}.tar.gz")
md5sums=('cd73664ca2d176b0e2cebfd11f57c4e0')

build() {
 cd "$srcdir/${pkgname}"
 cp -R usr $pkgdir/
}

