# Special thanks also to Brett, AKA "KittyKatt" for hosting a repository using my packages at http://repo.kittykatt.tk/

pkgname=mint-x-icons
pkgver=1.0.4
pkgrel=2
pkgdesc="Linux Mint 10's new icon theme."
arch=('any')
url="http://packages.linuxmint.com/pool/main/m/mint-x-icons"
license=('GPL')
depends=()
optdepends=("mint-x-theme: Linux Mint 10's new theme")
source=("${url}/${pkgname}_${pkgver}.tar.gz")
md5sums=('20dc1209b1e9a9329fe81d1b248c14b6')

build() {
  cd "$srcdir/${pkgname}"
  cp -R usr $pkgdir/
}
# vim:syntax=sh
