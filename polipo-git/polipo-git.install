_pkgname=polipo

post_install() {
  if [[ ! -f etc/$_pkgname/config ]]; then
    echo "Example configuration file is located in \`/usr/share/$_pkgname/config.sample'"
    echo "Copy the file to \`/etc/$_pkgname/config'"
    echo "If a configuration file is not created, $_pkgname will use its defaults"
  fi
}
