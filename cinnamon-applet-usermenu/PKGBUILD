# Maintainer: Ner0

pkgname=cinnamon-applet-usermenu
pkgver=2.0.3
pkgrel=1
pkgdesc="An indicator applet that adds the User menu options"
arch=('any')
url="http://cinnamon-spices.linuxmint.com/applets/view/45"
license=('GPL')
depends=('cinnamon')
source=("http://cinnamon-spices.linuxmint.com/uploads/applets/F8LT-2ZOT-M8FB.zip")
md5sums=('2dec8462650006115b338aac0f17e3b0')

package() {
  find UserMenuV2\@bownz -type f -exec install -Dm644 '{}' "$pkgdir/usr/share/cinnamon/applets/{}" \;
}
