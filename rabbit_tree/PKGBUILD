#Maintainer: Xyne <ac xunilhcra enyx, backwards>
pkgname=rabbit_tree
pkgver=2012.10
pkgrel=1
pkgdesc='Radix bit tries for implementing associative arrays and sets in C.'
arch=(any)
license=(GPL)
url="http://xyne.archlinux.ca/projects/rabbit_tree"
makedepends=(cmake doxygen graphviz)
source=(
  http://xyne.archlinux.ca/projects/rabbit_tree/src/rabbit_tree-2012.10.tar.xz
  http://xyne.archlinux.ca/projects/rabbit_tree/src/rabbit_tree-2012.10.tar.xz.sig
)
sha512sums=(
  85c6ab842a2cf4ea5f94b950fa149fab25861c680df744f948dcdbafb1972855f10a88b8ed7ef128a82149e813d5520cf3375e1c4e62f6b47d5debf99afba040
  a9fd04f452e02c303c0cc09e43f3c8778e9a91f7bde1ca6bf1611f23141428d5e545481b0028accc8a32e56c3cf17c9b5a22c652d6cde9fac857e0848bf81cd7
)
md5sums=(
  598862335a50cc768fc512f684f19446
  2f409bdf58d1ebf17be733e5e3395f53
)

build ()
{
  mkdir -p "$srcdir/build"
  cd -- "$srcdir/build"
  cmake ../"$pkgname-$pkgver" -DCMAKE_INSTALL_PREFIX=/usr
  make
}

package()
{
  cd -- "$srcdir/build"
  make install DESTDIR="$pkgdir"
}

