#Maintainer: Xyne <ac xunilhcra enyx, backwards>
pkgname=rabbit_tree
pkgver=2013.1
pkgrel=1
pkgdesc='Radix bit tries for implementing associative arrays and sets in C.'
arch=(any)
license=(GPL)
url="http://xyne.archlinux.ca/projects/rabbit_tree"
makedepends=(cmake doxygen graphviz)
source=(
  http://xyne.archlinux.ca/projects/rabbit_tree/src/rabbit_tree-2013.1.tar.xz
  http://xyne.archlinux.ca/projects/rabbit_tree/src/rabbit_tree-2013.1.tar.xz.sig
)
sha512sums=(
  9537851d47d9050bbc399f83d949e2f7789fb4a5bc13db54f555ae0cc7078c7af3a241efa178d1d2396aa3d046d7f4493942c45559e92187a2b8678b677404d7
  4cd0755fbfe8d2f8c93b016d90968158f76a50a805b57540637b4f012d97f3cd89b0992e1d66fa232432e439f7325c1b9d04de0778a9ab8d76422b835cbb7d70
)
md5sums=(
  80b87bb563d7121a34497b6d4f12738f
  cff024d6d532ac3215281f1e7bb8c331
)

build ()
{
  mkdir -p "$srcdir/build"
  cd -- "$srcdir/build"
  cmake ../"$pkgname-$pkgver" -DCMAKE_INSTALL_PREFIX=/usr
  make
}

package()
{
  cd -- "$srcdir/build"
  make install DESTDIR="$pkgdir"
}

