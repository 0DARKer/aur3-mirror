pkgname=python-everything
pkgver=1.0
pkgrel=1
pkgdesc="Includes all of the Python packages in the official repos"
arch=(x86_64 i686)
url="https://github.com/EvanTeitelman/pkgbuilds"
license=('custom')

package() {
  depends=($(pacman -Ssq | grep '^python-'))
}
