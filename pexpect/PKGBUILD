# Maintainer: Samir Faci <csgeek at archlinux dot us> 
_name=pexpect
pkgname=${_name}
pkgver=2.3
pkgrel=1
pkgdesc="Pexpect is a pure Python module that makes Python a better tool for controlling and automating other programs. "
arch=('i686' 'x86_64')
url="http://sourceforge.net/projects/pexpect"
##GPL?  I guess.
license=('GPL')
depends=('python2' )
source=(http://pexpect.sourceforge.net/pexpect-2.3.tar.gz)
md5sums=('bf107cf54e67bc6dec5bea1f3e6a65c3')

build() 
{
  cd ${srcdir}/${pkgname}-${pkgver}
  python2 setup.py install --root=$pkgdir/ --optimize=1 || return 1
}

