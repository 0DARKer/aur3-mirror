pkgname=acxi
pkgver=2.6.1
pkgrel=1
pkgdesc="audio conversion script"
url="http://smxi.org/opt/acxi"
license="GPL"
arch=('any')
depends=('vorbis-tools' 'flac' 'lame')
source=(http://smxi.org/opt/acxi)
md5sums=('5d6f0f7cc5d23bdce6bc0e9ae8ac1945')
build() {
    cd "$startdir/src"
  
    wget smxi.org/acxi
    
    install -m 755 -D acxi "$startdir/pkg/usr/bin/acxi"
}

