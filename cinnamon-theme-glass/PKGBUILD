# Maintainer: OmeGa <omega at mailoo dot org>

pkgname=cinnamon-theme-glass
pkgver=60
pkgrel=1
pkgdesc="Glass Elementary, Mint & Ubuntu themes for Cinnamon."
arch=('any')
url="http://cinnamon-spices.linuxmint.com/themes/view/128"
license=('unknown')
depends=('cinnamon')
source=(http://cinnamon-spices.linuxmint.com/uploads/themes/1K23-62SH-ODGA.zip)
sha1sums=('0153207ac23ed864bb14bfa808cfd2deabd473a0')

package() {
  find Glass* -type f -exec install -Dm644 '{}' "$pkgdir/usr/share/themes/{}" \;
}

