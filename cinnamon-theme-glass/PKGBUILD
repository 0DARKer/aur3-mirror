pkgname=cinnamon-theme-glass
pkgver=62
pkgrel=1
pkgdesc="Glass Elementary, Mint & Ubuntu themes for Cinnamon."
arch=('any')
url="http://cinnamon-spices.linuxmint.com/themes/view/128"
license=('unknown')
depends=('cinnamon')
source=(http://cinnamon-spices.linuxmint.com/uploads/themes/C73Z-IED4-DPBU.zip)
sha1sums=('717d4ddb328f9c769c74b9a5392b52faa3d90a25')

package() {
  find Glass* -type f -exec install -Dm644 '{}' "${pkgdir}/usr/share/themes/{}" \;
}

