pkgname=buildpae
pkgver=2010.12.11
pkgrel=1
pkgdesc="BASH script which builds kernel26-pae and kernel26-pae-headers packages by modifying stock kernel26 files from ABS."
arch=('any')
license=('GPL')
url="http://www2.wizetek.com"
source=(${pkgname})
md5sums=('054600fcca2470f56f3fb1a22514ac86')

package() {
  install -D -m 755 ${pkgname} ${pkgdir}/usr/bin/${pkgname}
}
