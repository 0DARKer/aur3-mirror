# Maintainer: Thibault Suzanne <>
pkgname=beamer-theme-supelec  
pkgver=1.0
pkgrel=1 
pkgdesc="Supelec theme for beamer"
url="http://wwwdi.supelec.fr/fb/download/LaTeX/beamersupelec/tex/latex/splcbeam/"
arch=("any")
license=("unknown")
depends=("texlive-core")
makedepends=("unzip")
source=("http://wwwdi.supelec.fr/fb/download/LaTeX/beamersupelec/splcbeam-1.0.zip")
md5sums=('3227bde86fa27803ac0e22cb5d929900')

build() {
  true
}

package() {
  cd "$srcdir/beamersupelec/tex/latex/splcbeam"
  install -Dm644 beamercolorthemesupelec.sty $pkgdir/usr/share/texmf-dist/tex/latex/beamer/themes/color/beamercolorthemesupelec.sty
  install -Dm644 beamerfontthemesupelec.sty $pkgdir/usr/share/texmf-dist/tex/latex/beamer/themes/color/beamerfontthemesupelec.sty
  install -Dm644 beamerinnerthemesupelec.sty $pkgdir/usr/share/texmf-dist/tex/latex/beamer/themes/color/beamerinnerthemesupelec.sty
  install -Dm644 beamerouterthemesupelec.sty $pkgdir/usr/share/texmf-dist/tex/latex/beamer/themes/color/beamerouterthemesupelec.sty
  install -Dm644 beamerthemesupelec.sty $pkgdir/usr/share/texmf-dist/tex/latex/beamer/themes/color/beamerthemesupelec.sty
  install -Dm644 beamerthemesupeleclogo.pdf $pkgdir/usr/share/texmf-dist/tex/latex/beamer/themes/color/beamerthemesupeleclogo.sty
  install -Dm644 beamerthemesupeleclogo.eps $pkgdir/usr/share/texmf-dist/tex/latex/beamer/themes/color/beamerthemesupeleclogo.sty
  
}
