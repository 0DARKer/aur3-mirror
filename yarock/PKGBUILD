# Maintainer: Ko≈°ava <kosava@archlinux.us>
pkgname=yarock
pkgver=0.0.41
pkgrel=1
pkgdesc="Yarock is Qt4 Modern Music Player with collection browser based on cover art."
arch=('i686' 'x86_64')
url="http://qt-apps.org/content/show.php?content=129372"
license=('GPL3')
depends=('qt' 'taglib' 'phonon')
source=(http://qt-apps.org/CONTENT/content-files/129372-Yarock_0.0.41_source.tar.gz yarock.desktop)
md5sums=('8a7bde8ff5bc111d4c1fa24750888c7a'
         '421d8a602ab1674568e5f553c6fc2694')

build() {
   cd $srcdir/Yarock_${pkgver}
   qmake PREFIX="/usr/"
   make INSTALL_ROOT=$pkgdir install
}