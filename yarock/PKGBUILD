# Maintainer: Ko≈°ava <kosava@archlinux.us>
pkgname=yarock
pkgver=0.0.59
pkgrel=1
pkgdesc="Yarock is Qt4 Modern Music Player with collection browser based on cover art."
arch=('i686' 'x86_64')
url="https://launchpad.net/yarock"
license=('GPL3')
depends=('qt' 'taglib' 'phonon' 'boost')
source=(https://launchpad.net/yarock/trunk/0.0.59/+download/Yarock_0.0.59_source.tar.gz yarock.desktop)
md5sums=('ff6b950b1a83280e5965049eeb606a10'
         '421d8a602ab1674568e5f553c6fc2694')

build() {
   cd $srcdir/Yarock_0.0.59_source
   qmake PREFIX="/usr/"
   make INSTALL_ROOT=$pkgdir install
}