# Archlive <http://archlive-pkg.googlecode.com>

pkgname=('kernel-devel')
pkgver=2.6.31
pkgrel=23.fc12
url="http://www.kernel.org/"
pkgdesc="The Linux Kernel Sources, for Fedora kernel. (Memory <= 4GB)"
arch=('i686' 'x86_64')
license=('GPL2')
depends=('kernel26-fedora')
makedepends=('rpmextract')
backup=()
provides=("kernel-devel=$pkgver")
conflicts=('kernel24' 'kernel24-scsi' 'kernel26-scsi' 'kernel26' 'kernel-devel-pae')
source=(http://kojipkgs.fedoraproject.org/packages/kernel/${pkgver}/${pkgrel}/${CARCH}/kernel-devel-${pkgver}-${pkgrel}.${CARCH}.rpm
	)
md5sums=()

build()
{
	cd $pkgdir
	rpmextract.sh $srcdir/kernel-devel-${pkgver}-${pkgrel}.${CARCH}.rpm
}
