# Maintainer: David Runge <david[d]runge[a]frqrec[d]com>
pkgname=systemd-dropbox-daemon
pkgver=0.1
pkgrel=2
pkgdesc="Systemd service file for Dropbox Daemon as a user"
arch=('any')
url="http://www.freedesktop.org/wiki/Software/systemd"
license=('GPL3')
depends=('dropbox-daemon' 'systemd')
install=$pkgname.install
source=('dropboxd.service')
md5sums=('99549ab7a142328490b88e8ca3d6e5c9')

build() {
	install -vDm644 ${srcdir}/dropboxd.service ${pkgdir}/usr/lib/systemd/system/dropboxd.service
}
