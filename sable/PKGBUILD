pkgrel=1
pkgdesc="A Black Tone Theme"
arch=('any')
url="http://fav.me/d77o811"
license=('GPLv3')
depends=('gtk-engines' 'gnome-themes-standard>=3.12.0' 'gtk-engine-murrine>=0.98.2')

#replaces=('sable')
#conflicts=('sable')
source=("https://dl.dropboxusercontent.com/u/330352/sable-3.12_01.09132014.zip")

