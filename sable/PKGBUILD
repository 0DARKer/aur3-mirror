pkgname=sable
pkgver=3.14.7_01222015
pkgrel=1
pkgdesc="A flat green and blue themes with patterened background"
arch=('any')
url="http://fav.me/d8eg4n0"
license=('GPLv3')
depends=('gtk-engines' 'gnome-themes-standard>=3.14' 'gtk-engine-murrine>=0.98.2')

source=("https://dl.dropboxusercontent.com/u/330352/sable-3.14.7_01222-15.zip")
#install=$pkgname.install
#replaces=('delorean-dark-themes-3.8')
#conflicts=('delorean-dark-theme-3.8')

md5sums=('113ca1116576e7978ee12e5a1b79b0e6')


package() {
  # install themes
  
  #cd DORIAN

  find Sable-3.14/ -type f \
      -exec install -Dm644 "{}" "$pkgdir/usr/share/themes/{}" \;
  find Sable-Blue-3.14/ -type f \
      -exec install -Dm644 "{}" "$pkgdir/usr/share/themes/{}" \;           
}

