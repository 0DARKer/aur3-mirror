pkgname=sable
pkgver=3.14.2_01192015
pkgrel=1
pkgdesc="A flat green and black theme with patterened background for gtk-3.14"
arch=('any')
url="http://fav.me/d8eg4n0"
license=('GPLv3')
depends=('gtk-engines' 'gnome-themes-standard>=3.14.0' 'gtk-engine-murrine>=0.98.2')

source=("https://www.dropbox.com/s/s1zbzfx1nga139m/sable-3.14.2_01192015.zip")


md5sums=('462f6ca4289fdc9666574d6270f9efb0')


package() {
  # install themes

  find "sable"/ -type f \
      -exec install -Dm644 "{}" "$pkgdir/usr/share/themes/{}" \;           
}

