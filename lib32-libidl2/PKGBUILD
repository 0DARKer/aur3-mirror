pkgname=lib32-libidl2
pkgver=0.8.14
pkgrel32=1
pkgrel=1
pkgdesc="A front-end for CORBA 2.2 IDL and Netscape's XPIDL"
url="http://www.gnome.org"
arch=('x86_64')
license=('LGPL' 'GPL')
groups=('lib32')
depends=('lib32-glib2')
source=(http://www.gtlib.gatech.edu/pub/linux/distributions/archlinux/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('2589d14436585ed37941c1a59a22b986')

build() {
mkdir -p $startdir/pkg/opt/lib32/usr/lib
cp -R $startdir/src/usr/lib/*.so* $startdir/pkg/opt/lib32/usr/lib
}
