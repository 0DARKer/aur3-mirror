pkgname=cppreference-qt
pkgver=20121202
pkgrel=1

pkgdesc="A complete reference for the features in the C++ Standard Library, for qt help"
arch=('any')
url="http://en.cppreference.com/"
license=('CCPL:cc-by-sa')
source=("http://upload.cppreference.com/mwiki/images/2/22/qch_book_20121202.tar.gz")
md5sums=('38b814067358a9173f058bbf25d6e30d')

package() {
    mkdir -p $pkgdir/usr/share/doc/qt
    cp $srcdir/*.qch $pkgdir/usr/share/doc/qt
}