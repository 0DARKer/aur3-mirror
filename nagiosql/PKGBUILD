license=('BSD')
groups=()
## requirements - http://www.nagiosql.org/about-nagiosql.html
depends=('php' 'mysql' 'nagios')
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
source=("http://downloads.sourceforge.net/project/nagiosql/nagiosql/NagiosQL%203.0.3/nagiosql303.tar.gz")
noextract=()
md5sums=('3b15650942cf0fea3b6bbec1700ace38')

build() {
  _base="$pkgdir/srv/http"
  mkdir -p  $_base
  mv "$srcdir/${pkgname}3" $_base/
}

