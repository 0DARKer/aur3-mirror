pkgname=tunlr
pkgver=0.0.1
pkgrel=1
pkgdesc="start and stop tunlr"
arch=('any')
url="http://tunlr.net/"
license=('GPL')
depends=('bash' 'coreutils')
source=($pkgname)
md5sums=('6e8bcad4356aa0dcf2e8efaf86aad0a4')

build() {
    install -Dm 755 $pkgname $pkgdir/usr/bin/$pkgname
}
