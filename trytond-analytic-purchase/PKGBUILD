_pkgname=trytond_analytic_purchase
pkgver=2.4.1
_pkgdir=2.4
pkgrel=2
pkgdesc="The analytic_purchase module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.4' 'trytond-purchase>=2.4' 'trytond-analytic_account>=2.4' 'trytond-analytic_invoice>=2.4')
