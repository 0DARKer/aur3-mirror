  7ea06e61ef20f28c82f869d8f0c30cfd5f54fb4578a25f6305ed1ad2a7af32c06bab0c2bd2d2f3c1248ecf4574aff1771747fae01b527388d6328c769383f1d5
)
md5sums=(
  20861018f234086cf878580ee83250d8
  e3facf56cfb2155fa19d66be6837bdd7
)

build () 
{ 
  install -D -m755 "$srcdir/$pkgname-$pkgver/$pkgname" "$pkgdir/usr/bin/$pkgname";
  install -D -m644 "$srcdir/$pkgname-$pkgver/man/$pkgname.1.gz" "$pkgdir/usr/share/man/man1/$pkgname.1.gz"
}


