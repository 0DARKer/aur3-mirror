depends=(python3 subversion)
source=(
  http://xyne.archlinux.ca/projects/svn-export/src/svn-export-2013.1.25.tar.xz
  http://xyne.archlinux.ca/projects/svn-export/src/svn-export-2013.1.25.tar.xz.sig
)
sha512sums=(
