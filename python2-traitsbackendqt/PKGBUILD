# Maintainer: Andrzej Giniewicz <gginiu@gmail.com>
pkgname=python2-traitsbackendqt
pkgver=3.6.0
_githubtag=79eb7e4
pkgrel=1
pkgdesc="PyQt backend for Traits and TraitsGUI"
arch=('any')
url="https://github.com/enthought/traitsbackendqt"
license=('BSD')
depends=('python2' 'python2-qt>=4' 'python2-traits' 'python2-enthoughtbase')
makedepends=('python2-distribute')
provides=('python2-traitsbackend')
conflicts=('python-enthought-traits' 'python2-traitsbackendqt-git' 'python-ets-traits-qt-svn')
options=(!emptydirs)

source=("https://github.com/enthought/traitsbackendqt/tarball/${pkgver}")
md5sums=('9b127fb1626796ca2383fe2714ce0c78')

build() {
  cd "$srcdir/enthought-traitsbackendqt-${_githubtag}"

  python2 setup.py install --root="$pkgdir"/ --optimize=1 || return 1
}

