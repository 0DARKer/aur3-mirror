pkgname=lotw
pkgver=20121225
pkgrel=1
pkgdesc="The unofficial LOTW users list"
arch=('any')
url="http://www.hb9bza.net/lotw-users-list"
license=('CCPL:by')
source=(http://hams.co.ua/download/${pkgname}_${pkgver}.txt.gz)

package() {
	cd "$srcdir"

	install -Dm644 "$srcdir"/${pkgname}_${pkgver}.txt "$pkgdir"/usr/share/${pkgname}/${pkgname}.txt
}

md5sums=('9342b9ea2bfc39efa86f171264aa0cbc')
sha256sums=('5cb329aa4529036ec0fa8871beb9eeeab847e049f2b131f531e9e6b7883ed237')
