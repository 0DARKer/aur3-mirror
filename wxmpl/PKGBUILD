pkgname=wxmpl
pkgver=1.2.9
pkgrel=1
pkgdesc="Painless matplolib embedding for wxPython"
arch=("i686")
url="http://agni.phys.iit.edu/~kmcivor/wxmpl/"
license=('GPL')
depends=('python' 'python-matplotlib')
source=(http://agni.phys.iit.edu/~kmcivor/wxmpl/downloads/$pkgname-$pkgver.tar.gz)
md5sums=('59e6277069b4783868a265dfb485fcc5')

build() {
    cd "$srcdir/$pkgname-$pkgver"
    python setup.py install --root=$pkgdir
}
