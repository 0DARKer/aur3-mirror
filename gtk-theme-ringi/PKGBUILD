pkgname=gtk-theme-ringi
pkgver=20111109
pkgrel=2
pkgdesc="Dark brown GTK3 and metacity theme" 
url="http://ubuntuforums.org/showpost.php?p=11441277&postcount=232"
license=('unknown')
arch=('any')
source=("http://dl.dropbox.com/u/13022065/Ringi.tar.gz")
md5sums=('7a95081ba42fb8e5e02e230dc14278cf')

build() {
  mkdir -p ${pkgdir}/usr/share/themes/
  mv ${srcdir}/Ringi ${pkgdir}/usr/share/themes
}


