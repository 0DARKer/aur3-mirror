pkgname=wxd
pkgver=0.15
pkgrel=1
pkgdesc="D binding for wxWidgets"
arch=('i686' 'x86_64')
url="http://wxd.sourceforge.net/"
license=('LGPL')
depends=('wxgtk' 'dmd' 'libphobos')
source=("http://downloads.sourceforge.net/project/wxd/wxD/0.15/wxd-0.15.tgz")
md5sums=('977774a912fec079cdcf873e317cdde8')

build() {
	cd ${srcdir}/wxd
	make prefix=/usr DESTDIR=${pkgdir} install
}
