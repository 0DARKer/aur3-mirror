# Contributer: said

pkgname=jcatgui
pkgver=b20110830
pkgrel=1
pkgdesc='An application written to solve jobs or rather to earn credits on captcha.trader in more comfortable way than in a browser.'
arch=('any')
url='http://sites.google.com/site/${pkgname}/'
license=('?		')
depends=('java-runtime')
source=('http://goo.gl/Xdkb4'
        'jcatgui.desktop'
	'jcatgui.png')

md5sums=('30499a4a266bf2b3deef82c219267b2c'
         'e660ae51ac20e9eef44e37abdf91f878'
         '4a6038e2a0e89bb608681eac7e94cd11')


package() {
  install -Dm644 ${srcdir}/Xdkb4 ${pkgdir}/usr/share/${pkgname}/jCatGUI.jar
  install -Dm644 ${srcdir}/${pkgname}.desktop ${pkgdir}/usr/share/applications/${pkgname}.desktop
  install -Dm644 ${srcdir}/${pkgname}.png ${pkgdir}/usr/share/pixmaps/${pkgname}.png
} 
