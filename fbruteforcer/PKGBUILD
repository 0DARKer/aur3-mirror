pkgname=fbruteforcer
pkgver=1.0
pkgrel=1
pkgdesc="Facebook bruteforcer tools"
arch=(any)
url="https://gunslingerc0de.wordpress.com/"
license="GPL"
depends=('python-mechanize' 'python2')

source=(http://dl.packetstormsecurity.net/Crackers/fbruteforcer.py.txt
	patch)
md5sums=('c1a881c74c55ae82b40e646268cab519' '60983a45655580414ec6662e2ea6b307') 

build() {
  

  cd $srcdir/
  patch -Np0 -i patch
  chmod +x fbruteforcer.py.txt
  mkdir -p $pkgdir/usr/bin
  mv fbruteforcer.py.txt $pkgdir/usr/bin/fbruteforcer.py
  
}
