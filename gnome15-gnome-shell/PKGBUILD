# Maintainer: Nuno Araujo <nuno.araujo@russo79.com>
pkgname=gnome15-gnome-shell
pkgver=0.0.1
pkgrel=1
pkgdesc="Gnome Shell plugin for Gnome15"
arch=(any)
url="http://www.gnome15.org"
license=('GPL')
depends=('gnome15' 'gnome-shell')
makedepends=('binutils')
install=$pkgname.install
source=("http://www.gnome15.org/quantal/all/${pkgname}_${pkgver}-1_all.deb")
md5sums=('d0b5c64a25330310bdc5dee188da0c64')

build() {
  tar xf data.tar.gz
}

package() {
  cd $srcdir
  cp -R usr "$pkgdir/"
}

