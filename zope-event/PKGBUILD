pkgname=zope-event
pkgver=3.5.0
pkgrel=1
pkgdesc="Zope Event Publication"
arch=('i686' 'x86_64')
url="http://cheeseshop.python.org/pypi/zope.event/"
license=('ZPL')
depends=('python2')
makedepends=('python2-distribute')
source=(http://pypi.python.org/packages/source/z/zope.event/zope.event-$pkgver-1.tar.gz)
md5sums=('5940673553add7a75ec47d3b916ba7d1')

build() {
  cd ${srcdir}/zope.event-$pkgver-1
  python2 setup.py install --root=${pkgdir} --prefix=/usr || return 1
}
