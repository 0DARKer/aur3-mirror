pkgname=kdm-theme-steampunk
pkgver=3.0
pkgrel=1
pkgdesc="This item is a part of Steam-Powered Linux KDE theme set."
arch=('i686' 'x86_64')
license=('GPL')
url="http://kde-look.org/content/show.php?content=142139"
depends=('kdebase-workspace')
source=(http://sites.google.com/site/binaryinspiration/download/SPL_KDM.tar.gz)
md5sums=('638c2442e2912de35982552ce0f3147c')

build()
{
 cd $startdir/src
 mkdir -p $startdir/pkg/kdm-theme-steampunk/usr/share/apps/kdm/themes/
 cp -rf SteampunK  $startdir/pkg/kdm-theme-steampunk/usr/share/apps/kdm/themes/
}
