pkgname=ibus-table-erbi-chaoqiang
pkgver=090726
pkgrel=1
pkgdesc="The Chaoqiang Erbi Input Method of tables engines for IBus."
arch=('i686' 'x86_64')
url="http://ibus-table-erbi-chaoqiang.googlecode.com/"
license=('LGPL')
depends=('ibus-table=1.2.0')
provides=('ibus-table-chaoqiangerbi=090726')
conflicts=()
makedepends=('')
options=('!libtool')
