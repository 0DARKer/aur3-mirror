# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-nautilus
pkgver=1.0.32
pkgrel=1
pkgdesc="Nautilus integration for insync"
url="https://support.insynchq.com/forums/discussion/3270/new-insync-version-1-0-32"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "nautilus" "python2-nautilus")
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}.31767-precise_all.deb")
sha256sums=("7697501aa61ff394574ee7735c8443ad3dd341e0324b8e432c1c162032d96324")
noextract=("${pkgname}-${pkgver}.deb")

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
