# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-nautilus
pkgver=1.0.25
pkgrel=1
pkgdesc="Nautilus integration for insync"
url="https://support.insynchq.com/forums/discussion/2416/new-insync-version-1-0-25"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "nautilus" "python2-nautilus")
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}_all.deb")
sha256sums=("7197cf4f1064dafade95ea06b48e8eeef87f210b642aae0160e33695bfc72692")
noextract=("${pkgname}-${pkgver}.deb")

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
