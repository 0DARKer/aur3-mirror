# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-nautilus
pkgver=1.1.4
pkgrel=1
pkgdesc="Nautilus integration for insync"
url="https://support.insynchq.com/forums/discussion/3479/new-insync-version-1-1-4"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "nautilus" "python2-nautilus")
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}.32046-precise_all.deb")
sha256sums=("92a3fd016bf9941bc355fecd25f5be4817908d02e95e1bf3983b65419d596fd2")
noextract=("${pkgname}-${pkgver}.deb")

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
