# Maintainer: Zhengyu Xu <xzy3186@gmail.com>

pkgname=insync-nautilus
pkgver=1.0.28
pkgrel=1
pkgdesc="Nautilus integration for insync"
url="https://support.insynchq.com/forums/discussion/2699/new-insync-version-1-0-28"
license=('custom:insync')
options=(!strip)
arch=('any')

depends=("insync" "nautilus" "python2-nautilus")
source=("${pkgname}-${pkgver}.deb::http://s.insynchq.com/builds/${pkgname}_${pkgver}.31731_all.deb")
sha256sums=("400dcc7f171b562307addfc2accc36cf55e520525b6a61be52ac1e97a74324de")
noextract=("${pkgname}-${pkgver}.deb")

package() {
   cd $srcdir
   ar x ${pkgname}-${pkgver}.deb
   tar xvf data.tar.gz
   cp -rp usr $pkgdir
}
