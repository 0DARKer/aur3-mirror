pkgname=shiinatimer
pkgver=1.0
pkgrel=1
epoch=
pkgdesc="Mayuri Shiina's timer"
arch=('i686' 'x86_64')
url="http://steinsgate.tv"
license=('Custom')
groups=()
depends=('wine')
makedepends=()
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
changelog=
source=(http://www30.zippyshare.com/d/35525803/24028/KaraageTimer.exe
        shiinatimer)
noextract=()
md5sums=('3b9619ce2e22adcdee1f6b2680e20e3e'
          '6cb9afee32b738604a31fd60d8b51248') #generate with 'makepkg -g'


package() {
  chmod 0755 shiinatimer
  mkdir -p "$pkgdir/usr/bin"
  mkdir -p "$pkgdir/usr/local/lib"
  cd "$srcdir/"
  cp shiinatimer "$pkgdir/usr/bin/"
  cp KaraageTimer.exe "$pkgdir/usr/local/lib/"
}

