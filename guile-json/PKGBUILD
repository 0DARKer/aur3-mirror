pkgname=guile-json
pkgrel=1
pkgver=0.3.0
pkgdesc="JSON module for Guile"
arch=(any)
license=(lgpl3)
makedepends=()
source=("http://download.savannah.gnu.org/releases/guile-json/guile-json-0.3.0.tar.gz")
url="https://github.com/aconchillo/guile-json"
md5sums=('95f2d38b5fb8427766ce19892deec73f')

build() {
  cd "$pkgname-$pkgver" && ./configure --prefix="$pkgdir/usr"
  make
}

package() {
  cd "$pkgname-$pkgver" && make install
}