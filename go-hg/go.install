post_install() {
  echo "To install 3rd party packages, setup your GOPATH"
  echo "eg. mkdir -p ~/go ; export GOPATH=~/go"
  echo "Note: Go has a bug, it may write into $GOROOT in some situation."
  echo "The temporary solution is"
  echo "  chown -R <your_user_id> /opt/go"
}
post_upgrade() {
  post_install $1
}
