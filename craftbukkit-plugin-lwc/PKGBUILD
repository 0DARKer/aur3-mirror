# Maintainer: TesX <tesfabpel@gmail.com>
pkgname=craftbukkit-plugin-lwc
pkgver=4.3.1
pkgrel=2
pkgdesc="A block protection plugin for Bukkit"
arch=(any)
url="http://dev.bukkit.org/bukkit-plugins/lwc/"
license=('BSD')
depends=('craftbukkit')
source=('http://dev.bukkit.org/media/files/647/269/LWC.jar')
md5sums=('fe29bd77061f1ced861554d6d6d28cf7')

package() {
  mkdir -p "$pkgdir/srv/craftbukkit/plugins" 
  install -m 644 -g craftbukkit -o craftbukkit "$srcdir/LWC.jar" "$pkgdir/srv/craftbukkit/plugins/"
}

