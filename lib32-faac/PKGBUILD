pkgname=lib32-faac
pkgver=1.28
pkgrel32=2
pkgrel=2
pkgdesc="FAAC is an AAC audio encoder."
url="http://www.audiocoding.com/"
license=()
arch=(x86_64)
depends=('lib32-glibc' 'lib32-libmp4v2')
groups=lib32
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('7beb819fb6f8a16e98b50af66b39dd75')

build() {
  mkdir -p ${startdir}/pkg/opt/lib32/usr/lib
  cp -rPf ${startdir}/src/usr/lib/*.so* ${startdir}/pkg/opt/lib32/usr/lib
}
