# Maintainer: Daniel Renninghoff

pkgname=mint-themes
pkgver=1.1.4
pkgrel=1
pkgdesc="The newest Mint-X theme, works with gtk 3.6."
arch=('any')
url="http://packages.linuxmint.com/pool/main/m/mint-themes"
license=('GPL')
depends=()
optdepends=('gtk-engine-murrine: for the GTK2 theme' 'mint-x-icons: Mint icon theme')
source=("${url}/${pkgname}_${pkgver}.tar.gz")
md5sums=('29a98fa03f87628cd9cd3dc5a1d1b8cf')

build() {
  cd "$srcdir/${pkgname}"
  cp -R usr $pkgdir/
}
# vim:syntax=sh
