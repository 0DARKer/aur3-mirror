pkgname=youtube-downloader
pkgver=1.7.8
pkgrel=1
pkgdesc="Search and download YouTube videos in all available formats, as: FLV (240p, 360p, 480p), MP4 (360p, 720p, 1080p)."
arch=('any')
url="http://trizen.go.ro/"
license=('GPL')
depends=('wget' 'perl-libwww')
