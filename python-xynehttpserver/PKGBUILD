pkgname=python-xynehttpserver
pkgver=2011.06.15.1
pkgrel=2
pkgdesc="BaseHTTPServer classes extended with authentication, SSL, and threading."
depends=('python2')
arch=('any')
license=('GPL')
url="http://xyne.archlinux.ca/projects/${pkgname}"
source=(http://xyne.archlinux.ca/src/${pkgname}-${pkgver}.tar.gz)
md5sums=('623bc72276192e04e57549be6ad1d402')

build() {
  cd "$srcdir/$pkgname"
  python2 setup.py install --root="$pkgdir"/ --optimize=1
}
