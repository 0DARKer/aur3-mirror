#Maintainer: Xyne <ac xunilhcra enyx, backwards>
pkgname=python3-xynexdg
pkgver=2011.07.04.1
pkgrel=2
pkgdesc="Implementation of various freedesktop.org standards, for internal use in Xyne's modules."
depends=('python3')
provides=('python-xynexdg')
arch=('any')
license=('GPL')
url="http://xyne.archlinux.ca/projects/${pkgname}"
source=(http://xyne.archlinux.ca/src/${pkgname}-${pkgver}.tar.gz)
md5sums=('89ca116119678d16ab34bc32fa56f509')

build() {
  cd "$srcdir/$pkgname"
  python3 setup.py install --root="$pkgdir"/ --optimize=1
}
