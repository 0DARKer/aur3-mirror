pkgname=qt4-smoke
pkgver=1.0.0
pkgrel=1
pkgdesc="Scripting Meta Object Kompiler Engine is a library contains interfaces to (almost) all Qt and KDE classes"
url="http://cougarpc.net/qyoto/"
depends=('qt4') 
makedepends=('cmake')
arch=(i686 x86_64)
license=(GPL)
source=(http://ekarchive.elikirk.com/test/$pkgname-$pkgver.tar.bz2)
md5sums=('c6591f1ea83839bad19c80eab08f3de4')

build() {
    cd ${startdir}/src/$pkgname-$pkgver
    cmake -DCMAKE_INSTALL_PREFIX=/usr .
    make || return 1
    make DESTDIR=${startdir}/pkg install
}

