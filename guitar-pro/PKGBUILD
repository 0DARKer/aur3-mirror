pkgname=guitar-pro
pkgver=6
pkgrel=1
pkgdesc="proprietary tab editor"
arch=(i686 x86_64)
url="http://www.guitar-pro.com/en/index.php"
license=('custom')
depends=()
source=(GuitarPro6Demo.deb)
md5sums=('d32d66cd3b51fe29e8396c76e8764661')

build() {
  cd "$srcdir"
  ar x GuitarPro6Demo.deb || return 1
  tar xzf data.tar.gz || return 1
  mv opt/ usr/ $pkgdir
}
