pkgname=lib32-gstreamer0.10-bad
pkgver=0.10.20
pkgrel=1
pkgdesc="GStreamer Multimedia Framework Bad Plugins (gst-plugins-bad)"
arch=(x86_64)
license=('GPL' 'LGPL')
groups=('lib32')
depends=('lib32-gstreamer0.10-base')
url="http://www.gnome.org"
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel}-i686.pkg.tar.xz)
md5sums=('25aeed78ee3428962a53f5a648794f80')

build() {
	install -dm755 $pkgdir/usr/lib32/gstreamer-0.10 || return 1
	cp -R $srcdir/usr/lib/gstreamer-0.10/*.so* $pkgdir/usr/lib32/gstreamer-0.10 || return 1
}
