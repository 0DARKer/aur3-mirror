pkgname=last.fm
pkgver=1.5.4.27091
pkgrel=1
pkgdesc="A Music Scrobbler"
arch=('x86_64')
url="http://www.last.fm/"
sourceurl=' http://ftp.us.debian.org/debian/pool/main/l/lastfm/lastfm_1.5.4.27091+dfsg1-1_amd64.deb'
license=(GPL)
depends=('mono' 'pacman>=4.0''Gtk#' 'gstreamer0.10>=0.10.0' 'libmad' 'libgpod' 'alsa-lib' 'gconf')
optdepends=('aurvote: vote for favorite packages from AUR'
      'customizepkg: automatically modify PKGBUILD during install/upgrade'
      'rsync: retrieve PKGBUILD from official repositories'
      'pacman-color: fully colorized output')
makedepends=('deb2targz')
backup=('etc/yaourtrc')
source=($sourceurl)

build() { 
   deb2targz ./lastfm_1.5.4.27091+dfsg1-1_amd64.deb
   #cd $srcdir/$pkgname-$pkgver/
  #./configure --prefix=/usr
  #make PREFIX=/usr sysconfdir=/etc localstatedir=/var 
}

install() {
  #cd $srcdir/$pkgname-$pkgver/
  #/usr/bin/deb2targz ./lastfm_1.5.4.27091+dfsg1-1_amd64.deb
  sudo cp -r ./usr /
}

md5sums=('0c32c69ca05f11742c8944982d336445')
