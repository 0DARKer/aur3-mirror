post_install() {
    echo "You may need to add /usr/share/webapps to PHP's open_basedir setting"
}

post_upgrade() {
    echo "As of version 1.1.13, this package has been moved to /usr/share/webapps"
    echo "You may need to update PHP's open_basedir setting to include this path"
}

