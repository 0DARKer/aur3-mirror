pkgname=chinese-calendar
pkgver=1.0.0
pkgrel=1
pkgdesc="Chinese traditional calendar for Ubuntu Kylin"
arch=('i686')
url="https://launchpad.net/chinese-calendar/"
license=('GPL3')
source=("https://launchpad.net/chinese-calendar/1.0/1.0.0/+download/${pkgname}_${pkgver}-0~45~ubuntu14.10.1_i386.deb")

md5sums=('13559ca954af2388b3d0336d323d1d62')

package () {
  cd ${srcdir}
  ar xv ${pkgname}_${pkgver}-0~45~ubuntu14.10.1_i386.deb
  tar xJf data.tar.xz
  mv usr etc ${pkgdir}
}
