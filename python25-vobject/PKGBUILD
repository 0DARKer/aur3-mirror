pkgname=python25-vobject
pkgver=0.8.1c
pkgrel=1
pkgdesc="python-vobject comiled with python2.5"
url="http://vobject.skyhouseconsulting.com"
license='Apache'
arch=('i686' 'x86_64')
depends=('python-dateutil')
makedepends=('python25-setuptools>=0.6c9')
source=("http://vobject.skyhouseconsulting.com/vobject-${pkgver}.tar.gz")
md5sums=('c9686dd74d39fdae140890d9c694c076')

build() {
	cd ${startdir}/src/vobject-${pkgver}
	python2.5 setup.py install --root=${startdir}/pkg
	}
