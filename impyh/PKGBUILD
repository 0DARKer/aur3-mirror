# then please put 'unknown'.

# Maintainer: DaeS
pkgname=impyh
pkgver=0.1
pkgrel=1
epoch=
pkgdesc="a GUI for scrot written in python"
arch=('i686' 'x86_64')
url="http://ompldr.org/vZDhhYQ/impyh-0.1.tar.gz"
license=('GPL')
groups=()
depends=('python' 'python-gobject' 'scrot')
makedepends=()
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=.INSTALL
changelog=
source=(http://ompldr.org/vZDhhYQ/impyh-0.1.tar.gz)
noextract=()
md5sums=('cc272a143ee2cbe1d26faa5ef17943ae')

build() {
  cd "$srcdir/$pkgname"
}

package() {
  cp -Rf ${srcdir}/impyh ${pkgdir}/usr/share
  cp -Rf ${srcdir}/impyh ${pkgdir}/usr/bin
}

