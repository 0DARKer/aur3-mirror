#Maintaining by me

pkgname=pdfchain
pkgver=0.123
pkgrel=2
pkgdesc="PDF Chain is a Graphical User Interface for the PDF Tool Kit"
url="http://pdfchain.sourceforge.net/"
license="GPL"
arch=('i686' 'x86_64')
depends=('pdftk' 'gtkmm')
makedepends=(intltool)
install=('pdfchain.install')
source=(http://kent.dl.sourceforge.net/project/pdfchain/pdfchain-0.123/PDF%20Chain%20version%200.123/${pkgname}-${pkgver}.tar.gz)
md5sums=('88870ffaf28833b85d84b02da60f41a4')

build() {
	  cd $startdir/src/$pkgname-$pkgver
	  ./configure
	  make || return 1
	  make prefix=$startdir/pkg/usr install
	}
