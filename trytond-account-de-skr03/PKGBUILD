# Maintainer: Robin Baumgartner <robin@baumgartners.ch>
pkgname=trytond-account-de-skr03
_pkgname=trytond_account_de_skr03
pkgver=3.2.0
_pkgdir=3.2
pkgrel=1
pkgdesc="The account_de_skr03 module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=3.2' 'trytond-account>=3.2')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("31998dbe7b0d7da326ae3e9e131bac9f")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py build
}

package() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py install --root=$pkgdir
}