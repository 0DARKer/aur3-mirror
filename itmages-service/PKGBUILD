# Maintainer: Raizner Evgeniy <razrfalcon@gmail.com>
pkgname=itmages-service
pkgver=0.34
pkgrel=1
pkgdesc="Uploading daemon for ITmages."
arch=('any')
url="https://launchpad.net/itmages"
license=('LGPL')
groups=('network')
depends=('python2' 'python-pycurl' 'dbus-python' 'python-lxml' 'pygobject')
makedepends=('bzr')
md5sums=() #generate with 'makepkg -g'

build() {
  bzr branch lp:itmages/${pkgname}
  cd "$srcdir/$pkgname"
  python2 ./setup.py install --root="${pkgdir}"
}   
