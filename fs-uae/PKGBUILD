#Contributor: Sebastian Wolf < fatmike303 at gmail dot com >
pkgname=fs-uae
pkgver=0.9.5
pkgrel=2
pkgdesc="A multi-platform Amiga emulator based on UAE/WinUAE, with a focus on emulating floppy-disk and CD-ROM based games"
arch=("i686" "x86_64")
url="http://fengestad.no/fs-uae"
license=GPL
depends=("sdl" "libpng" "openal" "zlib" "glib2" "pulseaudio")
source=(http://fengestad.no/${pkgname}/files/${pkgname}-${pkgver}.tar.gz)
md5sums=('7664e77e4ef4f6a83c8faec05ecb0cb9')

build() {
	cd ${srcdir}/${pkgname}-${pkgver}
	make || exit 1
	make install prefix=${pkgdir}/usr
}
