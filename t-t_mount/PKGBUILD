# Contributor: T-T

pkgname='t-t_mount'
pkgver='1.0'
pkgrel=0
pkgdesc="T-T | mount"
arch=('i686' 'x86_64')
url="http://aur.archlinux.org/packages.php?SeB=m&K=T-T"
license=('GPL')
depends=(bash)
makedepends=()
conflicts=()
optdepends=()

source=('did' 'mount.sh')
md5sums=('85949176fb2c848307b518b68016d2cf' '77080f4cbaffd2ad83b202d676721c0d')

build() {
	cd ${srcdir}/
	
	mkdir ${pkgdir}/usr
	mkdir ${pkgdir}/usr/bin
	
	mv ${srcdir}/mount.sh ${pkgdir}/usr/bin/mount.sh
	mv ${srcdir}/did ${pkgdir}/usr/bin/did
}
