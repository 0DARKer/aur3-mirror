# Contributor: T-T

pkgname='t-t_mount'
pkgver='1.0'
pkgrel=1
pkgdesc="T-T | mount"
arch=('i686' 'x86_64')
url="http://aur.archlinux.org/packages.php?SeB=m&K=T-T"
license=('GPL')
depends=(bash)
makedepends=()
conflicts=()
optdepends=()

source=('did' 'mount.sh')
md5sums=('e8edb5a8d0a1a7ed9f50b221144b67d9' '255fff8a404703bf24dab08e5608bc1b')

build() {
	cd ${srcdir}/
	
	mkdir ${pkgdir}/usr
	mkdir ${pkgdir}/usr/bin
	
	mv ${srcdir}/mount.sh ${pkgdir}/usr/bin/mount.sh
	mv ${srcdir}/did ${pkgdir}/usr/bin/did
}
