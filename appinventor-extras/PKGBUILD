pkgname=appinventor-extras
pkgver=1.1
pkgrel=1
pkgdesc="A new tool that makes it easy for anyone to create mobile applications for Android-powered devices."
arch=('i686' 'x86_64')
url="http://appinventor.googlelabs.com/"
license=('unknown')
depends=('java-runtime')
makedepends=()
source=(http://dl.google.com/dl/appinventor/installers/linux/appinventor-setup_${pkgver/_/-}.tar.gz)
noextract=()
md5sums=('d9f78c3f17df702343623d82d57f16db')
build() {
	cd $srcdir
	install -d $pkgdir/usr/google/appinventor-extras/
	mv $srcdir/${pkgname}_1.0/appinventor-extras/ $pkgdir/usr/google/

}
