pkgname=lib32-dbus-glib
pkgver=0.92
pkgrel32=1
pkgrel=1
pkgdesc="GLib bindings for DBUS"
url="http://www.freedesktop.org/wiki/Software/DBusBindings"
license=('LGPL')
arch=(x86_64)
depends=('lib32-dbus-core' 'lib32-glib2')
groups=lib32
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('b6a53c50b66e64c2f073bdbc45ebc147')

build() {
  mkdir -p ${startdir}/pkg/opt/lib32/usr/lib
  cp -rPf ${startdir}/src/usr/lib/*.so* ${startdir}/pkg/opt/lib32/usr/lib
}
