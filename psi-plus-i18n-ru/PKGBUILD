#Packager: Nicktian Lezver Nicktian@mail.ru

pkgname=psi-plus-i18n-ru
pkgver=124
pkgrel=1
pkgdesc="Russian Language Pack for psi+ from psi-dev team"
depends=('psi-plus-svn')
conflicts=('psi-i18n')
arch=('i686' 'x86_64')
license=('GPL')
url="http://code.google.com/p/psi-dev/"

_svntrunk=http://psi-ru.googlecode.com/svn/trunk/
_svnmod=psi-ru

build() {
if [ -d $_svnmod ]
then 
svn up $_svnmod
else
svn co $_svntrunk $_svnmod
fi
install -D -m 544 $srcdir/$_svnmod/psi_ru.qm $startdir/pkg/usr/share/psi/psi_ru.qm
}
