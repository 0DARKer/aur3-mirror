pkgname=eclipse-i18n-fr
pkgver=4.3.0
pkgbuild=v20131123020001
pkgrel=1
pkgdesc="French language for eclipse IDE"
url="http://babel.eclipse.org"
arch=('any')
license=('EPL')
depends=('eclipse')
makedepends=('unzip')
source=("http://download.eclipse.org/technology/babel/babel_language_packs/R0.11.1/kepler/BabelLanguagePack-eclipse-fr_${pkgver}.${pkgbuild}.zip")
sha256sums=('5767de53b3496ced07084763506f41f35cee622e2d17ebdad579821aff8c42a7')

package() {
  mkdir -p ${pkgdir}/usr/share/
  cp -R ${srcdir}/eclipse ${pkgdir}/usr/share/
}
