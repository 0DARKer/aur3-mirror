# Contributor: pktfag

pkgname=yachat
pkgver=3.1.0.8318
pkgrel=1
pkgdesc="Chat module of Ya.Online is based on Psi"
arch=('i686' 'x86_64')
url="http://nano.yandex.ru/project/online/"
license=('GPL-2')
depends=('qt>=4.5')
source=(http://download.yandex.ru/online/$pkgname-source.tar.gz)

md5sums=('7c681e1b08f0641ec44ddd62c85db4ed')

build() {
	cd $startdir/src/$pkgname-source-8318
	./configure --prefix=/usr --disable-aspell

	make  || return 1
	make INSTALL_ROOT=${startdir}/pkg install || return 1
}
