# Contributor: Tocer Deng <tocer.deng@gmail.com>
pkgname=pacparser
pkgver=1.2.0b
pkgrel=1
pkgdesc="A library to make your web software pac (proxy auto-config) files intelligent"
arch=('i686' 'x86_64')
url="http://code.google.com/p/pacparser/"
license=('LGPL')
depends=()
makedepends=('python')
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
source=(http://pacparser.googlecode.com/files/$pkgname-$pkgver.tar.gz)
noextract=()
md5sums=('231bf93e6d4b7ad4bfe142a06f845f7d')

build() {
  cd "$srcdir/$pkgname-$pkgver"

  # pacparser C library and pactester
  make || return 1
  make DESTDIR="$pkgdir/" install || return 1
  # pacparser python module
  make pymod || return 1
  make DESTDIR="$pkgdir/" install-pymod || return 1

}

# vim:set ts=2 sw=2 et:
