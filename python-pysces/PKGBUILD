pkgname=python-pysces
pkgver=0.7.0
pkgrel=1
pkgdesc="Python simulator for Cellular Systems"
arch=('i686' 'x86_64')
depends=('python>=2.5' 'python-numpy>=1.2' 'python-scipy>=0.7' 'gcc>=4.2' 'python-matplotlib>=0.98')
url="http://pysces.sourceforge.net/"
license=('BSD-like license')
source="http://dl.sourceforge.net/project/pysces/pysces/pysces-0.7/pysces-$pkgver.tar.gz"
md5sums=('4579d3971e66f4eeb6f790f98b731aba')
build() {
	cd $srcdir/pysces-$pkgver/
	echo $pkgdir
	python setup.py install --prefix=$pkgdir/usr/
}
