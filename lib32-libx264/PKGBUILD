pkgname=lib32-libx264
_pkgname=libx264
pkgver=142.20140311
pkgrel=1
_pkgrel=4
_epoch=1
pkgdesc="Free library for encoding H264/AVC video streams, lib32"
arch=('x86_64')
url="http://www.videolan.org/developers/x264.html"
license=('GPL')
depends=("${_pkgname}")
replaces=("lib32-x264")
conflicts=("lib32-x264")
source=("ftp://ftp.archlinux.org/extra/os/i686/${_pkgname}-${_epoch}:${pkgver}-${_pkgrel}-i686.pkg.tar.xz")
md5sums=('4f5268bb8b191752904a5f9ffaab1bd6')


package() {
cd ${srcdir}
  
install -dm755 $pkgdir/usr/lib32/
cp -R $srcdir/usr/lib/*.so* $pkgdir/usr/lib32/
}
