# Maintainer: Sebastien Luttringer <seblu+arch@seblu.net>
pkgname=virtualbox-ext-oracle
pkgver=4.0.4
_build=70112
pkgrel=2
pkgdesc="Oracle VM VirtualBox Extension Pack"
arch=('i686' 'x86_64')
url="http://www.virtualbox.org/"
license=('custom:PUEL')
depends=("virtualbox")
options=(!strip)
source=( "http://download.virtualbox.org/virtualbox/$pkgver/Oracle_VM_VirtualBox_Extension_Pack-$pkgver-$_build.vbox-extpack" "PUEL" )
install=virtualbox-ext-oracle.install
md5sums=('5130c3678f85d849a277cceac65c8c46'
         '08b28b82d0ebd6962025100d4b5414a1')

package() {
  install -D -m 644 "$srcdir/Oracle_VM_VirtualBox_Extension_Pack-$pkgver-$_build.vbox-extpack" "$pkgdir/usr/share/virtualbox/extensions/Oracle_VM_VirtualBox_Extension_Pack-$pkgver.vbox-extpack"
  install -D -m 644 "$startdir/PUEL" "$pkgdir/usr/share/licenses/$pkgname/PUEL"
}

