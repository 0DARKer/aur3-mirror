pkgname=python-svg.path
pkgver=1.0b1
pkgrel=1
pkgdesc="SVG path objects and parser"
arch=(any)
url="https://github.com/regebro/svg.path"
license=('CCPL')
depends=('python')

source=(http://pypi.python.org/packages/source/s/svg.path/svg.path-${pkgver}.tar.gz)
md5sums=('e9c8fdc2ec1213b389c23daa10ee055e')

build() {
  cd "${srcdir}/svg.path-${pkgver}"
  python setup.py install --root="${pkgdir}" --optimize=1
}
