# Maintainer: Your Name <youremail@domain.com>
pkgname=transcribe
pkgver=8.10
pkgrel=1
pkgdesc="The software to help transcribe recorded music."
arch=('i686' 'x86_64')
url="http://www.seventhstring.com/"
license=('nonfree')
provides=('transcribe')
source=("http://www.seventhstring.com/xscribe/xscsetup.tar.gz")
md5sums=("ce8a9bdbb88351b4be78f3972837a781")

build() {
  cd "$srcdir/$pkgname"
  mkdir $pkgdir/{usr,usr/bin}
  cp transcribe "$pkgdir/usr/bin/"
}
