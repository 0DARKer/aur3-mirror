# Maintainer: Austin <doorknob60 at gmail dot com>
pkgname=lib32-glc
pkgver=0.5.8
pkgrel=1
pkgdesc="32-Bit Libraries for GLC to record 32 bit programs such as Wine"
arch=('x86_64')
url="http://nullkey.ath.cx/projects/glc"
license=('CUSTOM')
depends=('glc')
source=('http://www.mdn.fm/files/167221_pwq92/glc-32libs.tar.gz')
md5sums=('f7497dd0f42a12bdbf0c8133faf9da57')

build() {
mkdir -p $startdir/pkg/opt/lib32/usr/lib
cp -rPfp $startdir/src/usr/lib $startdir/pkg/opt/lib32/usr
}
