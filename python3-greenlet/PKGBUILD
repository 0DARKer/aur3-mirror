pkgname=python3-greenlet
pkgver=0.3.1
pkgrel=1
pkgdesc="python coroutine library"
license=("MIT")
url="http://pypi.python.org/pypi/greenlet"
depends=('python3')
source=(http://pypi.python.org/packages/source/g/greenlet/greenlet-$pkgver.tar.gz)
arch=('i686' 'x86_64')
md5sums=('8d75d7f3f659e915e286e1b0fa0e1c4d')

build() {
  cd $startdir/src/greenlet-$pkgver
  /usr/bin/python3 setup.py build || return 1
  /usr/bin/python3 setup.py install --root=$startdir/pkg
}
