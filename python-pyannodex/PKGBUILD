pkgname=python-pyannodex
pkgver=0.7.3
pkgrel=1
pkgdesc="Python bindings for the annodex libraries"
url="http://benno.id.au/code/pyannodex"
license="LGPL"
depends=('python' 'libannodex')
source=(http://benno.id.au/code/pyannodex/pyannodex-$pkgver.tar.gz)
md5sums=('ff61fc1564bfa58c49f880db02674e5b')

build() {
  cd $startdir/src/pyannodex-$pkgver
  python setup.py install --root=$startdir/pkg
}
