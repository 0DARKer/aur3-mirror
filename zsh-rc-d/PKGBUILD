# Maintainer: Clément Démoulins <clement@archivel.fr>
pkgname=zsh-rc-d
pkgver=2
pkgrel=1
pkgdesc="zsh completion for the script rc.d"
arch=(any)
url="http://aur.archlinux.org/packages.php?ID=49136"
license=('GPL')

source=(_rc.d)
md5sums=('0ebd0f43c945569581d5a7746f27654c')

package() {
  install -Dm644 $srcdir/_rc.d $pkgdir/usr/share/zsh/site-functions/_rc.d
}

