post_install() {
	MODULES="libXnDeviceSensorV2.so libXnDeviceFile.so"
	for module in $MODULES
	do 
		/usr/bin/niReg -r /usr/lib/$module /etc/primesense
	done
}

pre_remove() {
	MODULES="libXnDeviceSensorV2.so libXnDeviceFile.so"
	for module in $MODULES
	do 
		/usr/bin/niReg -u /usr/lib/$module
	done
}
