# Maintainer: Diego Pi
pkgname=tubeflush
pkgver=0.0
pkgrel=1
pkgdesc="A script  to converto youtube videos in mp3."
arch=('i686' 'x86_64')
url="https://github.com/vencizon/tubeflush"
license=('GPL3')
makedepends=('git')
depends=('youtube-dl' 'ffmpeg' 'lame')
source=(https://raw.github.com/vencizon/tubeflush/master/tubeflush)
md5sums=('57a3264a80a024ad605ca6236acf1b08')

package() {
  cd $srcdir
  install -Dm755 tubeflush $pkgdir/usr/bin/tubeflush
}
