# Contributor: Mateusz Herych <heniekk@gmail.com>
pkgname=natdet
pkgver=1.0.6
pkgrel=3
pkgdesc="Simple program for NAT detection"
arch=('i686')
url="http://elceef.itsec.pl/natdet/"
license=('GPL2')
depends=('libpcap')
source=("http://elceef.itsec.pl/natdet/natdet-1.0.6.tgz")
md5sums=('3300273c0cb9f582bbe8464ca764e91d')

build() {
	cd $startdir/src/$pkgname
	sed -i 's/local//g' config.h
	./configure
	mkdir -p $startdir/pkg/usr
	make || return 1
	make install PREFIX=$startdir/pkg/usr || return 1

}
