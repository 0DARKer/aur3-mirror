pkgname=lib32-gdbm
pkgver=1.8.3
pkgrel=8
pkgdesc="GNU database library"
depends=('lib32-glibc')
arch=('x86_64')
groups=('lib32')
url="http://www.gnu.org/software/gdbm/gdbm.html"
license=('GPL')
source=(ftp://ftp.archlinux.org/core/os/i686/${pkgname/lib32-}-$pkgver-$pkgrel-i686.pkg.tar.xz)
md5sums=('f723bf84ffe9ca40bb593b24ff36232b')

package() {
  mkdir -p $startdir/pkg/opt/lib32/usr/lib/
  cp -R $startdir/src/usr/lib/*.so* $startdir/pkg/opt/lib32/usr/lib
}
