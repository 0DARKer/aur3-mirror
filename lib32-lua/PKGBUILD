pkgname=lib32-lua
pkgver=5.1.4
pkgrel32=6
pkgrel=2
pkgdesc="A powerful light-weight programming language designed for extending applications."
arch=('x86_64')
url="http://www.lua.org/"
depends=()
license=('MIT')
groups=lib32
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('609d25effe31b82a475ad430e8cb515a')
 
build() {
  mkdir -p ${startdir}/pkg/opt/lib32/usr/lib
  cp -rPf ${startdir}/src/usr/lib/*.so* ${startdir}/pkg/opt/lib32/usr/lib
}
