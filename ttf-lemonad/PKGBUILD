pkgname=ttf-lemonad
pkgver=1
pkgrel=3
pkgdesc="cyrillic and latin fonts from the project of free fonts"
arch=('any')
url="http://www.jovanny.ru/free-fonts.html" 
license=('unknown')
source=(http://www.pumbur.net/file/lemonad.tar.gz)
md5sums=('c1c1d767a85fb13ec79c94124ea4dc1f')

build() {
  install -d $pkgdir/usr/share/fonts/OTF/lemonad
  cp -r $srcdir/*.otf $pkgdir/usr/share/fonts/OTF/lemonad
}
