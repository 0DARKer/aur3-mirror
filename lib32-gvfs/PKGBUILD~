pkgname=lib32-gvfs
pkgver=1.12.3
pkgrel=1
pkgrel32=1
pkgdesc="Userspace virtual filesystem implemented as a pluggable module for gio"
url="http://www.gnome.org/"
arch=('x86_64')
license=('LGPL')
depends=('lib32-libxml2' 'lib32-avahi' 'lib32-libgnome-keyring')
source=(http://ftp.archlinux.org/extra/os/i686/gvfs-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)

md5sums=('37dfc8c00b207251b8fa9258bb47104b')
 

build() {
mkdir -p ${pkgdir}/usr/lib32
cp -rPf ${srcdir}/usr/lib/* ${pkgdir}/usr/lib32
}
