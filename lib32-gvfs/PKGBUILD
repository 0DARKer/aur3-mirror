pkgname=lib32-gvfs
pkgver=1.6.3
pkgrel=1
pkgdesc="Userspace virtual filesystem implemented as a pluggable module for gio"
url="http://www.gnome.org/"
arch=('x86_64')
license=('LGPL')
groups=('lib32')
depends=('lib32-libtool' 'lib32-gconf')
source=(ftp://ftp.archlinux.org/extra/os/i686/gvfs-${pkgver}-${pkgrel}-i686.pkg.tar.xz)
md5sums=('75d4d35e91e5b5805c42a5720fbed53f')

build() {
  mkdir -p ${pkgdir}/opt/lib32/usr
  cp -rPf ${srcdir}/usr/lib ${pkgdir}/opt/lib32/usr
}
