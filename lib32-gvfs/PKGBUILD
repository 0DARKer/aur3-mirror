pkgname=lib32-gvfs
pkgver=1.8.1
pkgrel=1
pkgrel32=1
pkgdesc="Userspace virtual filesystem implemented as a pluggable module for gio"
url="http://www.gnome.org/"
arch=('x86_64')
license=('LGPL')
depends=('lib32-libxml2' 'lib32-avahi' 'lib32-libgnome-keyring')
source=(ftp://ftp.archlinux.org/extra/os/i686/gvfs-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('3dfe30280485fe316be443da182bbf6b')

build() {
  mkdir -p ${pkgdir}/usr/lib32
  cp -rPf ${srcdir}/usr/lib/* ${pkgdir}/usr/lib32
}
