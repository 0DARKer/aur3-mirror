post_install() {
cd /opt/mendeleydesktop/lib/
    echo ""
    echo "Are you KDE a user ?"
    select al in "Yes" "No"; do
        case $al in
            "Yes" ) rm -f libQt*;  break;;
            "No" )  break;;
        esac
    done
    echo ""
}

post_upgrade() {
   post_install
}

/opt/mendeleydesktop/lib/libQt*
