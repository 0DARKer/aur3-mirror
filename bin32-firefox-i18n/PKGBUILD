build() {
  cd ${srcdir}
  install -d ${pkgdir}/usr/lib32/bin32-firefox/extensions
  for i in *.xpi; do
    install -m644 $i ${pkgdir}/usr/lib32/bin32-firefox/extensions
  done
}
