# This is a default template for a post-install scriptlet.

post_install() {
echo -e "#/bin/sh\ncd /usr/share/landeseternelleslinux && /usr/bin/le.x86.linux.bin" > /usr/bin/landeseternelles
chmod 755 /usr/bin/landeseternelles
}

post_remove() {
rm -f /usr/bin/landeseternelles
}

