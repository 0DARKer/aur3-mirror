# Contributer: OramahMaalhur <oramahmaalhur@gmail.com>

pkgname=python-oursql
pkgver=0.9.2
pkgrel=1
pkgdesc="new set of MySQL bindings for python3"
arch=(any)
url="https://launchpad.net/oursql"
license=('BSD')
depends=('python>=3.1' 'libmysqlclient')
makedepends=('setuptools')
source=(http://launchpad.net/oursql/py3k/py3k-$pkgver/+download/oursql-0.9.2.zip)
md5sums=('caecfdec0200ff0a5e960476f75b6707')

build() {
	cd "$srcdir/oursql-$pkgver"
	python ./setup.py install --root=$pkgdir --prefix=/usr || return 1
}


