pkgname=dotpac
pkgver=0.3
pkgrel=2
pkgdesc="This little bash script helps you getting rid of your /etc/*.pac* files."
arch=(i686 x86_64)
license=()
url="http://wiki.archlinux.org/index.php/dotpac"
depends=()
source=(http://aur.archlinux.org/packages/dotpac/dotpac/dotpac)
md5sums=('bde9545d5555941bb5fa310fe9caaf9a')
build () {
	cd $startdir/src/
	install -D -m755 dotpac $startdir/pkg/usr/bin/dotpac
}
