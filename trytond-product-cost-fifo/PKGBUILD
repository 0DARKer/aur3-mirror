pkgname=trytond-product-cost-fifo
_pkgname=trytond_product_cost_fifo
pkgver=2.8.0
_pkgdir=2.8
pkgrel=1
pkgdesc="The product_cost_fifo module of the Tryton application platform"
arch=('any')
url='http://www.tryton.org/'
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=2.8' 'trytond-product>=2.8' 'trytond-stock>=2.8')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")
md5sums=("fb71b8f8923e7d86813ac7c62eb2073b")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py install --root=$pkgdir
}