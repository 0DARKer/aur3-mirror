# Contributor: Cedric Chabanois <cchabanois@gmail.com>
pkgname=imule
pkgver=1.4.6
pkgrel=1
pkgdesc="Anonymous P2P file sharing software"
url="http://www.imule.i2p.tin0.de"
license=('GPL')
depends=('wxgtk')
optdepends=('i2p')
arch=('i686' 'x86_64')
source=(http://aceini.no-ip.info/imule/${pkgver}/iMule_${pkgver}_src.tbz)
md5sums=('79f9de766f4481f2ce169888201c5427')
build() {
  cd $srcdir/iMule-${pkgver}-src
  ./configure --enable-debug=no --enable-optimize=yes --prefix=/usr --disable-profile 
  make || return 1
  make DESTDIR=${pkgdir} install || return 1
}
