pkgname=cinnamon-theme-eleganse
pkgver=20120217
pkgrel=1
pkgdesc="Cinnamon theme based on Elegance"
url="http://gnome-look.org/content/show.php/Eleganse+and+dark-glass+Cinnamon+themes?content=147807"
license=('GPL')
arch=('any')
depends=('cinnamon')
source=("http://gnome-look.org/CONTENT/content-files/147807-Eleg_dark_elem.zip")
md5sums=('95cfd2eb5deebd9d6f382bd4330ffb07')

package() {
  find . -type f -exec install -Dm644 '{}' "$pkgdir/usr/share/themes/{}" \;
}
