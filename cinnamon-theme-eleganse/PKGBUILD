# Maintainer: crazyelf5

pkgname=cinnamon-theme-eleganse
pkgver=20120104
pkgrel=2
pkgdesc="Cinnamon theme based on Elegance"
url="http://gnome-look.org/content/show.php/Eleganse+and+dark-glass+Cinnamon+themes?content=147807"
license=('GPL')
arch=('any')
depends=('cinnamon')
install=$pkgname.install
source=("http://gnome-look.org/CONTENT/content-files/147807-Eleg_dark_elem.zip")
md5sums=('a4a29aed02ad643f94ae43266b538e81')

package() {
    install -dm755 "$pkgdir/usr/share/themes"
    mv {dark-glass,Eleganse,ElementaryOS} "$pkgdir/usr/share/themes"
}
