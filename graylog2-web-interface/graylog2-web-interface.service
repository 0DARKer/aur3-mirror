[Unit]
Description=Graylog2 web-interface 
After=network.target

[Service]
EnvironmentFile=/etc/conf.d/graylog2-web-interface.conf
PIDFile=/run/graylog2.pid
ExecStart=/usr/share/webapps/graylog2-web-interface/bin/graylog2-web-interface -Dhttp.port=${HTTP_PORT} -Dhttp.address=${HTTP_ADDRESS}

[Install]
WantedBy=multi-user.target
