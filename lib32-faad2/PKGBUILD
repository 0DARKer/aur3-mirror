pkgname=lib32-faad2
pkgver=2.7
pkgrel32=1
pkgrel=1
pkgdesc="ISO AAC audio decoder"
url="http://www.audiocoding.com/"
license=()
arch=(x86_64)
depends=('lib32-glibc')
groups=lib32
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.gz)
md5sums=('87d9ec96310528814bddac48f92cb790')

build() {
  mkdir -p ${startdir}/pkg/opt/lib32/usr/lib
  cp -rPf ${startdir}/src/usr/lib/*.so* ${startdir}/pkg/opt/lib32/usr/lib
}
