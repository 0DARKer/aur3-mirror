pkgname=mysql-connector-python
pkgver=0.3.0
pkgrel=1 
pkgdesc="the MySQL Client/Protocol implemented in Python!"
url="http://forge.mysql.com/projects/project.php?id=302"
arch=('i686' 'x86_64')
license=('GPL')
depends=()
makedepends=(python2)
conflicts=()
replaces=()
backup=()
install=
source=(http://launchpad.net/myconnpy/0.3/0.3.0/+download/$pkgname-$pkgver-devel.tar.gz)
md5sums=('6a829c4b730ed7d7863928d8587a30e0')
build() {
  cd $startdir/src/$pkgname-$pkgver-devel
  python2 setup.py install --root=$pkgdir/ --optimize=1
}
