pkgname=icon-de
pkgver=1.0.3
pkgrel=1
pkgdesc=""
arch=('i686' 'x86_64')
url="http://forum.ubuntu.org.cn/viewtopic.php?f=1&t=372366"
license=("unknown")
depends=("wmctrl" "feh" "wqy-zenhei" "xorg-server-xephyr" "libpng12")
makedepends=()
provides=()
conflicts=()
install='./icon-de.install'
if [ "$CARCH" = "x86_64" ]; then
    source=("http://sourceforge.net/projects/icon-de/files/icon-DE_amd64_1.0.3.deb")
    sha1sums=("21feef0597679013c786159653095695227603f9")
elif [ "$CARCH" = "i686" ]; then
    source=("http://sourceforge.net/projects/icon-de/files/icon-DE_i386_1.0.3.deb")
    sha1sums=("e129861b9225b4545fc366bfd57b16f9a0590bfa")
fi

build() {
    return 0
}

package() {
    tar -xzvpf "$srcdir/data.tar.gz" -C "$pkgdir" \
        --exclude '.directory' --exclude "src/*"
}