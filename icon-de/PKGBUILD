pkgname=icon-de
pkgver=1.1.1
pkgrel=1
pkgdesc=""
arch=('i686' 'x86_64')
url="http://forum.ubuntu.org.cn/viewtopic.php?f=1&t=372366"
license=("unknown")
depends=("wmctrl" "feh" "wqy-zenhei" "xorg-server-xephyr" "libpng12")
makedepends=()
provides=()
conflicts=()
install='./icon-de.install'
if [ "$CARCH" = "x86_64" ]; then
    source=("http://sourceforge.net/projects/icon-de/files/icon-DE_amd64_1.1.1.deb/download")
    sha1sums=("f65e0d0e520661f6dee06b55d3b3a3f623857246")
elif [ "$CARCH" = "i686" ]; then
    source=("http://sourceforge.net/projects/icon-de/files/icon-DE_i386_1.1.1.deb/download")
    sha1sums=("e2e25a3c3943822820deb5ee79f343e0f4d07c82")
fi

build() {
    return 0
}

package() {
    tar -xzvpf "$srcdir/data.tar.gz" -C "$pkgdir" \
        --exclude '.directory' --exclude "src/*"
}
