pkgname=ttf-schoolboy
pkgver=2.0
pkgrel=2
pkgdesc="SchoolBoy font"
arch=('i686' 'x86_64')
url="http://www.kde-look.org/"
license=('Creative Commons')
depends=('fontconfig' 'xorg-font-utils')

install=ttf.install
source=(http://www.kde-look.org/CONTENT/content-files/110806-Schoolboy%20Text.ttf)
md5sums=('aed8bfc7190c8b7863a613612c2e9cee')
sha1sums=('b4eaa142187e0bb7fe3c31aa2f2fdcf4c223ce1e')

build() {
install -Dm 644 $srcdir/110806-Schoolboy%20Text.ttf $pkgdir/usr/share/fonts/TTF/SchoolBoy.ttf
} 
