#Maintainer theNerd247 <noah.harvey247@gmail.com>

pkgname=libllist
pkgver=0.0.2
pkgrel=3
pkgdesc="A C library for linked lists"
arch=('i686' 'x86_64')
url=https://www.github.com/theNerd247/libllist
license=('GPL')
source=("https://github.com/theNerd247/libllist/raw/master/$pkgname-$pkgver.tar.gz")
depends=('glibc')
md5sums=('b593e0fe84d2ab375648b7a730d6b293')

  
build()
{
	cd "$srcdir/$pkgname-$pkgver"
	make
}

package()
{
	cd "$srcdir/$pkgname-$pkgver"
	make DESTDIR="$pkgdir" install
}
