# Maintainer: Alexander Straube <alexander.straube@googlemail.com>
# Contributor: Alexander Straube <alexander.straube@googlemail.com>
pkgname=gmabooster
pkgver=0.9c
pkgrel=20090625.1
pkgdesc="GMABooster is a free program which (over)clocks currently supported chipsets: Intel 945GM/GME/GMS/GSE and 943/940GML/GU Express featuring GMA 950"
arch=(i686)
url="http://www.gmabooster.com/"
license=('donationware')
source=('http://omploader.org/vMmxvaA/GMABooster_Linux_09c.zip'
	'GMABooster.sh')
md5sums=('7f8986383b474fff6ea3138a52e4d8d2'
         'd03af189f656490b1335d7e27121dd24')

build() {
	cd $startdir/src/
	install -D -c -m755 GMABooster $startdir/pkg/opt/GMABooster/GMABooster || return 1
	install -D -c -m755 data.bin $startdir/pkg/opt/GMABooster/data.bin || return 1
	install -D -c -m755 Readme_GMABooster.txt $startdir/pkg/opt/GMABooster/Readme_GMABooster.txt || return 1
	install -D -c -m755 $startdir/GMABooster.sh $startdir/pkg/usr/bin/GMABooster || return 1
}
