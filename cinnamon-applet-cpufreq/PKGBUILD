# Maintainer: ValHue

pkgname=cinnamon-applet-cpufreq
pkgver=2.2
pkgrel=1
pkgdesc="A Cinnamon CPU Frequency Applet. Display and control cpu frequency."
arch=('i686' 'x86_64')
url="http://cinnamon-spices.linuxmint.com/applets/view/70"
license=('GPL3')
depends=('cinnamon' 'gnome-applets')
source=("http://cinnamon-spices.linuxmint.com/uploads/applets/01SW-XN8W-EY1V.zip")
md5sums=('fd2e58f20d60030c3df88243436e762b')

package() {
    find cpufreq\@mtwebster -type f -exec install -Dm644 '{}' "$pkgdir/usr/share/cinnamon/applets/{}" \;
}
