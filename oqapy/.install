## arg 1:  the new package version
post_install() {
echo "#!/bin/bash" > /usr/bin/oqapy
    echo "/usr/share/oqapy-0.4.1/oqapy.py" > /usr/bin/oqapy
    chmod +x /usr/bin/oqapy
    if [ ! -e /usr/lib/libboost_python.so.1.46.0 ]
	then
	    ln -s /usr/lib/libboost_python.so.1.46.1 /usr/lib/libboost_python.so.1.46.0
        fi
}
post_upgrade() {
    post_install
}
