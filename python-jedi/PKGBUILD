# Maintainer: Jesus Alvarez
pkgname=python-jedi
pkgver=0.5b4
pkgrel=1
pkgdesc="An autocompletion tool for Python that can be used for text editors."
arch=('any')
url="https://github.com/davidhalter/jedi-vim"
license=('LGPL')
depends=('python')
source=("http://pypi.python.org/packages/source/j/jedi/jedi-${pkgver}.tar.gz")
md5sums=('1ce28453b1f11df973c00b5f5f9b3da2')

package() {
  cd "$srcdir/jedi-$pkgver"
  python setup.py install --root="$pkgdir/" --optimize=1
}

