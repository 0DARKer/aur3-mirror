pkgname=systemd-emacs-daemon
pkgver=1.0
pkgrel=1
pkgdesc="A per-user emacs daemon service for systemd"
url='http://packages.alpha-lang.net/systemd-emacs-daemon-1.0/'
license=('BSD')
arch=('any')
_arsrc=$pkgname-$pkgver.tgz
source=($_arsrc::http://packages.alpha-lang.net/$pkgname-$pkgver/source.tgz)

package() {
  cd "$pkgdir"
  tar -xzf "$srcdir/$_arsrc"
}

md5sums=('971df4b5b166cc43e2e581b7450a82e4')
