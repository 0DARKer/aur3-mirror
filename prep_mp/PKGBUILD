# Maintainer: Your Name <youremail@domain.com>

pkgname=prep_mp
pkgver=3.1
pkgrel=3
pkgdesc="A random .MP3 at random speed script using mplayer"

arch=('any')
url="http://aur.archlinux.org/"
license=('GPL')
depends=('mplayer')
source=($pkgname)

md5sums=('547295d1d7a6114544a18fb95968ce14')


build() {

  echo
  echo "Will be installed as: /usr/bin/prep_mp"
  install -Dm0755 prep_mp $pkgdir/usr/bin/$pkgname
  echo

}

