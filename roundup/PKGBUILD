pkgname=roundup
pkgver=1.4.19
pkgrel=1 
pkgdesc="Issue tracking system."
url="http://www.roundup-tracker.org"
arch=('i686' 'x86_64')
license="PSFL" 
depends=('python2') 
source=(http://pypi.python.org/packages/source/r/$pkgname/$pkgname-$pkgver.tar.gz)

build() { 
 cd $startdir/src/$pkgname-$pkgver
 python2 setup.py build
 python2 setup.py install --prefix=$startdir/pkg/usr
}
md5sums=('70ea4373da67e7b18a21550266f41a90')
