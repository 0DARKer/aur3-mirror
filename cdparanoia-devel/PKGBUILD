pkgname=cdparanoia-devel
pkgver=10pre2
pkgrel=1
pkgdesc="CDDA extraction tool"
arch=('i686' 'x86_64')
url="http://www.xiph.org/paranoia/"
license=('GPL')
provides=('cdparanoia')
conflicts=('cdparanoia')
source=(http://downloads.xiph.org/releases/cdparanoia/cdparanoia-III-${pkgver}.src.tgz)
md5sums=('678dc751a6e74e4a19c67737b306dfec')
sha1sums=('28715586aceacdc26dfa0038b48d01d47296fd54')

build() {
	cd cdparanoia-III-${pkgver}
	./configure --prefix=/usr
	make || return 1
	make prefix=${startdir}/pkg/usr install
}

