# Contributor: Andrea Scarpino <bash.lnx@gmail.com>

pkgname=jdk6-docs
pkgver=6_b30_1
pkgrel=1
pkgdesc="Java SE Development Kit Documentation 6"
arch=('any')
url="http://java.sun.com/"
license=('custom')
options=('docs')
md5sums=('bb70bdc15b4ec322191660c90adb99ba')
source=('http://download.oracle.com/otn-pub/java/jdk/6u30-b12/jdk-6u30-apidocs.zip')

build(){
  cd "$srcdir/docs" || return 1
  mkdir -p "$pkgdir/usr/share/doc/java" || return 1
  cp -rf * "$pkgdir/usr/share/doc/java" || return 1
}
