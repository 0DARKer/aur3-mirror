
post_install() {
  daggerfall --enable-mod=fixes
}

pre_remove() {
  daggerfall --disable-mod=fixes
  daggerfall --disable-mod=dfqfix
  daggerfall --disable-mod=fixqs001
  daggerfall --disable-mod=jumping_spell_fix
  daggerfall --disable-mod=mages_biography
  daggerfall --disable-mod=faction_fix
  daggerfall --disable-mod=wrothgarian_enclave_fix
  daggerfall --disable-mod=dunmer_fall
}

