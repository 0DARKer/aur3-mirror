default=arch
timeout=5
# lba32
prompt

image=/vmlinuz26
	label=arch
	append="rootfstype=ext4 nomodeset add_efi_memmap"
	root=/dev/disk/by-uuid/XXXXXXXXXXXXXXXXXXXXXXXXXXXX
	initrd=/kernel26.img
	read-only

image=/vmlinuz26
	label=arch-fallback
	append="rootfstype=ext4 nomodeset add_efi_memmap"
	root=/dev/disk/by-uuid/XXXXXXXXXXXXXXXXXXXXXXXXXXXX
	initrd=/kernel26-fallback.img
	read-only
