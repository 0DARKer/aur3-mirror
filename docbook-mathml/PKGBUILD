# Maintainer: Sylvain HENRY <hsyl20@gmail.com>

pkgname=docbook-mathml
pkgver=1.1CR1
pkgrel=1
pkgdesc="MathML XML scheme ${pkgver}"
arch=(any)
url="http://www.oasis-open.org/docbook/"
license=('MIT')
depends=('libxml2')
install=docbook-mathml.install
source=(http://www.docbook.org/xml/mathml/1.1CR1/dbmathml.dtd)
md5sums=('bbf92a1d88ce217d736c222470d6811c')

build() {
  mkdir -p "${pkgdir}/usr/share/xml/docbook/mathml/${pkgver}"
  install dbmathml.dtd "${pkgdir}/usr/share/xml/docbook/mathml/${pkgver}/"
}
