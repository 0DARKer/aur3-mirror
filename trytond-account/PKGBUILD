pkgname=trytond-account
_pkgname=trytond_account
pkgver=1.8.0
_pkgdir=1.8
pkgrel=1
pkgdesc="Account module of the Tryton application platform"
arch=('any')
url="http://www.tryton.org/"
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=1.8' 'trytond-company>=1.8' 'trytond-country>=1.8' 'trytond-currency>=1.8')
makedepends=('python2-distribute')
source=("http://downloads.tryton.org/$_pkgdir/$_pkgname-$pkgver.tar.gz")

build() {
  cd $srcdir/$_pkgname-$pkgver
  python2 setup.py install --root=$pkgdir
}

md5sums=('d79d945e7647c9aa47f7420d81eb6fb5')
