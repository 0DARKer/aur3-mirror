pkgname=trytond-account
_pkgname=trytond_account
pkgver=1.4.1
pkgrel=1
pkgdesc="Account module of the Tryton application platform"
arch=('any')
url="http://www.tryton.org/"
license=('GPL3')
groups=('trytond-modules')
depends=('trytond>=1.4' 'trytond-company>=1.4' 
'trytond-country>=1.4' 'trytond-currency>=1.4')
makedepends=('setuptools')
source=("http://downloads.tryton.org/1.4/$_pkgname-$pkgver.tar.gz")
md5sums=('b45f094b8960dc852fcab5055994e9c0')

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}

