pkgname=cinnamon-theme-bluebird
pkgver=1.1
pkgrel=1
pkgdesc="Bluebird theme for cinnamon"
arch=('any')
url="http://cinnamon-spices.linuxmint.com/themes/view/126"
license=('unknown')
depends=('cinnamon')
source=('http://cinnamon-spices.linuxmint.com/uploads/themes/JJ9R-N8UC-JEYX.zip')
md5sums=('8b6303728e4661a4656bb4712c282412')

package() {
  mkdir -p ${pkgdir}/usr/share/themes/
  cp -r ${srcdir}/Bluebird-Cinnamon ${pkgdir}/usr/share/themes/
}

