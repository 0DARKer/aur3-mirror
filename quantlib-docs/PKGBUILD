pkgname=quantlib-docs
pkgver=1.0.1
pkgrel=1
pkgdesc='HTML documentation for quantlib.'
arch=('any')
url='http://quantlib.org'
license=('BSD')
source=("http://downloads.sourceforge.net/project/quantlib/QuantLib/1.0.1/docs/QuantLib-docs-1.0.1-html.tar.gz")
md5sums=('395d4c233e79dde69003fd8d0a4e185f')

package() {
  install -d ${pkgdir}/usr/share/doc/quantlib

  install -m644 QuantLib-docs-${pkgver}-html/* \
    ${pkgdir}/usr/share/doc/quantlib
}
