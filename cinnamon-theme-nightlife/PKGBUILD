# Maintainer: Steven Dall <k3fka@k3fka.net>
pkgname=cinnamon-theme-nightlife
pkgver=2.3
pkgrel=1
pkgdesc="Cinnamon theme in seven colours"
arch=('any')
url="http://cinnamon-spices.linuxmint.com/themes/view/31"
license=('unknown')
depends=('cinnamon')
source=("http://cinnamon-spices.linuxmint.com/uploads/themes/QV2P-2ZY7-BKY8.zip")
md5sums=('c7bf3932a95db583944277ac4cc7c041')

package() {
  mkdir -p ${pkgdir}/usr/share/themes/
  cp -r ${srcdir}/Nightlife* ${pkgdir}/usr/share/themes/
}

