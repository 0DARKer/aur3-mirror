# Maintainer: Adam Hani Schakaki <adam@schakaki.net>
pkgname=cinnamon-theme-nightlife
pkgver=2.2
pkgrel=1
pkgdesc="Cinnamon theme in seven colours"
arch=('any')
url="http://cinnamon-spices.linuxmint.com/themes/view/31"
license=('unknown')
depends=('cinnamon')
source=("http://cinnamon-spices.linuxmint.com/uploads/themes/BDHQ-FHRP-G68X.zip")
md5sums=('606f11527787b572f3e906a51101635b')

package() {
  mkdir -p ${pkgdir}/usr/share/themes/
  cp -r ${srcdir}/Nightlife* ${pkgdir}/usr/share/themes/
}

