pkgname=cinnamon-theme-nightlife
pkgver=3.1
pkgrel=1
pkgdesc="Cinnamon theme in seven colors."
arch=('any')
url="http://cinnamon-spices.linuxmint.com/themes/view/31"
license=('unknown')
depends=('cinnamon')
source=(http://cinnamon-spices.linuxmint.com/uploads/themes/68V2-TJOB-0Y95.zip)
sha1sums=('1c8fed10a75ba1233b973b98123bf40e1c522f53')

package() {
  mkdir -p ${pkgdir}/usr/share/themes/
  cp -r ${srcdir}/Nightlife* ${pkgdir}/usr/share/themes/
}
