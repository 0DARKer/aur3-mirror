# Maintainer: M0Rf30


pkgname=humanity-icon-theme
pkgver=0.5.3.4
pkgrel=1
pkgdesc="Latest Humanity icons from Ubuntu...without modefications..."
arch=('any')
url="https://launchpad.net/humanity"
license=('GPL2')
conflicts=('humanity-icons')
depends=()
source=(https://launchpad.net/ubuntu/+archive/primary/+files/${pkgname}_${pkgver}.tar.gz)
md5sums=('b8da5413c41fccc0f53a8b23415aebf9')

build() {
	mkdir -p $pkgdir/usr/share/icons/
	cd $srcdir/${pkgname}-${pkgver}/
	cp -r "Humanity-Dark" "$pkgdir/usr/share/icons"
	cp -r "Humanity" "$pkgdir/usr/share/icons"
}
