pkgname=mate-icon-theme-symbolic
pkgver=0.0.0
pkgrel=1
pkgdesc="MATE icon theme, symbolic icons"
arch=('any')
license=('GPL')
depends=('gtk-update-icon-cache' 'icon-naming-utils' 'hicolor-icon-theme')
makedepends=('intltool')
url="http://matsusoft.com.ar/projects"
install=mate-icon-theme-symbolic.install
groups=('mate')
source=(http://matsusoft.com.ar/uploads/gnu-linux/mate/${pkgname}.tar.gz)
sha256sums=('9647073264a81e019bcbdb40ff89a796f274852679846f97672546ef70210783')

build() {
	cd "${srcdir}/${pkgname}"
	GTK_UPDATE_ICON_CACHE=/bin/true ./configure --prefix=/usr
	make
}

package() {
	cd "${srcdir}/${pkgname}"
	make DESTDIR="${pkgdir}" install
}
