pkgname=trytond-party
_pkgname=trytond_party
pkgver=1.4.1
pkgrel=1
pkgdesc="Party module of the Tryton application platform"
arch=('any')
url="http://www.tryton.org/"
license=('GPL3')
groups=('trytond-modules')
depends=('python' 'trytond>=1.4' 'trytond-country>=1.4')
optdepends=('python-vatnumber: support for validating VAT numbers')
makedepends=('setuptools')
source=("http://downloads.tryton.org/1.4/$_pkgname-$pkgver.tar.gz")
md5sums=('265733dae912ef7b2ea0a92ce183c76d')

build() {
  cd $srcdir/$_pkgname-$pkgver
  python setup.py install --root=$pkgdir
}

