# Maintainer: Sascha Pfau <MrPeacock@web.de>
# Contributor: Sascha Pfau <MrPeacock@web.de>

pkgname=usb-modeswitch-data
pkgver=20100707
pkgrel=1
pkgdesc="USB_ModeSwitch configuration data"
arch=('i686' 'x86_64')
url="http://www.draisberghof.de/usb_modeswitch"
license=('GPL')
depends=('usb-modeswitch')
options=('!libtool' '!emptydirs')
source=($url/$pkgname-$pkgver.tar.bz2)
md5sums=('d1fcc283c64a0877cdcac9cd682052f9')

build() {
  cd $srcdir/$pkgname-$pkgver/
  make || return 1
  make DESTDIR=$pkgdir files-install
}