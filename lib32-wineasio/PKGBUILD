# Contributor: Marco Süß <m_a_r_c_o.suess at gmx.de>

pkgname=lib32-wineasio
pkgver=0.7.5
pkgrel=1
pkgdesc="ASIO drivers for wine (32bit lib)"
arch=('x86_64')
url="http://wineasio.sf.net/"
license=('LGPL')
groups=('lib32')
depends=('lib32-jack-audio-connection-kit')
_dll=wineasio.dll.so
source=($_dll)
md5sums=('6a6b9bb799823a40d1d342c3a2326cd6')
install=$pkgname.install

build() {
        winepkg=$pkgdir/usr/lib/wine
        install -dm755 $winepkg || return 1
        install -m755 $srcdir/../$_dll $winepkg || return 1
}
