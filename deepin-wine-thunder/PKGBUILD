pkgname=deepin-wine-thunder
_pkgname=deepinwine-thunder5
pkgver=0.0.3_1
pkgrel=3
pkgdesc='Wine Thunder5 by Deepin Linux Team'
arch=('i686' 'x86_64')
url="http://www.linuxdeepin.org/"
license=('Other')
source=("http://packages.linuxdeepin.com/deepin/pool/non-free/d/deepinwine-thunder5/${_pkgname}_${pkgver//_/-}_all.deb" wine-thunder5_preconfig.diff)
depends=('winetricks')

package()
{
    tar xzvf ${srcdir}/data.tar.gz -C ${pkgdir}/
    cd ${pkgdir}/usr/share/deepinwine/thunder5
    patch -p0 < ${srcdir}/wine-thunder5_preconfig.diff 
}

md5sums=('c42ca67714b9d517050f803f68cc49af'
'd3572c330f1b49b6eb9d26be907431c2')
