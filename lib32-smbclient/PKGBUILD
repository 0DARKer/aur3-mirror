pkgname=lib32-smbclient
pkgver=3.5.6
pkgrel32=1
pkgrel=1
pkgdesc="Tools to access a server's filespace and printers via SMB"
arch=('x86_64')
url="http://www.gnome.org"
depends=('lib32-heimdal' 'lib32-libcap' 'lib32-libldap' 'lib32-popt')
license=('GPL' 'LGPL')
groups=lib32
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz)
md5sums=('0c90a466894da73f82b2b968c06bd302')

build() {
  mkdir -p ${startdir}/pkg/opt/lib32/usr
  cp -rPf ${startdir}/src/usr/lib ${startdir}/pkg/opt/lib32/usr
}
