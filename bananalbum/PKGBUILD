pkgname=bananalbum
pkgver=6.1.22
pkgrel=1
pkgdesc="An elegant flash based JAlbum skin."
arch=('any')
url="http://www.bananalbum.net"
license=("freeware")
depends=('jalbum')
source=("BananAlbum.zip::http://jalbum.net/web/GetSkinFile?versionId=1808&type=zip")

package() {
 install -dm755 $pkgdir/usr/share/Jalbum/skins/BananAlbum
 cp -r $srcdir/BananAlbum/* $pkgdir/usr/share/Jalbum/skins/BananAlbum
}
sha256sums=('4cee0b9b607dec9803d242ffb14bc1bdb4831ececd68ad5ee8b869fac7f6a907')
