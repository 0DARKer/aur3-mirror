# Contributor: graysky <graysky AT archlinux dot us>
pkgname=bananalbum
pkgver=6.2.2
pkgrel=1
pkgdesc="An elegant flash based JAlbum skin."
arch=('any')
url="http://www.bananalbum.net"
license=("freeware")
depends=('jalbum')
source=("BananAlbum.zip::http://jalbum.net/web/GetSkinFile?versionId=2005&type=zip")

build() {
 install -dm755 $pkgdir/usr/share/jalbum/skins/BananAlbum
 cp -r $srcdir/BananAlbum/* $pkgdir/usr/share/jalbum/skins/BananAlbum
}
sha256sums=('4ea7ffb595f1ee48c6de5b17e595f96274e603ff0ab28a7b5b5ad2b9576831fd')
