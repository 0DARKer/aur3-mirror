# Maintainer: Marius Nestor <marius softpedia com>

pkgname=musictube
pkgver=1.2.7
pkgrel=1
pkgdesc="A YouTube music player"
arch=('i686' 'x86_64')
url="http://flavio.tordini.org/musictube"
license=('Proprietary')
depends=('qt4' 'phonon')
install='musictube.install'

if [[ "$CARCH" == "i686" ]]; then
	source=("http://flavio.tordini.org/files/musictube/musictube.deb")
	sha256sums=('f98485f078d892e0585986e805232dc0a9a4799811dcbc0c286ba5823a1c9679')
elif [[ "$CARCH" == "x86_64" ]]; then
	source=("http://flavio.tordini.org/files/musictube/musictube64.deb")
	sha256sums=('b447f68d1900bd73efe066a6a22721c8661dc1f33eacd760c255ca1df304d3b7')
fi

package() {
  tar xzvf "$srcdir/data.tar.gz" -C "$pkgdir/"
}

