pkgname=radit-essential
pkgver=2.4.10
pkgrel=1
pkgdesc="Libre√≠as para Radit/iRadit."
arch=('i686' 'x86_64')
url="http://www.radit.org/net"
license=('GPL')
md5sums=('SKIP') 
if [ "$CARCH" == x86_64 ]; then
	source=(https://dl.dropboxusercontent.com/u/148494036/GNU-Linux/Radit/radit-essential-x86_64.tar.gz)
	md5sums=('bd43b5c47e5e639fe33f86f655daf1fd')
elif [ "$CARCH" == i686 ]; then
	source=(https://dl.dropboxusercontent.com/u/148494036/GNU-Linux/Radit/radit-essential-i686.tar.gz)
	md5sums=('9d7fb91dd1e7bf51055972f1ff4ea862')
fi

package() {

    cd $srcdir/$pkgname-$CARCH

    install -d $pkgdir/lib
    cp -u $srcdir/$pkgname-$CARCH/lib/* $pkgdir/lib

} 

    
    



